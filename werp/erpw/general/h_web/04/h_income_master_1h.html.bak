function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
	var ls_temp,ll_seq,i,col,ls_sa_position;
	if (ds_1.IsUpdated || ds_2.IsUpdated || ds_3.IsUpdated){
		if  (confirm("변경되었습니다 저장하시겠습니까?")){
			if (ds_2.IsUpdated){
				var ll_dup
				if (tb_1.ActiveIndex == 9 ){
					ll_dup = f_dup_chk("ds_2","input_date@input_seq")
					if (ll_dup > 0) {
						if (ds_2.NameValue(ll_dup,'input_date') == '' || ds_2.NameValue(ll_dup,'input_seq' == 0 )){
							alert('일자 및 순번을 반드시 입력하십시오...')
						}
						else {
							alert("순번이 중복되었습니다: 순번을 수정하십시요 ")
						}
						ds_2.RowPosition = ll_dup
						is_chk = 'Y'
						return
					}
				}
				if (tb_1.ActiveIndex == 10 ){
					ll_dup = f_dup_chk("ds_2","unique_div@effect_no")
					if (ll_dup > 0) {
						if (ds_2.NameValue(ll_dup,'unique_div') == '' || ds_2.NameValue(ll_dup,'effect_no' == '' )){
							alert('구분 및 사건번호를 반드시 입력하십시오...')
						}
						else {
							alert("사건번호가 중복되었습니다: 사건번호를 수정하십시요 ")
						}
						ds_2.RowPosition = ll_dup
						is_chk = 'Y'
						return
					}
				}
			} 	       

			if (ds_1.IsUpdated){
				is_rowchg_tag = 'Y'
				for (i=ds_1.CountRow ;i >0 ;i--){
		     		if (ds_1.RowStatus(i) == '3' || ds_1.RowStatus(i) == '1') {
						ds_1.NameValue(i,'input_date') = f_sysdatetime()
						ds_1.NameValue(i,'input_id')   = is_empno
					}
				}
				tr_1.Post()
				is_rowchg_tag = 'N'
			}
			if (ds_2.IsUpdated) {
				if (tb_1.ActiveIndex == 3 || tb_1.ActiveIndex == 8 || tb_1.ActiveIndex == 10) {
					for (i=ds_2.CountRow ;i >0 ;i--){
			     		if (ds_2.RowStatus(i) == '3' || ds_2.RowStatus(i) == '1') {
			     			if (tb_1.ActiveIndex != 10 ) {
								ds_2.NameValue(i,'input_date') = f_sysdatetime()
							}
							ds_2.NameValue(i,'input_id')   = is_empno
						}
					}
				}
            tr_2.Action   = is_tr_2_addr[tb_1.ActiveIndex]
			 	tr_2.KeyValue = is_tab_addr_tr_keyvalue[tb_1.ActiveIndex]
				tr_2.Post()
			}
			if (ds_3.IsUpdated) {
				if (tb_1.ActiveIndex == 12) {
						tr_3.Action   = is_tr_3_addr
						tr_3.Post()
						return;
						
				}
			}
		}

		//btnquery_onclick()	
//		gd_1.ReDraw = true
	}
}

