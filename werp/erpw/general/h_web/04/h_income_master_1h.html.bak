function income_cmpt()                       //  분양수입금계산
//------------------------------------------------------------------------
{
	var ls_dongho,li_seq,li_input_amt,ls_sa_position

	//if (ds_1.NameValue(ds_1.RowPosition,'contract_code') == "02") {
	//	alert("임대는 임대료 입금관리에서 입력하세요.")
	//	return
	//}
	if (tb_1.ActiveIndex != 1 && tb_1.ActiveIndex != 3 ){
		alert('분양대금 입금화면에서만 처리가 가능합니다...')
		return
	}
	
   if (!(ds_1.RowPosition > 0 )) { return};
	ls_dongho = ds_1.NameValue(ds_1.RowPosition,'dongho')    
	li_seq    =	ds_1.NameValue(ds_1.RowPosition,'seq')       

	ls_sa_position = ds_1.RowPosition
	
	if (em_input_date.text == '') {
		alert('납입일자를 반드시 입력하십시오...')
		return
	}
	
	li_input_amt = parseInt(em_input_amt.text,10)

	/*if (li_input_amt == 0) {
		alert('납입금액을 반드시 입력하십시오...')
		return
	}	*/																								  

	ls_temp = ccom_input_class.CodeValue
	ls_temp2 = ccom_input_deposit.CodeValue

	if  ( (ls_temp == '11' || ls_temp=='12')  &&  !(ls_temp2.length > 0)){
		 alert('계좌를 반드시 입력하십시오...')
		 return
	}
	
	ds_spcall.DataID = 'h_income_cmpt_procedure.jsp?arg_dept_code=' + is_dept_code + '&arg_sell_code=' + is_sell_code +
	                   '&arg_dongho=' + ls_dongho + '&arg_seq=' + li_seq + '&arg_input_date=' + em_input_date.text + 
	                   '&arg_input_amt=' + li_input_amt + '&arg_input_class=' + ccom_input_class.CodeValue +
	                   '&arg_input_deposit=' + ccom_input_deposit.CodeValue + '&arg_user=' + is_empno +
							 '&arg_receipt_class_code=' + ccom_receipt_class_code.CodeValue +
							 '&arg_receipt_number=' + em_receipt_number.value +
							 '&arg_card_app_num=' + em_card_app_num.text +
							 '&arg_card_bank_code=' + is_bank_code

	ds_spcall.SyncLoad = true
	ds_spcall.reset()

	ds_1.RowPosition = ls_sa_position
	div_btnquery_onclick()
	em_input_amt.text = 0
	em_receipt_number.value= ''
}
