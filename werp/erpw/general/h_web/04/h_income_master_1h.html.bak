//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
	if ( il_screen == 1 ) {
		return
	}
	if (tb_1.ActiveIndex != 9 && tb_1.ActiveIndex != 10 && tb_1.ActiveIndex != 12) {
		return
	}
	if (tb_1.ActiveIndex == 12){
		ds_3.AddRow()		
		init_move()
		return;
	}
	 ds_2.AddRow()
	 init_move()
}
//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
	if ( il_screen == 1 ) {
		return
	}
	if (tb_1.ActiveIndex != 9 && tb_1.ActiveIndex != 10 && tb_1.ActiveIndex != 12) {
		return
	}

   if (tb_1.ActiveIndex == 12){  /////////////
		if (ds_3.RowPosition < 1){
			ds_3.AddRow()
		}
		else {
			ds_3.InsertRow(ds_3.RowPosition)
		}
		init_move()
		return;
	}
	if (ds_2.RowPosition < 1){
		ds_2.AddRow()
	}
	else {
		ds_2.InsertRow(ds_2.RowPosition)
	}
	init_move()
}

