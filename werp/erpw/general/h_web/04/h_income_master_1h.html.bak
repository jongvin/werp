function receive_print()                     // 입금표 미리보기
//------------------------------------------------------------------------
{
//   print_condition_query()
   var arg_temp

	// @@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
	// is_update_ok, ds_select10,  ds_update10 이 반드시 있어야 출력이 됩니다.
	// @@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
	
	if (ds_daily.RowPosition < 1) {
		return
	}
	//if (tb_1.ActiveIndex != 1 && tb_1.ActiveIndex != 3 && tb_1.ActiveIndex != 6 && tb_1.ActiveIndex != 8 ){
	//	alert('분양대금/별도품목 입금화면에서만 처리가 가능합니다...')
	//	return
	//}

	
	ls_cust_code  = ds_1.NameValue(ds_1.RowPosition,'cust_code')
	ls_cust_name  = ds_1.NameValue(ds_1.RowPosition,'cust_name')
	
	var row 
	row = ds_daily.RowPosition

	ls_date       = ds_daily.NameValue( row, 'receipt_date')
	ll_pyong      = ds_1.NameValue(ds_1.RowPosition,'pyong')
	ls_dongho     = ds_1.NameValue(ds_1.RowPosition,'dongho')
	ll_seq        = ds_daily.NameValue( row, 'seq')
   ll_amt        = ds_daily.NameValue( row, 'amt')

	ls_remarks = '0'
	if (tb_1.ActiveIndex == 1 || tb_1.ActiveIndex == 2 || tb_1.ActiveIndex == 3) {
		if (ds_1.NameValue(ds_1.RowPosition,'contract_code') == '02')
			ls_remarks    = '임대 보증금 입금'
		else
			ls_remarks    = '분양금 입금'	
	}
	if (tb_1.ActiveIndex == 6 || tb_1.ActiveIndex == 7 || tb_1.ActiveIndex == 8) {
		if (ds_1.NameValue(ds_1.RowPosition,'contract_code') == '02')
			ls_remarks    = '임대 별도품목 입금'
		else
			ls_remarks    = '분양 별도품목 입금'	
	}

	if (ll_amt == 0) {
		alert("금액이 0 입니다")
		return
	}
   
	ls_remarks = prompt("입금표에 출력할 내용을 입력하세요", ls_remarks)

	if (ls_remarks.length < 1) 
		ls_remarks = "0"
		   
   arg_temp ="general_rpt/h_web/01r/"        // 보고서 디렉토리

   arg_temp = arg_temp + "h_receive_print" + "&RptParams=" + is_dept_code + 
                                             "&RptParams=" + is_dept_name + 
                                             "&RptParams=" + ls_cust_code + 
                                             "&RptParams=" + ls_cust_name + 
                                             "&RptParams=" + ll_pyong + 
                                             "&RptParams=" + ls_dongho + 
                                             "&RptParams=" + ls_date + 
                                             "&RptParams=" + ll_amt + 
                                             "&RptParams=" + is_empno + 
                                             "&RptParams=" + ls_remarks

   f_crystal_report(arg_temp)   //보고서 호출

}

