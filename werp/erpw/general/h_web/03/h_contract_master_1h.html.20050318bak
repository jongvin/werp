//------------------------------------------------------------------------
function contract_print()                     // 계약서 미리보기
//------------------------------------------------------------------------
{
	if (ds_1.NameValue(ds_1.RowPosition,'contract_yn') != "Y") {
		alert("미계약 세대는 출력할 수 없습니다")
		return
	}

	if (ds_1.NameValue(ds_1.RowPosition,'contract_date') == "") {
		alert("계약 일자가 없는것은 출력할 수 없습니다")
		return
	}
	
	if (ds_1.NameValue(ds_1.RowPosition,'cust_code') == "") {
		alert("고객이 없는것은 출력할 수 없습니다")
		return
	}

	ls_dongho = ds_1.NameValue(ds_1.RowPosition,'dongho')
	ll_seq    = ds_1.NameValue(ds_1.RowPosition,'seq')
			
	ll_cnt = f_select_q("select max(deposit_no) select_1 from h_sale_income where dept_code = '" + is_dept_code + "' and sell_code = '" + is_sell_code + "' and dongho = '" + ls_dongho + "' and seq = " + ll_seq )       

	if (ll_cnt > 0)
		ls_deposit_no = ds_select.NameValue(1,'select_1')
	else
		ls_deposit_no = "."

	if (ls_deposit_no.length < 1)
		ls_deposit_no = "."
		
	if (confirm("특약 사항을 출력 하시겠습니까?") == true)
		ls_agree = "Y"
	else
		ls_agree = "N"

   arg_temp ="general_rpt/h_web/03r/"        // 보고서 디렉토리

	if (ds_1.NameValue(ds_1.RowPosition,'contract_code') == "02") {
		arg_temp = arg_temp + "h_cont_docu_lease"       // 임대 계약서
	}
	else if (is_sell_code == "03") { 
		arg_temp = arg_temp + "h_cont_docu_sale_store"  // 상가 분양 계약서
	}
	else  { 
		arg_temp = arg_temp + "h_cont_docu_sale_apt"    // 아파트 분양 계약서
	}
   arg_temp = arg_temp + "&RptParams=" + is_dept_code + 
                         "&RptParams=" + is_sell_code + 
                         "&RptParams=" + ls_dongho + 
                         "&RptParams=" + ll_seq + 
                         "&RptParams=" + ls_agree + 
                         "&RptParams=" + ls_deposit_no
   f_crystal_report(arg_temp)   //보고서 호출

}

//------------------------------------------------------------------------
function receive_print()                     // 입금표 미리보기
//------------------------------------------------------------------------
{
//   print_condition_query()
   var arg_temp

	// @@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
	// is_update_ok, ds_select10,  ds_update10 이 반드시 있어야 출력이 됩니다.
	// @@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
	
	if (ds_1.NameValue(ds_1.RowPosition,'contract_yn') != "Y") {
		alert("미계약 세대는 출력할 수 없습니다")
		return
	}

	if (ds_1.NameValue(ds_1.RowPosition,'contract_date') == "") {
		alert("계약 일자가 없는것은 출력할 수 없습니다")
		return
	}
	
	if (ds_1.NameValue(ds_1.RowPosition,'cust_code') == "") {
		alert("고객이 없는것은 출력할 수 없습니다")
		return
	}

	ls_cust_code  = ds_1.NameValue(ds_1.RowPosition,'cust_code')
	ls_cust_name  = ds_1.NameValue(ds_1.RowPosition,'cust_name')
	ls_date       = ds_1.NameValue(ds_1.RowPosition,'contract_date')
	ll_pyong      = ds_1.NameValue(ds_1.RowPosition,'pyong')
	if (ds_1.NameValue(ds_1.RowPosition,'contract_code') == '02')
		ls_remarks    = '임대 계약금 입금'
	else
		ls_remarks    = '분양 계약금 입금'
	ls_dongho     = ds_1.NameValue(ds_1.RowPosition,'dongho')
	ll_seq        = ds_1.NameValue(ds_1.RowPosition,'seq')

	ll_cnt = f_select_q("select sum(nvl(r_amt,0)) select_1, sum(nvl(delay_amt,0)) select_2, sum(nvl(discount_amt,0)) select_3 from h_sale_income where dept_code = '" + is_dept_code + "' and sell_code = '" + is_sell_code + "' and dongho = '" + ls_dongho + "' and seq = " + ll_seq + " and receipt_date = '" + ls_date + "' ")       

   if (ll_cnt > 0) {
		ll_amt = parseInt(ds_select.NameValue(1,'select_1')) + parseInt(ds_select.NameValue(1,'select_2')) - parseInt(ds_select.NameValue(1,'select_2'))
		if (!(ll_amt > 0)) {
			alert("계약일자로 입금된금액이 없습니다")
			return
		}
	}
	else {
		alert("계약일자로 입금된금액이 없습니다")
		return
	}

	ll_receive_no = f_h_receive_print(is_dept_code, is_sell_code, ls_dongho, ls_cust_code, 
                            			  ls_date, ll_amt,  is_empno, ls_remarks)	

   if (ll_receive_no == 0)  {
		alert("입금표 출력에 문제가 있습니다.\n담당자에게 문의하세요")
		return
   }
 
	ls_remarks = prompt("입금표에 출력할 내용을 입력하세요", ls_remarks)

	if (ls_remarks.length < 1) 
		ls_remarks = "0"
		   
   arg_temp ="general_rpt/h_web/01r/"        // 보고서 디렉토리

   arg_temp = arg_temp + "h_receive_print" + "&RptParams=" + is_dept_code + 
                                             "&RptParams=" + is_dept_name + 
                                             "&RptParams=" + ls_cust_code + 
                                             "&RptParams=" + ls_cust_name + 
                                             "&RptParams=" + ll_pyong + 
                                             "&RptParams=" + ls_dongho + 
                                             "&RptParams=" + ls_date + 
                                             "&RptParams=" + ll_amt + 
                                             "&RptParams=" + is_empno + 
                                             "&RptParams=" + ls_remarks

   f_crystal_report(arg_temp)   //보고서 호출

}


