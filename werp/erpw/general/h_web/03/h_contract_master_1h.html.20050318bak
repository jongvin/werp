				'@' + ds_1.NameValue(ds_1.RowPosition,'contract_code') + '@' + em_input_date.text + '@' + 
				ccom_input_class.CodeValue + '@' + ccom_input_deposit.CodeValue + '@'
	
		result = window.showModalDialog(gs_home + "/general/h_web/03/h_subs_prize_master_find_1h.html", arg,"dialogWidth:350px;dialogHeight:400px");
