		   return;
	   }
		array = result.split("@");
		ds_1.NameValue(ds_1.RowPosition,'cust_code')     = array[0]
		ds_1.NameValue(ds_1.RowPosition,'cust_name')     = array[1]
		ds_1.NameValue(ds_1.RowPosition,'contract_code') = array[2]
		ds_1.NameValue(ds_1.RowPosition,'input_date') = f_sysdatetime()
		ds_1.NameValue(ds_1.RowPosition,'input_id')   = is_empno
		em_input_date.text           = array[3]
		ccom_input_class.CodeValue   = array[4]
		ccom_input_deposit.CodeValue = array[5]
		em_input_amt.text            = array[6]
		is_subs_no                   = array[7]
		
		em_cust_code.value = array[0]
		tx_cust_name.value = array[1]
		em_cont_zip_code.text = array[8]
		tx_cont_addr1.value = array[9]
		tx_cont_addr2.value = array[10]

		is_subs_chk = 'Y'
		contract_process_main()
	}
	else {
		alert('미계약 자료에서만 처리가능 합니다...')
	}
}
