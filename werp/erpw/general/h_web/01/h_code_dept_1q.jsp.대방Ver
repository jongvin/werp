     dSet.addDataColumn(new GauceDataColumn("lease_e_date",GauceDataColumn.TB_STRING,18));
    String query = "SELECT DEPT_CODE," + 
     "         LONG_NAME," + 
     "         PHONE," + 
     "         FAX," + 
	 "         ZIP_CODE,   " + 
     "         ADDR1,   " + 
     "         ADDR2,   " + 
