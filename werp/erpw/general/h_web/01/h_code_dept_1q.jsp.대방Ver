     "         CHARGE," + 
     "         DESC_1," + 
     "         DESC_2," + 
     "         DESC_3," + 
     "         DESC_4," + 
     "         DESC_5," + 
     "         DESC_6," + 
     "         DESC_7," + 
     "         DESC_8," + 
     "         to_char(CONST_FROM,'yyyy.mm.dd') const_from," + 
     "         to_char(CONST_TO,'yyyy.mm.dd') const_to," + 
     "         to_char(SALE_DATE,'yyyy.mm.dd') sale_date," + 
     "         to_char(MOVEIN_DATE,'yyyy.mm.dd') movein_date," + 
     "         to_char(LEASE_S_DATE,'yyyy.mm.dd') lease_s_date," + 
     "         to_char(LEASE_E_DATE,'yyyy.mm.dd') lease_e_date  FROM H_CODE_DEPT  order by dept_code     ";
