var gs_home = top.frames['security'].gs_home.value

var is_ds_1_addr = gs_home + '/general/h_web/01/h_code_dept_1q.jsp?'
var is_tr_1_addr = gs_home + '/general/h_web/01/h_code_dept_1tr.jsp'

var is_ds_2_addr = new Array(5)
is_ds_2_addr[1] = "h_code_dept_2q.jsp?arg_dept_code=" 
is_ds_2_addr[2] = "h_code_dept_3q.jsp?arg_dept_code=" 
is_ds_2_addr[3] = "h_code_dept_4q.jsp?arg_dept_code=" 
is_ds_2_addr[4] = "h_code_dept_5q.jsp?arg_dept_code=" 

var is_tr_2_addr = new Array(5)
is_tr_2_addr[1] = gs_home +  '/general/h_web/01/h_code_dept_2tr.jsp'                                 
is_tr_2_addr[2] = gs_home +  '/general/h_web/01/h_code_dept_3tr.jsp'                                 
is_tr_2_addr[3] = gs_home +  '/general/h_web/01/h_code_dept_4tr.jsp'                                  
is_tr_2_addr[4] = gs_home +  '/general/h_web/01/h_code_dept_5tr.jsp'                                  

var is_tab_addr_tr_keyvalue =  new Array(5)
is_tab_addr_tr_keyvalue[1] = 'JSP(I:h_code_dept_2tr=ds_2)'         
is_tab_addr_tr_keyvalue[2] = 'JSP(I:h_code_dept_3tr=ds_2)'         
is_tab_addr_tr_keyvalue[3] = 'JSP(I:h_code_dept_4tr=ds_2)'          
is_tab_addr_tr_keyvalue[4] = 'JSP(I:h_code_dept_5tr=ds_2)'          

var is_work_date 
var il_screen = 1,il_tagpage=1
var tr_fail=1                 // update정상:1  비정상:0

//------------------------------------------------------------------------
function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{
	ds_1.DataID = is_ds_1_addr
	ds_1.SyncLoad=true
	ds_1.Reset()

	div_btnquery_onclick()
}
//------------------------------------------------------------------------
function div_btnquery_onclick()                     // 하위 조회  
//------------------------------------------------------------------------
{
//	if ((ds_1.IsUpdated || ds_2.IsUpdated ) && ds_1.RowStatus(ds_1.RowPosition) != 1){
//		btnsave_onclick()
//	}
	ls_dept_code  = ds_1.NameValue(ds_1.RowPosition,'dept_code')
	
	ds_2.DataID = is_ds_2_addr[tb_1.ActiveIndex] + ls_dept_code 
	ds_2.SyncLoad=true
	ds_2.Reset()
	
}
//-----------------------------------------------------------------------
function Fn_SetColumn()   // 커서위치 세팅
//-----------------------------------------------------------------------
{
	if ( tb_1.ActiveIndex == 1 ) {
//		tab_page_1.Focus();
//		tab_page_1.SetColumn("phone");
	}
}
//-----------------------------------------------------------------------
function init_move()               // 초기값세팅
//-----------------------------------------------------------------------
{
	var ll_seq
	if ( il_screen == 1 ) {
		ds_1.NameValue(ds_1.RowPosition,'project_div') = '01'
		ds_1.NameValue(ds_1.RowPosition,'lease_yn') = '1'
		ds_1.NameValue(ds_1.RowPosition,'process_code') = '01'
	}
	else {
		ds_2.NameValue(ds_2.RowPosition,'dept_code') = ds_1.NameValue(ds_1.RowPosition,'dept_code')
		if ( tb_1.ActiveIndex == 1 ) {
			 ll_seq = ds_2.Max(2,0,0)
			 ds_2.NameValue(ds_2.RowPosition,'seq') = ll_seq + 1
			 ds_2.NameValue(ds_2.RowPosition,'r_amt_yn') = 'N'
		}
		if ( tb_1.ActiveIndex == 2 ) {
			 ds_2.NameValue(ds_2.RowPosition,'kind') = '01'
			 ds_2.NameValue(ds_2.RowPosition,'status') = '02'
		}
		if ( tb_1.ActiveIndex == 3 ) {
			 ds_2.NameValue(ds_2.RowPosition,'input_date') = is_work_date
			 ds_2.NameValue(ds_2.RowPosition,'finish_yn') = 'N'
		}
		if ( tb_1.ActiveIndex == 4 ) {
			 ds_2.NameValue(ds_2.RowPosition,'dept_code') = ds_1.NameValue(ds_1.RowPosition,'dept_code')
		}
	}
	

	Fn_SetColumn()
}
//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
	var ls_temp,ls_degree,ls_row
	var result;
	var send_arg;
	var array = new Array();
	
	if ( il_screen == 1 ) {
		send_arg = gs_home + '@' + ''
		result = window.showModalDialog(gs_home + "/general/h_web/00/h_dept_find_1h.html", send_arg,"dialogWidth:580px;dialogHeight:500px");
		if(result == "") return;

		array = result.split("@");
		ds_1.InsertRow(1)
		ds_1.NameValue(ds_1.RowPosition,"dept_code") = array[0]
		ds_1.NameValue(ds_1.RowPosition,"long_name") = array[1]
		ds_1.NameValue(ds_1.RowPosition,"zip_code")  = array[2]
		ds_1.NameValue(ds_1.RowPosition,"addr1")     = array[3]
		ds_1.NameValue(ds_1.RowPosition,"addr2")     = array[4]
		ds_1.NameValue(ds_1.RowPosition,"phone")     = array[5]
		init_move()
		return
	}
	else {
		ds_2.AddRow()
		init_move()
	}
}
//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
	 btnadd_onclick()
}

//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
	var ls_dept_code
	
	if ( il_screen == 1 ) {
		ds_1.DeleteRow(ds_1.RowPosition)  
	}	
	else {
		ds_2.DeleteRow(ds_2.RowPosition)
	}	
}
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
	var ls_chk=0,ls_spec_chk=0
 if (ds_2.IsUpdated){
		 var ll_dup
		 if (il_tagpage == 3) {
			 ll_dup = f_dup_chk("ds_2","input_date@input_seq")
			 if (ll_dup > 0) {
			     alert("일자/순번이 중복되었습니다: 일자/순번을 수정하십시요 ")
			     ds_2.RowPosition = ll_dup
			     return
			 }
		 }
 } 	       
	
	if (ds_1.IsUpdated || ds_2.IsUpdated ){
		if  (confirm("변경되었습니다 저장하시겠습니까?")){
			if (ds_1.IsUpdated ) 
       		tr_1.Post()
			
			if (ds_2.IsUpdated ) {
			 	tr_2.Action   = is_tr_2_addr[il_tagpage]
			 	tr_2.KeyValue = is_tab_addr_tr_keyvalue[il_tagpage]

		   	for (i=1; i<= ds_2.CountRow; i++){
					if (il_tagpage == 1) {
						if (ds_2.NameValue(i,"pyong") == 0 || ds_2.NameValue(i,'style') == '' || ds_2.NameValue(i,'item_code') == '' || ds_2.NameValue(i,'item_size') == '' )
			   			ls_chk++
	   			}
					if (il_tagpage == 2) {
				   	if (ds_2.NameValue(i,"seq") == '' || ds_2.NameValue(i,'kind') == '' )
			   			ls_chk++
	   			}
					if (il_tagpage == 3) {
				   	if (ds_2.NameValue(i,"input_date") == '' || ds_2.NameValue(i,'input_seq') == 0)
			   			ls_chk++
	   			}
	  			}
	  			if (ls_chk == 0) {
	       			tr_2.Post()
	  			}
     			else {
	   			alert("필수입력항목을 반드시 입력하십시오 \n        저장되지않았습니다.")
	   			return
	     		}
			}
		}
	}
}
//---------------------------------------------------------------------------
function bt_zip_onclick()                     //우편번호 찾기
//---------------------------------------------------------------------------
{
	var result;
	var array = new Array();
	result = window.showModalDialog(gs_home + "/comm_search/z_zip_find_1h.html", tx_addr1.value,"dialogWidth:640px;dialogHeight:500px");
	if(result == "") return;

	array = result.split("@");
	em_zip_code.text = array[0]
	tx_addr1.value = array[1]

}
//---------------------------------------------------------------------------
function bt_zip_onclick1()                     //우편번호 찾기(매도인)
//---------------------------------------------------------------------------
{
	var result;
	var array = new Array();
	result = window.showModalDialog(gs_home + "/comm_search/z_zip_find_1h.html", tx_sell_addr1.value,"dialogWidth:640px;dialogHeight:500px");
	if(result == "") return;

	array = result.split("@");
	em_sell_zip_code.text = array[0]
	tx_sell_addr1.value = array[1]

}
//------------------------------------------------------------------------
function get_focus(focus) // gd가 포커스를 받을때 동작.
//------------------------------------------------------------------------
{	
	if (focus == il_screen) {
		return
	}	
	il_screen = focus
}
//----------------------------------------------------------------------------
function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
	ds_1.Undo(ds_1.RowPosition)
}
//----------------------------------------------------------------------------
function btnexit_onclick(ls_arg)                 // 종료 
//-----------------------------------------------------------------------------

{
	if (ds_1.IsUpdated) {
	     if  (confirm("변경되었습니다 저장하시겠습니까?")) 
	        {  //저장 routine
	        }   
 
	   }     
	top.frames['main_ct'].location.href(ls_arg)
	top.frames['main_title'].div_r.style.visibility="hidden"             
	top.frames['main_title'].div_u.style.visibility="hidden"             

	// top.frames['main_ct'].window.close()     
}
//------------------------------------------------------------------------
function keyDown()                       //검색어 입력후 enter를 쳤을경우
//------------------------------------------------------------------------
{
	if (window.event.keyCode == 13) { 
		dept_code_find();
	} 
}
//----------------------------------------------------------------
function excel_down()                      //엑셀자료 내리기(공종별) 
//----------------------------------------------------------------
{
 var temp_1
 temp_1 = is_dept_name + '   [' + em_select_yymm.text + ']'
 gd_1.RunExcelSheet(temp_1)
   
}

//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
	top.frames['main_title'].mouse_over()
	is_work_date = f_sysdate()
	tr_1.Action = is_tr_1_addr 
	tr_2.Action = is_tr_2_addr 
	ll_cnt = f_select_q("select code select_1, code_name select_2  from h_code_common  where code_div = '19' order by code")       //보증종류 
	ll_cnt = f_select1_q("select code select_1, code_name select_2  from h_code_common  where code_div = '09' order by code")       //청약순위
	btnquery_onclick()
	tb_1.ActiveIndex = 1
	il_screen = 1
}
