//---------------------------------------------------------------------
function update_rtn()                     //업체반영
//---------------------------------------------------------------------
{
	var i,ls_unq_num,ls_sbcr_code
//	alert('아직개발중입니다...')
//	return
	if (ds_1.CountRow < 1) return
	if  (confirm("업체반영을 하시면 협력업체 기본사항의 자료가 변경됩니다. 계속하시겠습니까?")){
	}
	else {
		return
	}
	for (i=1 ;i <= ds_1.CountRow ;i++)	{
		if (ds_1.RowMark(i) == 1){
			ls_unq_num   = ds_1.NameValue(i,'sbcr_unq_num')
			ls_sbcr_code = ds_1.NameValue(i,'sbcr_code')
			ds_spcall.DataID = 's_sbcr_update_procedure.jsp?arg_unq_num=' + ls_unq_num + '&arg_sbcr_code=' + ls_sbcr_code 
			ds_spcall.SyncLoad = true
			ds_spcall.reset()
		}
	}
	btnquery_onclick()
	alert('정상처리되었습니다...')
}
