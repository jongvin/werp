      switch (is_filter) {
              case "tot":
                         return true
                         break;
              case "1p":
                         if (ds_2.NameValue(row,"evl_class") == '1') return true
                         else return false;
                         break;
              case "2p":
                         if (ds_2.NameValue(row,"evl_class") == '2') return true
                         else return false;
                         break;
              case "3p":
                         if (ds_2.NameValue(row,"evl_class") == '3') return true
                         else return false;
                         break;
        }
</SCRIPT>
<script language=JavaScript for=gd_1 event=OnClick(row,colid)>
	if (is_dept_class == 'P') {
		if (colid == 'evl_name') {
			if (is_emp_no != ds_1.NameValue(row,'evl_id')) {
				alert('평가자가 아니므로 평가화면으로 가실수 없습니다...')
				return
			}
		}
		if(colid == 'name2') {
			if (is_emp_no != ds_1.NameValue(row,'id2')){
				alert('평가자가 아니므로 평가화면으로 가실수 없습니다...')
				return
			}
		}
	}
	else {
		if (colid == 'evl_name' || colid == 'name2'){
			is_chk = '3';
			if (is_emp_no == ds_1.NameValue(row,"evl_id")) {
				is_chk = '1';
			}
			if (is_emp_no == ds_1.NameValue(row,"id2")) {
				is_chk = '2';
			}
	
			proj_insert()	
		}
//		is_chk = '3';
//		proj_insert()	
//		return
	}
	if (colid == 'evl_name' || colid == 'name2'){
		is_chk = '3';
		if (is_emp_no == ds_1.NameValue(row,"evl_id")) {
			is_chk = '1';
		}
		if (is_emp_no == ds_1.NameValue(row,"id2")) {
			is_chk = '2';
		}

		proj_insert()	
	}
</SCRIPT>
