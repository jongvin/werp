//--------------------------------------------------------------------------
function   rd_1_proc()
//----------------------------------------------------------------
{
	bt_1.checked=true
	bt_2.checked=false
	bt_3.checked=false
	bt_4.checked=false
	is_filter = "1p"
	filter_function()
}
//--------------------------------------------------------------------------
function   rd_2_proc()
//----------------------------------------------------------------
{
	bt_1.checked=false
	bt_2.checked=true
	bt_3.checked=false
	bt_4.checked=false
	is_filter = "2p"
	filter_function()
}
//--------------------------------------------------------------------------
function   rd_3_proc()
//----------------------------------------------------------------
{
	bt_1.checked=false
	bt_2.checked=false
	bt_3.checked=true
	bt_4.checked=false
	is_filter = "3p"
	filter_function()
}
//--------------------------------------------------------------------------
function   rd_tot_proc()
//----------------------------------------------------------------
{
	bt_1.checked=false
	bt_2.checked=false
	bt_3.checked=false
	bt_4.checked=true
	is_filter = "tot"
	filter_function()
}
//--------------------------------------------------------------------------
function   filter_function()
//----------------------------------------------------------------
{
	gd_2.ReDraw = false
	ds_2.Filter()
	gd_2.ReDraw = true
}  
//--------------------------------------------------------------------------
function   sbcr_search()
//----------------------------------------------------------------
{
	if (tx_sbcr_search_name.value == '') {
		is_long_name = ''
	}
	else {
		is_long_name = tx_sbcr_search_name.value
	}
	div_btnquery_onclick()
}
//--------------------------------------------------------------------------
function   sbcr1_search()
//----------------------------------------------------------------
{
	if (tx_sbcr_name.value == '') {
		is_sbcr_name = ''
	}
	else {
		is_sbcr_name = tx_sbcr_name.value
	}
	div_btnquery_onclick()
}
//------------------------------------------------------------------------
function btnquery_prt_onclick()                     // 미리보기
//------------------------------------------------------------------------
{
	if (is_dept_class == 'P') { 
		return
	}
	ls_dept_code 	 = ds_1.NameValue(ds_1.RowPosition,'dept_code')
	ls_order_number = ds_1.NameValue(ds_1.RowPosition,'order_number')

   var arg_temp
   arg_temp ="const_rpt/s_web/99r/s_dept_evl"        // 보고서 디렉토리

   arg_temp = arg_temp + "&RptParams=" + ls_year + "&RptParams=" + ls_degree + "&RptParams=" + ls_class 
   						  + "&RptParams=" + ls_dept_code + "&RptParams=" + ls_order_number 

   f_crystal_report(arg_temp)   //보고서 호출
}
//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
	top.frames['main_title'].mouse_over()
	is_work_date = f_sysdate()
	tr_1.Action = is_tr_1_addr 

	em_select_yymm.text = is_work_date.substr(0,4)
	is_year = is_work_date.substr(0,4)

	btnquery_onclick()
	var result="",ll_cnt,ls_dept_code;
	var array = new Array();
   result = readCookie("@s_evl_proj_sbcrdetail@")
	if (result != "err") {
		array = result.split("@");
	
		ds_2.RowPosition  = array[6]	
		makeCookie("@s_evl_proj_sbcrdetail@","")
	}
	ll_cnt = f_select_q("select dept_code select_1 from z_authority_user where empno = '" + is_emp_no + "'" )

	if (ll_cnt > 0) {
		ls_dept_code = ds_select.NameValue(ds_select.RowPosition,'select_1');
		ll_cnt = f_select_q("select dept_proj_tag select_1 from z_code_dept where dept_code = '" + ls_dept_code + "'" )

		if (ll_cnt > 0){
			is_dept_class = ds_select.NameValue(ds_select.RowPosition,'select_1');
		}
		else{
			is_dept_class = 'P'
		}
	}
	else{
		is_dept_class = 'P'
	}
	if (is_dept_class == 'P') {
		gd_1.ColumnProp('score1','Show') = false
		gd_1.ColumnProp('score2','Show') = false
		gd_1.ColumnProp('eva_score','Show') = false
		gd_1.ColumnProp('add_score','Show') = false
	}			
	top.frames['main_title'].title_name.value =  " 현장평가 "         

}
