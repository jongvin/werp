	btnquery_onclick()

	bt_query.style.visibility="hidden"

	var result="";
	var array = new Array();
   result = readCookie("@s_req_insert_evl@")
	if (result != "err") {
		array = result.split("@");
	        
		is_filter = array[3]
	   switch (is_filter) {
	        case "okproc":
	                   rd_ok_proc();
	                   break;
	        case "3proc":
	                   rd_3_proc();
	                   break;
	        case "4proc":
	                   rd_4_proc();
	                   break;
	        case "7proc":
	                   rd_7_proc();
	                   break;
	        case "noproc":
	                   rd_no_proc();
	                   break;
	  }
	
		ds_1.RowPosition  = array[4]	
		makeCookie("@s_req_insert_evl@","")
	}
		
	tb_1.ActiveIndex = 1
	il_screen = 1
}
