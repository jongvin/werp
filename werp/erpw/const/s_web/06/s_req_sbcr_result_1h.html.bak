function fc_insert_evl()               //등록평가
//---------------------------------------------------------------------
{
var parm;
	if (ds_1.CountRow < 1) { 
		alert("선택한 내역이 없기때문에 들어갈수 없습니다")
		return
	}   

	makeCookie("@s_req_insert_evl@",is_emp_no + "@" + ds_1.NameValue(ds_1.RowPosition,"sbcr_unq_num") + "@" + 
											  ds_1.NameValue(ds_1.RowPosition,"profession_wbs_code")  + "@" + 	
