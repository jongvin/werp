    style="Z-INDEX: 100; FONT-SIZE: 12px; LEFT: 60px; POSITION: absolute; TOP: 8px"
    onkeyDown="keyDown()">
<INPUT  style="Z-INDEX: 106; LEFT: 200px; POSITION: absolute; TOP: 7px" type=button  class="img_btnFind"  value=검색 onclick=btnquery_onclick()>
<INPUT id=Button_confrim  style="LEFT: 360px; POSITION: absolute;  TOP: 7px; "  type=button  class="img_btn2_1"  value=확인 onclick=Button_confirm_onclick()>
<INPUT id=Button_close  style="LEFT: 410px; POSITION: absolute;  TOP: 7px; "  type=button  class="img_btn2_1"  value=닫기 onclick=Button_close_onclick() >
