//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
	if (f_reject_window()) return
	top.frames['main_title'].mouse_over()

	tr_1.Action = is_tr_1_addr 

	f_ds_select_q("ds_se_FWATCHD","SELECT AA.A select_1 FROM (SELECT '전체' A, 1 B FROM dual UNION SELECT DISTINCT rtrim(FWATCHD), 2 FROM WATCH_GRADE WHERE FWATCHD IS NOT NULL) AA ORDER BY AA.B, AA.A")
	f_ds_select_q("ds_se_LASTGRD2_s","SELECT AA.A select_1 FROM (SELECT '전체' A, 1 B FROM dual UNION SELECT DISTINCT rtrim(LASTGRD2), 2 FROM DCC_PACKET01 WHERE LASTGRD2 IS NOT NULL) AA ORDER BY AA.B, F_CREDIT_GRADE_NUM(AA.A)")
	f_ds_select_q("ds_se_LASTGRD2_e","SELECT AA.A select_1 FROM (SELECT '전체' A, 1 B FROM dual UNION SELECT DISTINCT rtrim(LASTGRD2), 2 FROM DCC_PACKET01 WHERE LASTGRD2 IS NOT NULL) AA ORDER BY AA.B, F_CREDIT_GRADE_NUM(AA.A)")
	f_ds_select_q("ds_se_DECISION_s","SELECT AA.A select_1 FROM (SELECT '전체' A, 1 B FROM dual UNION SELECT DISTINCT rtrim(DECISION), 2 FROM DCC_PACKET11 WHERE DECISION IS NOT NULL) AA ORDER BY AA.B, AA.A")
	f_ds_select_q("ds_se_DECISION_e","SELECT AA.A select_1 FROM (SELECT '전체' A, 1 B FROM dual UNION SELECT DISTINCT rtrim(DECISION), 2 FROM DCC_PACKET11 WHERE DECISION IS NOT NULL) AA ORDER BY AA.B, AA.A")

	se_year.text = f_sysdate().substr(0,4)
	se_LASTGRD2_s.CodeValue = 'A'

	btnquery_onclick()
}
//---------------------------------------------------------------------
function wbs_search()                     //전문 공종 찾기 
//---------------------------------------------------------------------
{
	var result;
	var array = new Array();

	result = window.showModalDialog(gs_home + "/const/s_web/00/s_profession_wbs_search_1h.html", se_profession_wbs_name.value,"dialogWidth:600px;dialogHeight:500px");
	if(result == "") return;

	array = result.split("@");
	se_profession_wbs_code.value = array[0]
	se_profession_wbs_name.value = array[1]
}
