<SCRIPT language=JavaScript>
var gs_home
var is_ds_1_addr = 's_sbcr_chg_1q.jsp?arg_sbcr_code='                        //품의 목록 
var is_tr_1_addr 
var is_ds_seq_addr
var is_dept_code = '',is_sbcr_code
var is_date
var is_confirm_check = 'no'
//---------------------------------------------------------------
function btnquery_onclick()               //조회 
//---------------------------------------------------------------
{
	var arg_cmd
	
	ds_1.DataID = is_ds_1_addr + is_sbcr_code
	ds_1.SyncLoad=true
	ds_1.reset()
}
//-----------------------------------------------------------------------
function init_move()                       // 초기값
//------------------------------------------------------------------------
{
	var ll_row,ll_unq
	if (ds_1.RowPosition < 2) {
	   arg_cmd = "  INSERT INTO S_SBCR_CHG  " +
	             "  SELECT SBCR_CODE,m_spec_unq_no.nextval,sysdate,SBCR_NAME,BUSINESSMAN_NUMBER,   " +
	             "         CORP_NO,ZIP_NUMBER1,ADDRESS1,TEL_NUMBER1,REP_NAME1,REP_NAME2 " +
	             "    FROM S_SBCR  WHERE sbcr_code = '" + is_sbcr_code + "'"
	
	   f_update_sql(arg_cmd)
		ds_1.reset()
		ds_1.UserStatus(1) = 1;
		gd_1.Editable="true"
		return
	}	
	
  	ds_seq.DataID = is_ds_seq_addr                       // SEQ NO 의 DATAID
	ds_seq.SyncLoad=true;             // seq no 구하기
	ds_seq.reset()
	ll_row = ds_1.RowPosition - 1;
	ll_unq = ds_seq.NameValue(1,"nextval")
	ds_1.NameValue(ds_1.RowPosition,"detail_unq_num")     = ll_unq
	ds_1.NameValue(ds_1.RowPosition,'sbcr_code')          = is_sbcr_code
	ds_1.NameValue(ds_1.RowPosition,'chg_dt')             = is_date
	ds_1.NameValue(ds_1.RowPosition,'sbcr_name')          = ds_1.NameValue(ll_row,'sbcr_name')
	ds_1.NameValue(ds_1.RowPosition,'businessman_number') = ds_1.NameValue(ll_row,'businessman_number')
	ds_1.NameValue(ds_1.RowPosition,'corp_no')            = ds_1.NameValue(ll_row,'corp_no')
	ds_1.NameValue(ds_1.RowPosition,'zip_number1')        = ds_1.NameValue(ll_row,'zip_number1')
	ds_1.NameValue(ds_1.RowPosition,'address1')           = ds_1.NameValue(ll_row,'address1')
	ds_1.NameValue(ds_1.RowPosition,'tel_number1')        = ds_1.NameValue(ll_row,'tel_number1')
	ds_1.NameValue(ds_1.RowPosition,'rep_name1')          = ds_1.NameValue(ll_row,'rep_name1')
	ds_1.NameValue(ds_1.RowPosition,'rep_name2')          = ds_1.NameValue(ll_row,'rep_name2')
}
//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
	
	ds_1.AddRow()
	init_move()
}
//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
	var ll_row,ls_sbcr_name,ls_businessman_number,ls_corp_no,ls_zip_number,ls_addr,ls_tel,ls_rep1,ls_rep2
	var ls_unq
	
	if  (confirm("삭제하시면 바로 저장이됩니다. 계속하시겠습니까?")){
		if (ds_1.RowPosition < 2){
			ds_1.DeleteRow(1)
			tr_1.Post()
			return
		}
		ll_row = ds_1.RowPosition - 1;
		ls_sbcr_name          = ds_1.NameValue(ll_row,'sbcr_name')
		ls_businessman_number = ds_1.NameValue(ll_row,'businessman_number')
		ls_corp_no            = ds_1.NameValue(ll_row,'corp_no')
		ls_zip_number         = ds_1.NameValue(ll_row,'zip_number1')
		ls_addr               = ds_1.NameValue(ll_row,'address1')
		ls_tel                = ds_1.NameValue(ll_row,'tel_number1')
		ls_rep1               = ds_1.NameValue(ll_row,'rep_name1')
		ls_rep2               = ds_1.NameValue(ll_row,'rep_name2')
		ls_unq = ds_1.NameValue(ds_1.RowPosition,'detail_unq_num')
		
	   arg_cmd = "  update S_SBCR  " +
	             "     set sbcr_name = '" + ls_sbcr_name + "', businessman_number = '" + ls_businessman_number + "'," +
	             "         corp_no = '" + ls_corp_no + "', zip_number1 = '" + ls_zip_number + "', " +
	             "         address1 = '" + ls_addr + "', tel_number1 = '" + ls_tel + "', " +
	             "         rep_name1 = '" + ls_rep1 + "', rep_name2 = '" + ls_rep2 + "'" +
	             "   where sbcr_code = '" + is_sbcr_code + "'" + 
	             " @ delete from s_sbcr_chg where sbcr_code = '" + is_sbcr_code + "' and detail_unq_num = " + ls_unq
	
	   f_update_sql_multy(arg_cmd)
		ds_1.reset()
		
	}
}
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
	if (ds_1.IsUpdated){
		if  (confirm("변경되었습니다 저장하시겠습니까?")){
     		tr_1.Post()
		}
	}
}
//----------------------------------------------------------------
function Button_close_onclick()               //닫기
//----------------------------------------------------------------
{
	btnsave_onclick()
	window.returnValue = ''
	window.close();
}
//----------------------------------------------------------------------------
function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
	ds_1.Undo(ds_1.RowPosition)
}

//------------------------------------------------------------------------
function get_focus(focus) // gd가 포커스를 받을때 동작.
//------------------------------------------------------------------------
{
//	il_screen = focus
}

//----------------------------------------------------------------
function window_onload()                  // window open event 
//-----------------------------------------------------------------
{
	var result;
	var array = new Array();
	result = window.dialogArguments;
	array = result.split("@");
	gs_home          = array[0];     
	is_sbcr_code   = array[1];
	is_date = array[2];
	is_tr_1_addr = gs_home + '/const/s_web/98/s_sbcr_chg_1tr.jsp'
	is_ds_seq_addr = gs_home + '/const/m_web/00/m_spec_unq_no_1q.jsp'                 // seq_no구하기 
	tr_1.Action = is_tr_1_addr
   btnquery_onclick()
}
//-----------------------------------------------------------------
function window_onunload()     // windoe.close()문장을 만나면 이루틴은 자동으로 수행됨으로 복사여부를 체크.
//-----------------------------------------------------------------
{
  if (is_confirm_check == 'no')
      window.returnValue = ""
}
 
</SCRIPT>
<script language="javascript" for=gd_1 event=OnPopup(row,colid,data)>
	var result,ls_code;
	var array = new Array();
	if  (colid == "zip_number1") { 
		ls_code = ds_1.NameValue(row,'zip_number1')
		result = window.showModalDialog(gs_home + "/comm_search/z_zip_find_1h.html", ls_code,"dialogWidth:640px;dialogHeight:500px");
		if(result == "") return;
	
		array = result.split("@");
		ds_1.NameValue(row,'zip_number1') = array[0]
		ds_1.NameValue(row,'address1')    = array[1]
		return
	}
</SCRIPT>

