function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
	var ls_chk=0,ls_spec_chk=0
 	if (ds_2.IsUpdated ){
 	   if (tb_1.ActiveIndex == 2) {
 	      var ll_dup
	      ll_dup = f_dup_chk("ds_2","profession_wbs_code")
	      if (ll_dup > 0) {
	          alert("등록공종이 중복되었습니다:등록공종을  수정하십시요 ")
	          ds_2.RowPosition = ll_dup
          	 gd_2.SetColumn("profession_wbs_code");
	          return
	      }
 	   }
 	}
	if (ds_1.IsUpdated || ds_2.IsUpdated || ds_9.IsUpdated){
		if  (confirm("변경되었습니다 저장하시겠습니까?")){
			if (ds_1.IsUpdated ) {
       		tr_1.Post()
       	}
			if (ds_9.IsUpdated ) {
       		tr_9.Post()
			}			
			if (ds_2.IsUpdated ) {
				if ( tb_1.ActiveIndex != 10 && tb_1.ActiveIndex != 11) {
				 	tr_2.Action   = is_tr_2_addr[il_tagpage]
				 	tr_2.KeyValue = is_tab_addr_tr_keyvalue[il_tagpage]
	     			tr_2.Post()
	     		}
			}
		}
	}
}
