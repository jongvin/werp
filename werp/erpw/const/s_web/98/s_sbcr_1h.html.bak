//-----------------------------------------------------------------------
function init_move()               // 초기값세팅
//-----------------------------------------------------------------------
{
	var ll_seq
	
	if ( il_screen == 1 ) {
		return
	}
	else {
		ds_2.NameValue(ds_2.RowPosition,'sbcr_code') = ds_1.NameValue(ds_1.RowPosition,'sbcr_code')
		if (tb_1.ActiveIndex == 2){
			ds_seq.SyncLoad=true;             // seq no 구하기
			ds_seq.reset()
			ds_2.NameValue(ds_2.RowPosition,"seq") = ds_seq.NameValue(1,"nextval")
		}

		if ( tb_1.ActiveIndex == 5 ) {
			ds_2.NameValue(ds_2.RowPosition,'class') = '1'
			ds_2.NameValue(ds_2.RowPosition,'spec_seq') =ds_2.Max(3,0,0) + 1;
		}
		if ( tb_1.ActiveIndex == 6 ) {
			ds_2.NameValue(ds_2.RowPosition,'class') = '2'
			ds_2.NameValue(ds_2.RowPosition,'spec_seq') =ds_2.Max(3,0,0) + 1;
		}
		if ( tb_1.ActiveIndex == 7 || tb_1.ActiveIndex == 3 || tb_1.ActiveIndex ==4 ) {
			ds_2.NameValue(ds_2.RowPosition,'seq') =ds_2.Max(2,0,0) + 1;
		}
		if ( tb_1.ActiveIndex == 8 ) {
			ds_2.NameValue(ds_2.RowPosition,'othercompany_seq') =ds_2.Max(2,0,0) + 1;
		}
		if ( tb_1.ActiveIndex == 12 ) {
			ds_2.NameValue(ds_2.RowPosition,'imp_class') ='N'
		}
	}
	

	Fn_SetColumn()
}

//-----------------------------------------------------------------------
function top_init_move()               // 초기값세팅
//-----------------------------------------------------------------------
{
	var ll_cnt,ls_sbcr_code,ll_seq,ls_businessman_number,ll_row,ls_wbs_code,ls_wbs_name
	var result;
	var array = new Array();
	
	result = window.showModalDialog(gs_home + "/const/s_web/98/s_businessman_insert_1h.html",'',"dialogWidth:410px;dialogHeight:160px");
	if(result == "") return;
	if(result == "@") return;
	array = result.split("@");
	ls_businessman_number = array[0]
	if(ls_businessman_number.replace(/[ ]/gi,"") == "") return;

	ll_cnt = f_select_q("select nvl(count(*),0) select_1 from s_sbcr where businessman_number = '" + ls_businessman_number + "' and nvl(sbcr_class,'2') <> '1'")
	if (ds_select.ErrorCode != 0) {alert("에러발행");return;}
	if (parseInt(ds_select.NameValue(ds_select.RowPosition,'select_1')) > 0 ){
		alert("현장구매 등록업체 입니다. 전산실에 확인하세요");
		return
	}

	ll_cnt = f_select_q("select nvl(count(*),0) select_1 from s_sbcr where businessman_number = '" + ls_businessman_number + "'")
	if (ds_select.ErrorCode != 0) {alert("에러발행");return;}
	if (parseInt(ds_select.NameValue(ds_select.RowPosition,'select_1')) > 0 ){
		ll_row = ds_1.NameValueRow('businessman_number',ls_businessman_number)
		if (ll_row < 1) {
		    em_from_date.text = ''
			tx_sbcr_search_name.value = ''
			btnquery_onclick()
			ll_row = ds_1.NameValueRow('businessman_number',ls_businessman_number)
		}
		ds_1.RowPosition = ll_row
		ls_sbcr_code = ds_1.NameValue(ll_row,'sbcr_code')
		if (ls_businessman_number != ds_1.NameValue(ll_row,'businessman_number')) {
			alert("사업자번호가 틀렸습니다.");
			return;
		}
	}
	else {
		ll_cnt = f_select_q("select substrb(max(sbcr_code),2,length(max(sbcr_code)))! 1 select_1 from s_sbcr where substrb(sbcr_code,1,2) = 'S0'")
		if (ds_select.ErrorCode != 0) {alert("에러발행");return;}
		if (ll_cnt > 0) {
			if (ds_select.NameValue(ds_select.RowPosition,'select_1') != ''){
				ll_seq = parseInt(ds_select.NameValue(ds_select.RowPosition,'select_1'))
	
				if ( ll_seq < 10) {
					ls_sbcr_code = 'S0000' + ll_seq;
				}
				else {
					if (ll_seq < 100) {
						ls_sbcr_code = 'S000' + ll_seq;
					}
					else {
						if (ll_seq < 1000) {
							ls_sbcr_code = 'S00' + ll_seq;
						}
						else {
							if (ll_seq < 10000) {
								ls_sbcr_code = 'S0' + ll_seq;
							}
							else {
								ls_sbcr_code = 'S' + ll_seq;
							}
						}
					}
				}
			}
			else  {
				ls_sbcr_code = 'S00001'
			}
		}	
		else {
			ls_sbcr_code = 'S00001'
		}
		if (ds_1.RowPosition < 1) {
			ll_row = ds_1.AddRow()
		}
		else {
			ll_row = ds_1.InsertRow(ds_1.RowPosition)
		}
		ds_1.NameValue(ds_1.RowPosition,'sbcr_code') = ls_sbcr_code
		ds_1.NameValue(ds_1.RowPosition,'businessman_number') = ls_businessman_number
		ds_1.NameValue(ds_1.RowPosition,'sbcr_class') = '1'
		ds_1.NameValue(ds_1.RowPosition,'bill_dis_pay') = 'Y'
		ds_1.NameValue(ds_1.RowPosition,'use_tag') = 'Y'
		tr_1.Post()
	}
	result = window.showModalDialog(gs_home + "/const/s_web/00/s_profession_wbs_search_1h.html", '',"dialogWidth:600px;dialogHeight:500px");
	if(result == "") return;
	if(result == "@") return;
	array = result.split("@");
	ls_wbs_code = array[0]
	ls_wbs_name = array[1]
	if(ls_wbs_code.replace(/[ ]/gi,"") == "") return;

	ll_cnt = f_select_q("select nvl(count(*),0) select_1 from s_wbs_request where sbcr_code = '" + ls_sbcr_code + 
	                    "' and profession_wbs_code = '" + ls_wbs_code + "'")
	if (ds_select.ErrorCode != 0) {alert("에러발행");return;}
	if (parseInt(ds_select.NameValue(ds_select.RowPosition,'select_1')) > 0 ){
		alert('이미 등록되어있는 공종입니다...')
		return
	}

	ds_9.AddRow()
	ds_9.NameValue(ds_9.RowPosition,'sbcr_code') = ls_sbcr_code
	ds_9.NameValue(ds_9.RowPosition,'register_chk') = '3'
	ds_9.NameValue(ds_9.RowPosition,'register_date') = is_work_date
	ds_9.NameValue(ds_9.RowPosition,'grantee_rt') = 10
	ds_9.NameValue(ds_9.RowPosition,'use_tag') = 'Y'

	tx_profession_wbs_code.value = ls_wbs_code
	tx_profession_wbs_name.value = ls_wbs_name
	ds_1.NameValue(ds_1.RowPosition,'profession_wbs_name') = ls_wbs_name
	Fn_SetColumn()
}
