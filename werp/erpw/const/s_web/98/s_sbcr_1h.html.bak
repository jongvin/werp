function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
	if ( il_screen == 1 ) {
		return
	}
	if ( il_screen == 1 ) {
		top_init_move()
		return
	}
	if ( il_screen == 9 ) {
		return
	}
	else {
		if ( tb_1.ActiveIndex == 1 ) {
			ds_1.AddRow()
			top_init_move()
			return
		}
		if( tb_1.ActiveIndex == 9 || tb_1.ActiveIndex == 10 || tb_1.ActiveIndex == 11){
			return
		}
		ds_2.AddRow()
		init_move()

	}
}
