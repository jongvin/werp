function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{
   is_from_date = em_from_date.text.substr(0,4) 

	is_sbcr_name = tx_sbcr_search_name.value

	ds_1.DataID = is_ds_1_addr + is_from_date + '&arg_sbcr_name=' + is_sbcr_name 
	              + '&arg_wbs_code=' + tx_wbs_code.value + '&arg_region_code=' + tx_region_code.value + '&arg_use_tag=' + ccom_use_tag.CodeValue
	ds_1.SyncLoad=true
	ds_1.Reset()

	div_btnquery_onclick()
}
//------------------------------------------------------------------------
function div_btnquery_onclick()                     // 하위 조회  
//------------------------------------------------------------------------
{
	var li_sbcr_code,ls_wbs_code,i,j
	if (ds_1.RowPosition < 1) {
		ds_2.ClearData()
		return 
	}
	li_sbcr_code  = ds_1.NameValue(ds_1.RowPosition,'sbcr_code')
	ls_wbs_code   = ds_1.NameValue(ds_1.RowPosition,'profession_wbs_code')

	ds_9.DataID = is_ds_9_addr + li_sbcr_code + '&arg_wbs_code=' + ls_wbs_code 
	ds_9.SyncLoad=true
	ds_9.Reset()

	if ( tb_1.ActiveIndex != 1){
		ds_2.DataID = is_ds_2_addr[tb_1.ActiveIndex] + li_sbcr_code 
		ds_2.SyncLoad=true
		ds_2.Reset()	
		if (tb_1.ActiveIndex == 9){
			if (ds_2.CountRow < 1){
				j=0;
				for (i=1 ;i <=15 ;i++)
				{
					j++;
					ds_2.AddRow()
					if (i < 6){
						ds_2.NameValue(ds_2.RowPosition,'year_class') = '1'
					}
					else {
						if ( i < 11){
							ds_2.NameValue(ds_2.RowPosition,'year_class') = '2'
						}
						else {
							ds_2.NameValue(ds_2.RowPosition,'year_class') = '3'
						}
					}
					ds_2.NameValue(ds_2.RowPosition,'sbcr_code') = li_sbcr_code
					ds_2.NameValue(ds_2.RowPosition,'seq') = j;
					if ( j == 5){
						j = 0;
					}
				}
			 	tr_2.Action   = is_tr_2_addr[9]
			 	tr_2.KeyValue = is_tab_addr_tr_keyvalue[9]
	  			tr_2.Post()
	  		}
		}
	}
	
}
