<SCRIPT language=JavaScript for=ds_2 event=OnColumnChanged(row,colid)>
var ll_amt,ll_rt
/*
if (ds_2.OrgNameValue(row,"approve_class") > "1") {
   ds_2.NameValue(row,colid) =  ds_2.OrgNameValue(row,colid)
   alert("요청된 자료는 수정할수 없습니다")
   return false
}
*/
if (colid == "supply_amt_tax" ) {
	ds_2.NameValue(row,'vat') = parseInt(ds_2.NameValue(row,'supply_amt_tax') * 0.1,10)
}
if (colid == "tot_noamt" ) {
	ds_2.NameValue(row,'supply_amt_notax') = ds_2.NameValue(row,'tot_noamt') - ds_2.NameValue(row,"purchase_vat");
}

if (colid == "previous_amt_rt" ) {
   ds_2.NameValue(row,'safety_amt1') = parseInt(ds_2.NameValue(row,'supply_amt_tax') * ds_2.NameValue(row,'previous_amt_rt') * 0.01 ,10)
   ds_2.NameValue(row,'safety_amt2') = parseInt(ds_2.NameValue(row,'tot_noamt') * ds_2.NameValue(row,'previous_amt_rt') * 0.01 ,10)
   ds_2.NameValue(row,'previous_vat') = parseInt(ds_2.NameValue(row,'vat') * ds_2.NameValue(row,'previous_amt_rt') * 0.01 ,10)
   ds_2.NameValue(row,'previous_amt') = parseInt((ds_2.NameValue(row,'supply_amt_tax') + ds_2.NameValue(row,'tot_noamt') + ds_2.NameValue(row,'vat')) * ds_2.NameValue(row,'previous_amt_rt') * 0.01 ,10)
}
if (colid == "vat" || colid == 'supply_amt_tax' || colid == 'supply_amt_notax' || colid == 'purchase_vat' || colid == 'tot_noamt'){
   em_comp_tax_amt.text = ds_2.NameValue(row,"supply_amt_tax") + ds_2.NameValue(row,"vat")
   em_comp_notax_amt.text = ds_2.NameValue(row,"supply_amt_notax") + ds_2.NameValue(row,"purchase_vat") + ds_2.NameValue(row,"supply_amt_tax")
   ds_2.NameValue(row,'sbc_amt') = ds_2.NameValue(row,"supply_amt_notax") + ds_2.NameValue(row,"purchase_vat") + ds_2.NameValue(row,"supply_amt_tax") + ds_2.NameValue(row,"vat")

   ds_2.NameValue(row,'safety_amt1') = parseInt(ds_2.NameValue(row,'supply_amt_tax') * ds_2.NameValue(row,'previous_amt_rt') * 0.01 ,10)
   ds_2.NameValue(row,'safety_amt2') = parseInt(ds_2.NameValue(row,'tot_noamt') * ds_2.NameValue(row,'previous_amt_rt') * 0.01 ,10)
   ds_2.NameValue(row,'previous_vat') = parseInt(ds_2.NameValue(row,'vat') * ds_2.NameValue(row,'previous_amt_rt') * 0.01 ,10)
   ds_2.NameValue(row,'previous_amt') = parseInt((ds_2.NameValue(row,'supply_amt_tax') + ds_2.NameValue(row,'tot_noamt') + ds_2.NameValue(row,'vat')) * ds_2.NameValue(row,'previous_amt_rt') * 0.01 ,10)
}
   
if (colid == "prgs_cash_rt") 
   ds_2.NameValue(row,"prgs_bill_rt") = 100 -  ds_2.NameValue(row,"prgs_cash_rt")   
if (colid == "prgs_bill_rt") 
   ds_2.NameValue(row,"prgs_cash_rt") = 100 -  ds_2.NameValue(row,"prgs_bill_rt")   
if (colid == 'sbc_guarantee_rt') {
	ds_2.NameValue(row,'sbc_guarantee_amt') = parseInt(ds_2.NameValue(row,'sbc_amt') * ds_2.NameValue(row,'sbc_guarantee_rt') * 0.01,10);
}
if (colid == 'warrant_guarantee_rt') {
	ds_2.NameValue(row,'warrant_guarantee_amt') = parseInt(ds_2.NameValue(row,'sbc_amt') * ds_2.NameValue(row,'warrant_guarantee_rt') * 0.01,10);
}
