//------------------------------------------------------------------------
function div_btndelete_onclick()                //하위 삭제
//------------------------------------------------------------------------
{
 tr_2.Action = is_tr_2_addr
 if  (is_dept_code == "") return
 if  (ds_2.RowPosition < 1) return
 if (ds_2.NameValue(ds_2.RowPosition,'chg_degree') == 1) {
 	alert("최초계약은 현장에서 삭제하실 수 없습니다...")
 	return
 }
 if  (ds_2.NameValue(ds_2.RowPosition,"approve_class") != "1") {
     alert("작성중이 아니므로 삭제 할수 없습니다")
     return
 }     
  ls_sbcr_code  = ds_2.NameValue(ds_2.RowPosition,"sbcr_code") 
  ll_chg_degree = ds_2.NameValue(ds_2.RowPosition,"chg_degree")
  ll_order_number = ds_2.NameValue(ds_2.RowPosition,"order_number")
  ll_chg_degree = ds_2.NameValue(ds_2.RowPosition,"chg_degree")
   ds_2.DeleteRow(ds_2.RowPosition)
  if  (confirm("삭제되었습니다 저장하시겠습니까?")){
		  arg_cmd = "delete from s_chg_cn_detail   " +   
		                 "  where (dept_code = '" + is_dept_code + "' and "   + 
		                 "         order_number = " + ll_order_number + " and "   + 
				           "         chg_degree = " + ll_chg_degree + ")   "   
		   f_update_sql(arg_cmd)
		   arg_cmd = "delete from s_chg_cn_parent   " +   
		                 "  where (dept_code = '" + is_dept_code + "' and "   + 
		                 "         order_number = " + ll_order_number + " and "   + 
				           "         chg_degree = " + ll_chg_degree + ")   "   
		   f_update_sql(arg_cmd)
		   tr_2.Post()
		  if  (ds_2.CountRow == 0) {
		      arg_cmd = "update s_estimate_list set " + 
		                           " select_yn='N'  " + 
		                           " where (dept_code= '" + is_dept_code + "' and  " + 
		                           " order_number= " + ll_order_number + " and " + 
		                           " sbcr_code = '" + ls_sbcr_code + "') "
		      f_update_sql(arg_cmd)
		      arg_cmd = "update s_order_list set " + 
		                    " approve_class='4',    " + 
		                    " sbcr_code=''   " + 
		                    " where (dept_code= '" + is_dept_code + "' and " + 
		                    " order_number=" + ll_order_number + ")";  
		      f_update_sql(arg_cmd) 
		   } 
  }  
   il_screen = 1   //parent를 다시 읽음 
	btnquery_onclick()

 }
