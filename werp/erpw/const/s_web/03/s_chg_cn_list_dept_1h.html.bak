//------------------------------------------------------------------------
function f_start_end_chk()                       //종료일자가 시작일자보다 반드시 크거나 같아야하는 로직 체크
//------------------------------------------------------------------------
{
  var ll_ret_row
     ll_ret_row = gf_gd_start_end_date('ds_2','plan_start_dt','plan_end_dt')
	  if (ll_ret_row > 0)  {
	     alert('공사기간(당초)의 종료일자가 시작일자보다 반드시 커야합니다')
	     ds_2.RowPosition = ll_ret_row	      
	     return false
	  }   
     ll_ret_row = gf_gd_start_end_date('ds_2','start_dt','end_dt')
	  if (ll_ret_row > 0)  {
	     alert('공사기간(변경)의 종료일자가 시작일자보다 반드시 커야합니다')
	     ds_2.RowPosition = ll_ret_row	      
	     return false
	  }   
     ll_ret_row = gf_gd_start_end_date('ds_2','sbc_guarantee_start_dt','sbc_guarantee_end_dt')
	  if (ll_ret_row > 0)  {
	     alert('보증기간(계약)의 종료일자가 시작일자보다 반드시 커야합니다')
	     ds_2.RowPosition = ll_ret_row	      
	     return false
	  }   
     ll_ret_row = gf_gd_start_end_date('ds_2','warrant_guarantee_start_dt','warrant_guarantee_end_dt')
	  if (ll_ret_row > 0)  {
	     alert('보증기간(하자)의 종료일자가 시작일자보다 반드시 커야합니다')
	     ds_2.RowPosition = ll_ret_row	      
	     return false
	  }   
  return true        
}

