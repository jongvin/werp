//------------------------------------------------------------------------
function request_rtn()                       //요청처리
//------------------------------------------------------------------------
{
	var ls_approve_class,ls_sbc_dt,ls_estimate_dt,ls_chg_degree
	if (ds_2.RowPosition < 1 ) return
	
	ls_approve_class = ds_2.NameValue(ds_2.RowPosition,'approve_class')
	ls_sbc_dt = ds_2.NameValue(ds_2.RowPosition,'sbc_dt')
	ls_chg_degree = ds_2.NameValue(ds_2.RowPosition,'chg_degree')
	
	if (ls_chg_degree == 1){
		alert('최초계약 요청은 현장에서 하실 수 없습니다...')
		return
	}
	
	if (ls_approve_class != '1') {
		alert('작성중에서만 요청이 가능합니다...')
		return
	}
	if (ls_sbc_dt == '') {
		alert('계약일자는 반드시 입력되어야 요청이 가능합니다...')
		return
	}
	
	if  (confirm("승인요청을하면 수정할 수 없습니다 계속하시겠습니까?")){
	}
	else {
		return
	}
	
	if (ds_2.IsUpdated){
	   if  (confirm("변경되었습니다 저장하시겠습니까?"))
			 tr_2.Action = is_tr_2_addr
	       tr_2.Post()
	}
	
	ld_date = f_sysdate()
	ds_2.NameValue(ds_2.RowPosition,"request_dt")  = ld_date
	ds_2.NameValue(ds_2.RowPosition,"approve_class") = '2'
   tr_2.Action = is_tr_2_addr
	tr_2.post()
}

//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
    top.frames['main_title'].mouse_over()
  top.frames['main_title'].title_name.value = " " + "계약대장(현장용)"  

	 ll_cnt = f_select_q("select etc_code select_1, child_name select_2  from z_code_etc_child where class_tag = '022' order by etc_code") //승인구분
	is_dept_code = top.frames['security'].gs_dept_code.value
	is_dept_name = top.frames['security'].gs_dept_name.value

	dept_name.value = is_dept_name

   result = readCookie("@s_chg_cn_list_ret@")   //계약 승인 처리 에서 왔는지 체크하는 곳.
   if (result == "err"){
       div_sbc_ret.style.visibility="hidden"
   }    
   else {
        	var array = new Array();
	      array = result.split("@");
         is_dept_code = array[0]     // 승인처리에서 넘어온 부서코드
         is_dept_name = array[1]     // 승인처리에서 넘어온 부서명
         il_order_number = parseInt(array[2])  // 승인처리에서 넘어온 발주번호
         il_chg_degree = parseInt(array[3])  // 승인처리에서 넘어온 변경차수
         is_return = array[4]
         dept_name.value = is_dept_name
       div_sbc_ret.style.visibility="visible"
   }       
	is_sec = gf_initial_security()
	if (is_sec == 'r') {
	   button_query0.style.visibility='hidden'
	}

	tr_2.Action = is_tr_2_addr
   setTimeout("btnquery_onclick()",1)  
	tb_1.ActiveIndex = 1
	f_select2_q("select vattag select_1,tax_rate select_2,free_rate select_3 from z_code_dept where dept_code = '" + is_dept_code + "'")
	is_vattag   = ds_select2.NameValue(ds_select2.RowPosition,'select_1')
	il_tax_rt   = ds_select2.NameValue(ds_select2.RowPosition,'select_2')
	il_notax_rt = ds_select2.NameValue(ds_select2.RowPosition,'select_3')

}
