//--------------------------------------------------------------------------
function   make_rtn()
//----------------------------------------------------------------
{
	var ls_class,ls_data,ls_reportname,ls_parm
	var ll_unq_num,ll_cnt,ll_chk
	var ls_dept_code,ls_order_number,ls_degree,ls_const_no,ls_unque_file_name,ls_order_class

	if (ds_1.RowPosition < 1) { 
		alert("선택한 내역이 없기때문에 계약서를 생성하실 수 없습니다")
		return
	}

	ls_class        = ds_1.NameValue(ds_1.RowPosition,"approve_class")
	ls_dept_code    = ds_1.NameValue(ds_1.RowPosition,"dept_code")
	ls_order_number = ds_1.NameValue(ds_1.RowPosition,"order_number")
	ls_degree       = ds_1.NameValue(ds_1.RowPosition,"chg_degree")
	ls_order_class  = ds_1.NameValue(ds_1.RowPosition,'order_class')
	ls_const_no     = ls_order_number

	ll_cnt = f_select2_q("select NVL(COUNT(*),0) select_1  from s_const_add_file where dept_code = '" + ls_dept_code + "' and cst_doc_no = '" + ls_const_no + "' and mod_no = " + ls_degree + " and cre_by is null") //
	if (ll_cnt > 0 ) {
		ll_chk = ds_select2.NameValue(ds_select2.RowPosition,'select_1')
		if (ls_order_class == '1') {     //외주
			if (ls_degree == 1) {
				ls_unque_file_name = "s_cn_contract.pdf"
				ls_reportname = "C:\\WISENET\\Report\\b2b_rpt\\s_cn_contract.rpt"
			} else {
				ls_unque_file_name = "s_chg_cn_contract.pdf"
				ls_reportname = "C:\\WISENET\\Report\\b2b_rpt\\s_chg_cn_contract.rpt"
		  	}
		} else {   //납품설치
   			if (ls_degree == 1) {
				ls_unque_file_name = "s_cn_contract_1.pdf"
				ls_reportname = "C:\\WISENET\\Report\\b2b_rpt\\s_cn_contract_1.rpt"
			} else {
				ls_unque_file_name = "s_chg_cn_contract_1.pdf"
				ls_reportname = "C:\\WISENET\\Report\\b2b_rpt\\s_chg_cn_contract_1.rpt"
			}
		}
		if (ll_chk == 0) {
			if ( ls_class == '3') {
				if  (confirm("계약서를 생성하시겠습니까?")) {
					ds_seq.SyncLoad=true;             // seq no 구하기
					ds_seq.reset()
					ll_unq_num = ds_seq.NameValue(1,"nextval")
					ls_parm = "sm_code=s_web" + 
						      "&dept_Code=" + ls_dept_code + 
					          "&order_number=" + ls_order_number + 
							  "&chg_degree=" + ls_degree +
							  "&rptname=" + ls_reportname +
							  "&expname=" + ls_unque_file_name + 
							  "&seq=" + ll_unq_num
					//frames['exportFrame'].location.href("http://rpt.worldro.net:100/EXPORTHTML.asp?" + ls_parm)
					window.showModalDialog("http://rpt.worldro.net:100/EXPORTHTML.asp?" + ls_parm,null,"dialogWidth:300px;dialogHeight:100px");
					btnquery_onclick();
					return
				}
			}
		} else {
		   ls_data = gs_home + "@" +ls_dept_code + "@" + 
		             ls_const_no + "@" + 
		             ls_degree + "@" + 
		             ls_unque_file_name + "@" + ls_order_number + "@" +
		             gs_home + "/const/s_web/03/s_sbc_list_make_1h.html"
			makeCookie("@s_sbc_status@",ls_data)
			top.frames['main_ct'].location.href(gs_home + "/const/s_web/03/s_sbc_status_1h.html" +"?"+'r')
		}
	}
}
//--------------------------------------------------------------------------
function   sbcr_search()
//----------------------------------------------------------------
{
	if (tx_dept_search_name.value == '') {
		is_long_name = ''
	}
	else {
		is_long_name = tx_dept_search_name.value
	}
	if (tx_sbcr_search_name.value == '') {
		is_sbcr_name = ''
	}
	else {
		is_sbcr_name = tx_sbcr_search_name.value
	}
	btnquery_onclick()
}
//--------------------------------------------------------------------------
function   rd_1_proc()
//----------------------------------------------------------------
{
	var ll_cnt,ll_chk,ls_dept_code,ls_order_number,ls_degree,ls_const_no
	bt_make.value = '계약서생성'
	bt_1.checked=true
	bt_2.checked=false
	bt_3.checked=false
	bt_4.checked=false
	bt_5.checked=false
	is_filter = "1p"
	filter_function()
	if (ds_1.CountRow > 0) {
		ls_dept_code    = ds_1.NameValue(1,'dept_code')
		ls_order_number = ds_1.NameValue(1,'order_number')
		ls_degree       = ds_1.NameValue(1,'chg_degree')
		ls_const_no = ls_order_number
	 	ll_cnt = f_select2_q("select NVL(COUNT(*),0) select_1  from s_const_add_file where dept_code = '" + ls_dept_code + "' and cst_doc_no = '" + ls_const_no + "' and mod_no = " + ls_degree + " and cre_by is null") //
	 	if (ll_cnt > 0 ) {
	 		ll_chk = ds_select2.NameValue(ds_select2.RowPosition,'select_1')
	 		if (ll_chk == 0) {
				bt_make.value = '계약서생성'
			}
			else {
				bt_make.value = '계약서조회'
			}
		}
	}
}
