<SCRIPT language=JavaScript for=ds_1 event=onFilter(row)>
      switch (is_filter) {
              case "tot":
                         return true
                         break;
              case "1p":
                         if (ds_1.NameValue(row,"approve_class") == '3') return true
                         else return false;
                         break;
              case "2p":
                         if (ds_1.NameValue(row,"approve_class") == '4') return true
                         else return false;
                         break;
              case "3p":
                         if (ds_1.NameValue(row,"approve_class") == '5') return true
                         else return false;
                         break;
              case "4p":
                         if (ds_1.NameValue(row,"approve_class") == '6') return true
                         else return false;
                         break;
        }
