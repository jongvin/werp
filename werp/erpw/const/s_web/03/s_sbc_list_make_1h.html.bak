<SCRIPT language=JavaScript event=OnRowPosChanged(row) for=ds_1>
	var ll_cnt,ll_chk,ls_dept_code,ls_order_number,ls_degree,ls_const_no,arg_cmd
	if (row < 1)  return
	if (is_filter == 'tot' ) {
		if(ds_1.NameValue(row,"approve_class") == '3') {
			ls_dept_code = ds_1.NameValue(row,'dept_code')
			ls_order_number = ds_1.NameValue(row,'order_number')
			ls_degree       = ds_1.NameValue(row,'chg_degree')
			ls_const_no = ls_order_number

			arg_cmd = "   UPDATE S_CHG_CN_LIST  " +
				     	 "		  SET CONST_NO = " + ls_const_no + 
			   		 "    WHERE DEPT_CODE = '" + ls_dept_code + "'" +
			   		 "      AND order_number = " + ls_order_number +
			   		 "      AND chg_degree = " + ls_degree

		   f_update_sql(arg_cmd)

		 	ll_cnt = f_select2_q("select NVL(COUNT(*),0) select_1  from s_const_add_file where dept_code = '" + ls_dept_code + "' and cst_doc_no = '" + ls_const_no + "' and mod_no = " + ls_degree + " and cre_by is null") //
		 	if (ll_cnt > 0 ) {
		 		ll_chk = ds_select2.NameValue(ds_select2.RowPosition,'select_1')
		 		if (ll_chk == 0) {
					bt_make.value = '계약서생성'
				}
				else {
					bt_make.value = '계약서조회'
				}
			}
		}
		else {
			bt_make.value = '계약서조회'
		}
	}
	if (is_filter == '1p' ) {
		ls_dept_code = ds_1.NameValue(row,'dept_code')
		ls_order_number = ds_1.NameValue(row,'order_number')
		ls_degree       = ds_1.NameValue(row,'chg_degree')
		ls_const_no = ls_order_number
	 	ll_cnt = f_select2_q("select NVL(COUNT(*),0) select_1  from s_const_add_file where dept_code = '" + ls_dept_code + "' and cst_doc_no = '" + ls_const_no + "' and mod_no = " + ls_degree + " and cre_by is null") //
	 	if (ll_cnt > 0 ) {
	 		ll_chk = ds_select2.NameValue(ds_select2.RowPosition,'select_1')
	 		if (ll_chk == 0) {
				bt_make.value = '계약서생성'
			}
			else {
				bt_make.value = '계약서조회'
			}
		}
	}

</SCRIPT>
