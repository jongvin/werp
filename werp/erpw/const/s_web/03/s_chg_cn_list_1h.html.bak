function order_contract()                     //계약서 조회.
//---------------------------------------------------------------------------
{

	ls_class        = ds_2.NameValue(ds_2.RowPosition,"approve_class")
	ls_dept_code    = ds_2.NameValue(ds_2.RowPosition,"dept_code")
	ls_order_number = ds_2.NameValue(ds_2.RowPosition,"order_number")
	ls_degree       = ds_2.NameValue(ds_2.RowPosition,"chg_degree")
	ls_order_class  = ds_2.NameValue(ds_2.RowPosition,'order_class')
	ls_const_no     = ls_order_number

	ll_cnt = f_ds_select_q("ds_contract","select NVL(COUNT(*),0) select_1  from s_const_add_file where dept_code = '" + ls_dept_code + "' and cst_doc_no = '" + ls_const_no + "' and mod_no = " + ls_degree) //
   ll_cnt = ds_contract.NameValue(1,'select_1')
   ll_cnt = parseInt(ll_cnt,10)
   if (ll_cnt == 0) {
      alert('계약서가 작성되지 않았습니다')
      return
   }   
	if (ll_cnt > 0 ) {
		ll_chk = ds_contract.NameValue(ds_contract.RowPosition,'select_1')
		if (ls_order_class == '1') {     //외주
			if (ls_degree == 1) {
				ls_unque_file_name = "s_cn_contract.pdf"
				ls_reportname = "C:\\WISENET\\Report\\b2b_rpt\\s_cn_contract.rpt"
			} else {
				ls_unque_file_name = "s_chg_cn_contract.pdf"
				ls_reportname = "C:\\WISENET\\Report\\b2b_rpt\\s_chg_cn_contract.rpt"
		  	}
		} else {   //납품설치
   			if (ls_degree == 1) {
				   ls_unque_file_name = "s_cn_contract_1.pdf"
				   ls_reportname = "C:\\WISENET\\Report\\b2b_rpt\\s_cn_contract_1.rpt"
			   } else {
				   ls_unque_file_name = "s_chg_cn_contract_1.pdf"
				   ls_reportname = "C:\\WISENET\\Report\\b2b_rpt\\s_chg_cn_contract_1.rpt"
			   }
		  }
    ls_sec = gf_initial_security()
    ls_data = gs_home + "@" +is_dept_code + "@" + 
		             ls_order_number + "@" + 
		             ls_degree + "@" + 
		             ls_unque_file_name + "@" + ls_order_number + "@" +
		             gs_home + "/const/s_web/03/s_chg_cn_list_1h.html?" + ls_sec  + "@" + "N"
			makeCookie("@s_sbc_status@",ls_data)
			top.frames['main_ct'].location.href(gs_home + "/const/s_web/03/s_sbc_status_1h.html" +"?"+'r')
  }
}

