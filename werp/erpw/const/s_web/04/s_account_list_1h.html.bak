function contract_rtn()                        //세금계산서조회
//---------------------------------------------------------------------------
{
	if (ds_1.RowPosition < 1) return
	var ls_const_no,ls_data
	
	ls_const_no = ds_1.NameValue(ds_1.RowPosition,"order_number")

   ls_data = gs_home + "@" + ds_1.NameValue(ds_1.RowPosition,"slipseq") + "@" + ls_const_no + "@" +
             gs_home + "/const/s_web/04/s_account_list_1h.html" + "@" + is_dept_code
 
	makeCookie("@s_account_status@",ls_data)

	top.frames['main_ct'].location.href(gs_home + "/const/s_web/04/s_account_status_1h.html" +"?"+'r')
}

