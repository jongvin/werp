              "&arg_yymm="  + is_yymm + 
              "&arg_seq="  +  il_seq + 
              "&arg_order_number="  +  il_order_number +   
              "&arg_spec_no_seq="  +  ds_tree_1.NameValue(ds_tree_1.RowPosition,"spec_no_seq") + 
              "&arg_chg_degree=" + il_chg_degree  
