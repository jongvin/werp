//----------------------------------------------------------------
function tot_prgs_rtn()
//----------------------------------------------------------------
{
	var ll_cnt,i
   if (ls_update == 'T') {
		alert('전표가 생성된 자료여서 수정이 불가능합니다...')
		return
	}
	if (il_screen_zindex != 2) {
		alert('기성내역에서만 일괄적용이 가능합니다...')
		return
	}
	ll_cnt = ds_1.CountRow
	if (ll_cnt < 1) {
		alert('적용할 내역이 없습니다...')
		return
	}
	for (i=1;i<=ll_cnt;i++) {
		if (ds_1.RowMark(i) == 1){
			ds_1.NameValue(i,'tot_prgs_rt') = em_tot_prgs_rt.Text
			onexit_rtn(i,'tot_prgs_rt',ds_1.OrgNameValue(i,'tot_prgs_rt'))
		}
	}

}
//----------------------------------------------------------------
function window_onload()                  // window open event 
//-----------------------------------------------------------------
{
  if (f_reject_window()) return 
   top.frames['main_title'].mouse_over()
// top.frames['main_title'].rd_mnosee_onclick()               //메뉴를 안보이게

	is_sec = gf_initial_security()
	var result="";
	var array = new Array();
	result = readCookie("@s_prgs_detail@")
   if (result == "err") {
      is_dept_code = ""
      is_dept_name = ""
      alert("정보가 맞지않아 실행할수 없습니다(부서코드/발주순번)")
      return
   }   
	else {   
	   array = result.split("@");
      is_dept_code = array[0]            //부서 코드
      is_dept_name = array[1]            //부서 명
      is_yymm      = array[2]            //기성 년월 
      il_seq      = parseInt(array[3])   //기성 차수 
      il_order_number = parseInt(array[4])  //발주 번호
      is_sbc_name = array[5]    //계약 명
      ls_update = array[6]      //수정  구분 T: 수정 못함.
    }
           
   top.frames['main_title'].title_name.value = " " + "기성내역 : "  + is_dept_name + " (" + is_sbc_name + ") 차수: "+ il_seq                
  //ds_1.SyncLoad=true
  //ds_1.reset()

   if (ls_update == 'T') {
       gd_1.Editable = false
       bt_excel.style.visibility="hidden"
//        est_ctrl_copy.style.visibility="hidden"
   }       
   gd_1.format = is_format_1 
   tr_1.Action = is_tr_1_addr
   tree_btnquery_onclick()
   tree_1_OnSelChanged()              //tree를 조회한후 집계금액을 조회한다.
//   tb_1.ActiveIndex = 1
    
}
