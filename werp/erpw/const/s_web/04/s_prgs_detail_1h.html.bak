//-------------------------------------------------------------------------------
function   onexit_rtn(row,colid,olddata)       // onexit
//-------------------------------------------------------------------------------
{
 ll_sub_qty = ds_1.NameValue(row,"sub_qty")
 ll_sub_price = ds_1.NameValue(row,"sub_price")
 ll_sub_amt = ds_1.NameValue(row,"sub_amt")
 var i
 if (colid == "tot_prgs_qty") {
 	if (ds_1.NameValue(row,'unit') == '식') {
      ds_1.NameValue(row,"tot_prgs_qty") = ds_1.OrgNameValue(row,"tot_prgs_qty")
 		alert('단위가 식일 경우에는 수량으로 입력하실 수 없습니다...')
 		return false
 	}

   if (ll_sub_amt > 0) {
	   if (ll_sub_qty < ds_1.NameValue(row,"tot_prgs_qty")) {
	      ds_1.NameValue(row,"tot_prgs_qty") = ds_1.OrgNameValue(row,"tot_prgs_qty")
	      alert("기성수량이 계약수량을 초과할 수 없습니다")
	      return false
	   }   
	}
//	else {
//	   if (ll_sub_qty > ds_1.NameValue(row,"tot_prgs_qty")) {
//	      ds_1.NameValue(row,"tot_prgs_qty") = ds_1.OrgNameValue(row,"tot_prgs_qty")
//	      alert("기성수량이 계약수량을 초과할 수 없습니다")
//	      return false
//	   }   
//	}
   ds_1.NameValue(row,"tot_prgs_amt") = Math.round(ll_sub_price * ds_1.NameValue(row,"tot_prgs_qty"))
   if (ll_sub_amt != 0) {
   	if ((ds_1.Namevalue(row,'tot_prgs_amt') / ll_sub_amt * 100) > 999){
   		ds_1.NameValue(row,'tot_prgs_rt') = 999
   	}
   	else {
	   	if ((ds_1.Namevalue(row,'tot_prgs_amt') / ll_sub_amt * 100) < -999){
	   		ds_1.NameValue(row,'tot_prgs_rt') = -999
	   	}
	   	else {
		       ds_1.NameValue(row,"tot_prgs_rt") = (ds_1.NameValue(row,"tot_prgs_amt") / ll_sub_amt * 100)
		   }
		}
   }
   else {
       ds_1.NameValue(row,"tot_prgs_rt") = 0
   }
   ds_1.NameValue(row,"tm_prgs_qty") =  ds_1.NameValue(row,"tot_prgs_qty") - ds_1.NameValue(row,"pre_prgs_qty") 
   ds_1.NameValue(row,"tm_prgs_amt") =  ds_1.NameValue(row,"tot_prgs_amt") - ds_1.NameValue(row,"pre_prgs_amt")    
   ds_1.NameValue(row,"tm_prgs_rt") =  ds_1.NameValue(row,"tot_prgs_rt") - ds_1.NameValue(row,"pre_prgs_rt")    
 }

 if (colid == "tot_prgs_amt") {
	if (ll_sub_amt  < 0) {
//		if (ll_sub_amt > ds_1.NameValue(row,"tot_prgs_amt")) {
//			ds_1.NameValue(row,"tot_prgs_amt") = ds_1.OrgNameValue(row,"tot_prgs_amt")
//			alert("기성금액이 계약금액을 초과할 수 없습니다")
//			return false
//		}
	} else {
		if (ll_sub_amt < ds_1.NameValue(row,"tot_prgs_amt")) {
			ds_1.NameValue(row,"tot_prgs_amt") = ds_1.OrgNameValue(row,"tot_prgs_amt")
			alert("기성금액이 계약금액을 초과할 수 없습니다")
			return false
		}
	}
	if (ll_sub_amt != 0) {
		if ((ds_1.NameValue(row,"tot_prgs_amt") / ll_sub_amt * 100) > 999){
			ds_1.NameValue(row,"tot_prgs_rt") = 999 
		}
		else {
			if ((ds_1.NameValue(row,"tot_prgs_amt") / ll_sub_amt * 100) < -999){
//				ds_1.NameValue(row,"tot_prgs_rt") = (ds_1.NameValue(row,"tot_prgs_amt") / ll_sub_amt * 100)
				ds_1.NameValue(row,"tot_prgs_rt") = -999
			}
			else {
				ds_1.NameValue(row,"tot_prgs_rt") = (ds_1.NameValue(row,"tot_prgs_amt") / ll_sub_amt * 100)
			}
		}
	}
	else {
		ds_1.NameValue(row,"tot_prgs_rt") = 0
	}
	
   if( ds_1.NameValue(row,'unit') != '식'){
		ds_1.NameValue(row,"tot_prgs_qty") = ll_sub_qty  * ds_1.NameValue(row,"tot_prgs_rt") / 100
		ds_1.NameValue(row,"tm_prgs_qty") =  ds_1.NameValue(row,"tot_prgs_qty") - ds_1.NameValue(row,"pre_prgs_qty")
	}
	ds_1.NameValue(row,"tm_prgs_amt") =  ds_1.NameValue(row,"tot_prgs_amt") - ds_1.NameValue(row,"pre_prgs_amt")
	ds_1.NameValue(row,"tm_prgs_rt") =  ds_1.NameValue(row,"tot_prgs_rt") - ds_1.NameValue(row,"pre_prgs_rt")
 }

 if (colid == "tot_prgs_rt") {
 	if (ds_1.NameValue(row,'tot_prgs_rt') == 999 || ds_1.NameValue(row,'tot_prgs_rt') == -999) {
 		return true
 	}
	if (ll_sub_amt  > 0) {
	   if (100 < ds_1.NameValue(row,"tot_prgs_rt")) {
	      ds_1.NameValue(row,"tot_prgs_rt") = ds_1.OrgNameValue(row,"tot_prgs_rt")
	      alert("기성율이 계약율을 초과할 수 없습니다")
	      return false
	   }   
	}
   ds_1.NameValue(row,"tot_prgs_amt") = Math.round((ll_sub_amt  * ds_1.NameValue(row,"tot_prgs_rt") / 100))
   if( ds_1.NameValue(row,'unit') != '식'){
	   ds_1.NameValue(row,"tot_prgs_qty") = ll_sub_qty  * ds_1.NameValue(row,"tot_prgs_rt") / 100
	   ds_1.NameValue(row,"tm_prgs_qty") =  ds_1.NameValue(row,"tot_prgs_qty") - ds_1.NameValue(row,"pre_prgs_qty")
	}
   ds_1.NameValue(row,"tm_prgs_amt") =  ds_1.NameValue(row,"tot_prgs_amt") - ds_1.NameValue(row,"pre_prgs_amt")    
   ds_1.NameValue(row,"tm_prgs_rt") =  ds_1.NameValue(row,"tot_prgs_rt") - ds_1.NameValue(row,"pre_prgs_rt")    
 }

 if (colid == "tm_prgs_qty") {
