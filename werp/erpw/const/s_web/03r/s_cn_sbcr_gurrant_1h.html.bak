<SCRIPT language=JavaScript>
// <LINK rel="stylesheet" type="text/css" href="../../../comm_function/env.css">
var gs_home = top.frames['security'].gs_home.value
var is_sbcr_code 
var is_sbcr_name 
//------------------------------------------------------------------------
function btnquery_prt_onclick()                     // 미리보기
//------------------------------------------------------------------------
{ 
   var arg_temp
   arg_temp ="const_rpt/s_web/03r/"        // 보고서 디렉토리
   arg_temp = arg_temp + "s_cn_sbcr_gurrant"    // 보고서명 

   arg_temp = arg_temp + "&RptParams=" + is_sbcr_code  
   f_crystal_report(arg_temp)   //보고서 호출

}
//---------------------------------------------------------------------------
function sbcr_code_find()                       //현장 찾기 
//---------------------------------------------------------------------------
{
	var result;
	var array = new Array();
	ls_data =""
	  result = window.showModalDialog(gs_home + "/const/s_web/00/s_sbcr_search_1h.html",ls_data,"dialogWidth:800px;dialogHeight:500px");
	  if(result == "") return false
	  array = result.split("@");
	  is_sbcr_code = array[1];   // 협력업체 코드  
	  is_sbcr_name = array[3];   // 협력업체 명 
     sbcr_name.value  = is_sbcr_name
     return true
}   
//------------------------------------------------------------------------
function keyDown()                       //검색어 입력후 enter를 쳤을경우
//------------------------------------------------------------------------
{
	if (window.event.keyCode == 13) { 
		sbcr_code_find();
	} 
}
//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
	top.frames['main_title'].mouse_over()

	sbcr_name.value = ""

}
</SCRIPT>

</HEAD>
<BODY style="FONT-SIZE: 12px" onload=setTimeout('window_onload()',1) oncontextmenu="return false" >

<DIV id=print_condition 
      style="Z-INDEX: 107; LEFT: 300px; WIDTH: 300px; POSITION: absolute; TOP: 100px; HEIGHT: 80px">
	<table width="100%" height="100%"  BORDER="5"  align="center"   CELLSPACING="0" style="FONT-SIZE: 12px">
	<tr BGCOLOR="white" > 
		<td width="30%"   BGCOLOR="#BBE0F4" style="COLOR: BLACK"  > 
		<div align="right">협력업체명</div>
		</td>
		<td> 
		<INPUT style="FONT-SIZE:12px;" id=sbcr_name   onkeyDown="keyDown()">
		<input type="button_query" value=" 검색" 
		style="color:#2E4A4A; font-size:12px; CURSOR: hand; width:40px;
		background-color:#E9F5F5; border:1 #A1A9A9 solid; "   
		onmouseover="this.style.backgroundColor='#C0E8DB'; this.style.color='#EF315E'" 
		onmouseout="this.style.backgroundColor='#E9F5F5'; this.style.color='#0E4433'
		"  onclick=sbcr_code_find()>
		</td>
	</tr>                 
	</table>
</DIV>
