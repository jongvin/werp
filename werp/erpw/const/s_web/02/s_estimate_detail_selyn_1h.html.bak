<script language=JavaScript for=Button_file event=OnClick()>
	if (Button_file.value == "" || Button_file.value == null )   //파일을 선택하지않았을경우
		return
	
	var arg_cmd,i,ll_chk=0,ll_cnt=0,ll_code
	
	ds_excel.DataID = is_ds_excel_addr + is_dept_code + "&arg_seq="  +  il_seq
	ds_excel.SyncLoad=true
	ds_excel.reset()
	
	if (ds_excel.CountRow > 0)  {
		arg_cmd =  "delete from  s_estimate_temp where " + 
		        "     dept_code= '" + is_dept_code + "' and unq_num = " + il_seq 
		f_update_sql(arg_cmd)
		ds_excel.SyncLoad=true
		ds_excel.reset()
	}      
	 
	tx_temp.Src = Button_file.value           //엑셀자료를 찾아서 ds_excel에 넣어줌.
	tx_temp.SyncLoad=true
	tx_temp.reset()
	
	gd_excel1.ReDraw=true
	ds_excel.SyncLoad=true
	ds_excel.ImportData(tx_temp.Text) 
	
	gd_excel1.ReDraw=false
	gd_excel1.ReDraw=true

	for (i=ds_excel.CountRow; i>= 1; i--) {
		if (ds_excel.NameValue(i,'detail_unq_num') == '' || ds_excel.NameValue(i,'detail_unq_num') == 0) {
			ds_excel.DeleteRow(i)
		}
		else {
			ds_excel.NameValue(i,'unq_num') = il_seq
		}
	}

	if (ds_excel.IsUpdated) {
	    tr_excel.Action = is_tr_excel_addr
	    tr_excel.Post()
	}

	ds_excel.DataID = is_ds_excel_addr + is_dept_code + "&arg_seq="  +  il_seq
	ds_excel.SyncLoad=true
	ds_excel.reset()

	for (i=1 ;i<= ds_excel.CountRow;i++) {
		ll_code = ds_excel.NameValue(i,'detail_unq_num')
		ll_cnt = f_select1_q("select dept_code select_1  from s_order_detail where dept_code = '" + is_dept_code + 
		                     "' and order_number = " + il_order_number + " and detail_unq_num = " + ll_code) //발주내역 존재여부 체크
		if (ll_cnt < 1){
			ll_chk = ll_chk + 1;
		}
	}     
	if (ll_chk > 0){
		alert('엑셀자료에 있는 내역이 발주내역과 ' + ll_chk + '건이 일치하지않아 올릴 수 없습니다...')
		arg_cmd =  "delete from  s_estimate_temp where " + 
		        "     dept_code= '" + is_dept_code + "' and unq_num = " + il_seq 
		f_update_sql(arg_cmd)
		return
	}	

	ll_cnt = f_select1_q("select count(*) select_1  from s_order_detail where dept_code = '" + is_dept_code + "' and order_number = " + il_order_number ) //발주내역 존재여부 체크
   ll_cnt = ds_select1.NameValue(1,'select_1')
   if (ll_cnt != ds_excel.CountRow){
   	alert('엑셀자료에 있는 내역과 발주내역의 수가 서로 달라 작업할 수 없습니다...')
		arg_cmd =  "delete from  s_estimate_temp where " + 
		        "     dept_code= '" + is_dept_code + "' and unq_num = " + il_seq 
		f_update_sql(arg_cmd)
   	return
   }
   arg_cmd = " UPDATE s_estimate_detail  a " +
		       " SET (a.est_price,a.est_amt,a.ctrl_price,a.ctrl_amt,a.emat_price,a.emat_amt,a.cmat_price,a.cmat_amt, " +
             "      a.elab_price,a.elab_amt,a.clab_price,a.clab_amt,a.eexp_price,a.eexp_amt,a.cexp_price,a.cexp_amt ) = " +
             "     ( SELECT nvl(b.est_price,0),nvl(b.est_amt,0),nvl(b.est_price,0),nvl(b.est_amt,0),nvl(b.emat_price,0),nvl(b.emat_amt,0),nvl(b.emat_price,0),nvl(b.emat_amt,0), " +
             "              nvl(b.elab_price,0),nvl(b.elab_amt,0),nvl(b.elab_price,0),nvl(b.elab_amt,0),nvl(b.eexp_price,0),nvl(b.eexp_amt,0),nvl(b.eexp_price,0),nvl(b.eexp_amt,0) " +
				 "			  from s_estimate_temp b " +
				 "			 where a.detail_unq_num = b.detail_unq_num " +
		       " 			and b.dept_code = '" + is_dept_code + "'" +
		       " 			and b.unq_num   = " + il_seq + " ) " +
		       "  WHERE a.dept_code = '" + is_dept_code + "'" +
		       " 	AND a.order_number = " + il_order_number  +
             "    AND a.sbcr_code = '" + is_sbcr_code + "' " +
		       " 	AND EXISTS (SELECT b.est_price " +
		       " 					  from s_estimate_temp b " +
		       " 					 where a.detail_unq_num = b.detail_unq_num  " +
		       " 						and b.dept_code = '" + is_dept_code + "'"  +
		       " 						and b.unq_num   = " + il_seq + " ) " +
		       "@ delete from s_estimate_temp where dept_code = '" + is_dept_code + "' and unq_num = " + il_seq
	f_update_sql_multy(arg_cmd)

   arg_cmd = " UPDATE s_estimate_detail  a " +
		       " SET a.est_price = trunc(a.emat_price,0) + trunc(a.elab_price,0) + trunc(a.eexp_price,0), " +
		       "     a.ctrl_price = trunc(a.emat_price,0) + trunc(a.elab_price,0) + trunc(a.eexp_price,0), " +
		       "     a.emat_amt = trunc(a.est_qty  * a.emat_price,0), " +
		       "     a.elab_amt = trunc(a.est_qty  * a.elab_price,0), " +
		       "     a.eexp_amt = trunc(a.est_qty  * a.eexp_price,0), " +
		       "     a.cmat_amt = trunc(a.ctrl_qty * a.cmat_price,0), " +
		       "     a.clab_amt = trunc(a.ctrl_qty * a.clab_price,0), " +
		       "     a.cexp_amt = trunc(a.ctrl_qty * a.cexp_price,0) " +
		       "  WHERE a.dept_code = '" + is_dept_code + "'" +
		       " 	AND a.order_number = " + il_order_number  +
             "    AND a.sbcr_code = '" + is_sbcr_code + "' " 
	f_update_sql(arg_cmd)

   arg_cmd = " UPDATE s_estimate_detail  a " +
		       " SET a.est_amt = nvl(a.emat_amt,0) + nvl(a.elab_amt,0) + nvl(a.eexp_amt,0), " +
		       "     a.ctrl_amt = nvl(a.cmat_amt,0) + nvl(a.clab_amt,0) + nvl(a.cexp_amt,0) " +
		       "  WHERE a.dept_code = '" + is_dept_code + "'" +
		       " 	AND a.order_number = " + il_order_number  +
             "    AND a.sbcr_code = '" + is_sbcr_code + "' " 
	f_update_sql(arg_cmd)

	sp_tot_call()
	tree_btnquery_onclick()
	tree_1_OnSelChanged()
	alert('정상 처리되었습니다')
</SCRIPT>

