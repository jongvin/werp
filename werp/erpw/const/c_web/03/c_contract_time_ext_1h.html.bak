<SCRIPT language=JavaScript event=OnRowPosChanged(row) for=ds_0>
  if  (row < 1) return;
  btnquery_onclick()  

</SCRIPT>

<SCRIPT language=JavaScript event=OnRowPosChanged(row) for=ds_1>
	if  (row < 1) return;
	
	ll_cnt = f_ds_select_q("ds_select_1", "select collection_time select_1 from r_contract_time_collection" +
	                                      " where dept_code='" + ds_1.NameValue(row, "dept_code") + "' and " +
	                                      "       cont_no  ='" + ds_1.NameValue(row, "cont_no") + "' and " +
	                                      "       chg_degree='" + ds_1.NameValue(row, "chg_degree") + "' and " +
	                                      "       extablish_time='" + ds_1.NameValue(row, "extablish_time") + "' " )

	if (ll_cnt == 0)
		gd_1.ColumnProp("extablish_time", "Edit") = ''
	else
		gd_1.ColumnProp("extablish_time", "Edit") = 'none'	
		                                      
	
	if (ds_1.NameValue(row, "extablish_tag") == 1 || ds_1.NameValue(row, "extablish_tag") == 4 ) 
		gd_1.ColumnProp("reservation_amt", "Edit") = 'none'
	else
		gd_1.ColumnProp("reservation_amt", "Edit") = ''
		
	div_btnquery_onclick()   
</SCRIPT>
<SCRIPT language=JavaScript for=ds_1 event=OnColumnChanged(row,colid)>
	if (row < 1 ) return
	
	if (colid == "extablish_tag") {
		if(ds_1.NameValue(row, "extablish_tag") == 1 || ds_1.NameValue(row, "extablish_tag") == 4 ) 
			gd_1.ColumnProp("reservation_amt", "Edit") = 'none'
		else
			gd_1.ColumnProp("reservation_amt", "Edit") = ''
	}
	
	if (colid == "supply_amt" || colid == "vat_amt") {
		ds_1.NameValue(row, "sum_amt") = ds_1.NameValue(row, "supply_amt") + ds_1.NameValue(row, "vat_amt")
	}
</SCRIPT>		
<SCRIPT language=JavaScript for=ds_2 event=OnColumnChanged(row,colid)>
	if (row < 1 ) return
	
	if (colid == "cash_amt" || colid == "bill_amt") {
		ds_2.NameValue(row, "supply_amt") = ds_2.NameValue(row, "cash_amt") + ds_2.NameValue(row, "bill_amt")
	}
	
	if (colid == "supply_amt" || colid == "vat_amt") {
		ds_2.NameValue(row, "sum_amt") = ds_2.NameValue(row, "supply_amt") + ds_2.NameValue(row, "vat_amt")
	}
</SCRIPT>

