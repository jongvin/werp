<SCRIPT language=JavaScript>
//------------------------------------------------------------------------
function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{
	if (em_start_date.text == "" || em_end_date.text == "") {
		alert("접수일 기간을 입력해주세요")
		em_start_date.focus()
		return
	}
	if (em_start_date.text == "        " || em_end_date.text == "        ") {
		alert("접수일 기간을 입력해주세요")
		em_start_date.focus()
		return
	}
	if (em_start_date.text.length != 8 || em_end_date.text.length != 8 ) {
		alert("접수일이 올바르지 않습니다")
		return
	}
	if (em_start_date.text > em_end_date.text ) {
		alert("접수일 기간이 올바르지 않습니다")
		em_end_date.focus()
		return
	}
	ls_date = em_start_date.text
	ls_date = ls_date + "&arg_end_date=" + em_end_date.text
	
	if (radio_dept[0].checked == true)
		ls_dept = ""
	if (radio_dept[1].checked == true)
		ls_dept = is_dept_code
	
	ds_1.DataID = is_ds_1_addr + ls_date + "&arg_dept_code=" + ls_dept
	ds_1.SyncLoad=true
	ds_1.Reset()
	
	gd_1.ColumnProp('long_name','SubSumText') = ""
	gd_1.ColumnProp('code_name','SubSumText') = "합계"	

    gd_1.ColumnProp("long_name","SubBgColor") = 'silver'
    gd_1.ColumnProp("code_name","SubBgColor") = 'silver'
    gd_1.ColumnProp("count_1","SubBgColor") = 'silver'
