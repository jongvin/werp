//--------------------------------------------------------------------
function Button_confirm_onclick()               //확인
//--------------------------------------------------------------------
{
	var ls_return = ""
	
	if (em_dd.text.length != 2) {
		alert("날짜가 정확하지 않습니다")
		em_dd.focus()
		return
	}
	if (!f_check_date(em_yyyy.text+em_mm.text+em_dd.text)) {
		alert("날짜가 정확하지 않습니다")
		em_dd.focus()
		return
	}
	ls_last_date = ls_last.substr(8,2)
	if (parseInt(ls_last_date) >= em_dd.text) {
		alert("입력일자가 최종 일자보다 큰 값이어야 합니다")
		return
	}
	
	ls_return = em_yyyy.text + "." + em_mm.text + "." + em_dd.text
	window.returnValue = ls_return
 	window.close();
}

//----------------------------------------------------------------
function window_onload()                  // window open event 
//-----------------------------------------------------------------
{
	var result;
	var array = new Array();
	result = window.dialogArguments;
	
	if(result == "")
		return
	
	array = result.split("@");
	gs_home = array[0]
	gs_temp = array[1]
	ls_last = array[2]
	
	ls_temp = gs_temp  // 시스템 년월 선언.
	
	em_yyyy.text = ls_temp.substr(0,4)
	em_mm.text = ls_temp.substr(5,2)
	em_dd.text = ls_temp.substr(8,2)
	em_dd.focus()
}	
</SCRIPT>
