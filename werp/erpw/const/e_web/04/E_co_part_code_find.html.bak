	<PARAM NAME="ColSizing" VALUE="-1">
	<PARAM NAME="DataID" VALUE="ds_1">
	<PARAM NAME="Format" VALUE="
	   <C> Name='구분' ID=part_name Suppress=1 HeadAlign=Center HeadBgColor=#BBE0F4  HeadColor=BLACK Align='center'  Width=100 </C>
	   <C> Name='항목' ID=item_name HeadAlign=Center HeadBgColor=#BBE0F4  HeadColor=BLACK  Width=150 </C>	   
	   ">
 	</OBJECT>
</BODY></HTML>
