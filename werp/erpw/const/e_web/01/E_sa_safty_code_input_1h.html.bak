<SCRIPT language=JavaScript>
//------------------------------------------------------------------------
function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{

	if ( il_nindex == 1 )
    { 
    	class_tag = '32,33,34,35' 
    	ds_1.DataID = is_ds_1_addr + class_tag 
		ds_1.SyncLoad=true
		ds_1.Reset()
    }
	else if ( il_nindex == 2 )
	{  
		class_tag = '77,79'  
		ds_5.DataID = is_ds_1_addr + class_tag 
		ds_5.SyncLoad=true
		ds_5.Reset()	
	}	
	
	div_btnquery_onclick() 

}
//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
	if ( il_nindex == 1 ) 
	{
 		if (ds_1.CountRow < 1 ) return
	
		ds_2.AddRow()   
	
		div_init_move1()
    }
    
	if ( il_nindex == 2 ) 
	{
	 if ( il_screen  == 3 ) { 
 		if (ds_1.CountRow < 1 ) return
			if ( f_code_chk() == false ) return
  			ds_3.AddRow() 
		div_init_move2() }
	 if ( il_screen  == 4 ) { 
 		if (ds_3.CountRow < 1 ) return
			if ( f_code_chk() == false ) return
	   	ds_4.AddRow() 
		div_init_move3() }
	}   


}
//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
	if ( il_nindex == 1 ) 
	{
		if (ds_1.CountRow < 1 ) return
 			if (ds_3.CountRow == 0)				 // 한건도 없을경우는  0 
			{   
				btnadd_onclick()   
				return
			}  
			ds_2.InsertRow(ds_2.RowPosition)   
		    div_init_move1()
    }
    
	if ( il_nindex == 2 ) 
	{
	 if ( il_screen  == 3 ) { 
 		if (ds_1.CountRow < 1 )  return
			if (ds_3.CountRow == 0)				 // 한건도 없을경우는  0 
			{  
				if ( f_code_chk() == false ) return
				btnadd_onclick()   
				return
			}  
			ds_3.InsertRow(ds_3.RowPosition)   
		  div_init_move2()
		}
	 if ( il_screen  == 4 ) { 
 		if (ds_3.CountRow < 1 ) return
			if (ds_4.CountRow == 0)				 // 한건도 없을경우는  0 
			{
				if ( f_code_chk() == false ) return
				btnadd_onclick()   
				return
			}  
			ds_4.InsertRow(ds_4.RowPosition)   
		  div_init_move3()
		}
	}  
}
//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
 	if ( il_nindex == 1 ) 
	{
	  if (ds_2.CountRow < 1 ) return
		if  (gd_2.MultiRowSelect)
		  {                                   // multi row select 일 경우
		     for (i=ds_2.CountRow ;i >0 ;i--)                          // 로우의 제일끝에서부터 앞으로 루핑
		     {
		        if (ds_2.RowMark(i) == 1)                              // 선택되어 있으며는 
		               ds_2.DeleteRow(i)
		     }   
		     ds_2.MarkRows(ds_2.RowPosition,ds_2.RowPosition)           // 삭제작업완료후 MARK(멀티로우 때문에 한번 마킹해준다)
		  }
		  else
		 	ds_2.DeleteRow(ds_2.RowPosition)  
    }
    
	if ( il_nindex == 2 ) 
	{
		if ( il_screen  == 3 ){ 
	     if ( ds_4.CountRow > 0 ) 
	     {
	        alert("하위 안전코드가 남아있습니다");
	        return ;
	     }
	     
		  if (ds_3.CountRow < 1 ) return
			if  (gd_3.MultiRowSelect)
			  {                                   // multi row select 일 경우
			     for (i=ds_3.CountRow ;i >0 ;i--)                          // 로우의 제일끝에서부터 앞으로 루핑
			     {
			        if (ds_3.RowMark(i) == 1)                              // 선택되어 있으며는 
			               ds_3.DeleteRow(i)
			    }   
			    ds_3.MarkRows(ds_3.RowPosition,ds_3.RowPosition)           // 삭제작업완료후 MARK(멀티로우 때문에 한번 마킹해준다)
			  }
			  else
			 	ds_3.DeleteRow(ds_3.RowPosition)  
		}	 	

		if ( il_screen  == 4 ) 
		{ 
		   if (ds_4.CountRow < 1 ) return
			if  (gd_4.MultiRowSelect)
			  {                                   // multi row select 일 경우
			     for (i=ds_4.CountRow ;i >0 ;i--)                          // 로우의 제일끝에서부터 앞으로 루핑
			     {
			        if (ds_4.RowMark(i) == 1)                              // 선택되어 있으며는 
			            ds_4.DeleteRow(i)
			    }   
			    ds_4.MarkRows(ds_4.RowPosition,ds_4.RowPosition)           // 삭제작업완료후 MARK(멀티로우 때문에 한번 마킹해준다)
			  }
			  else
			 	ds_4.DeleteRow(ds_4.RowPosition)  
		 }  
	}	 	
 }
//------------------------------------------------------------------------
 function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
	if ( il_screen  == 2 ){ ds_2.Undo(ds_2.RowPosition) }                                  //해당 row만 취소한다.
	if ( il_screen  == 3 ){ ds_3.Undo(ds_3.RowPosition) }                                  //해당 row만 취소한다.
	if ( il_screen  == 4 ){ ds_4.Undo(ds_4.RowPosition) }                                  //해당 row만 취소한다.
}
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
var update_chk = 0

  if ( ds_2.IsUpdated )	 	//변경된것이 있으며는 
  { 
     ll_dup = f_dup_chk("ds_2","safety_code")
	 if (ll_dup > 0) {
	 	 alert("잘못된 번호가 입력되었습니다.:  수정하십시요 ")
	    ds_2.RowPosition = ll_dup
	    gd_2.SetColumn("safety_code");
	    return false
      }
                                    
		update_chk = 1 ; 
  } 
  
  if ( ds_3.IsUpdated )	 	//변경된것이 있으며는 
  { 
      ll_dup = f_dup_chk("ds_3","safety_code")
	  if (ll_dup > 0) {
	 	 alert("잘못된 번호가 입력되었습니다.:  수정하십시요 ")
	     ds_3.RowPosition = ll_dup
	     gd_3.SetColumn("safety_code");
	     return false
	   }     
   
		update_chk = 1 ; 
  } 
  
  if ( ds_4.IsUpdated )	 	//변경된것이 있으며는 
  {  
       ll_dup = f_dup_chk("ds_4","tmp_code")
	   if (ll_dup > 0) {
	      alert("잘못된 번호가 입력되었습니다.:  수정하십시요 ")
	      ds_4.RowPosition = ll_dup
	      gd_4.SetColumn("tmp_code");
	   	  return false
	   }	                                          
  
		update_chk = 1 ; 
  } 

	if ( 	update_chk == 1  )
   {
		if  (confirm("변경되었습니다 저장하시겠습니까?"))
		{
		  tr_1.Post()           // 저장(window_onload()에서 tr_1.Action 이 연결되어있어야한다.) 
		  tr_2.Post()           // 저장(window_onload()에서 tr_1.Action 이 연결되어있어야한다.) 
		  
			for ( i=0 ; i <= ds_4.CountRow; i++ )
			{ ds_4.NameValue(ds_4.RowPosition,"safety_code") =  ds_3.NameValue(ds_3.RowPosition,"safety_code")  +  ds_4.NameValue(ds_4.RowPosition,"tmp_code") } 

		  
		  tr_3.Post()           // 저장(window_onload()에서 tr_3.Action 이 연결되어있어야한다.)    
		}
		else
		{ 
			div_btnquery_onclick()  
		}
	}
}

//------------------------------------------------------------------------
function div_btnquery_onclick()                    
//------------------------------------------------------------------------
{

 if (ds_1.CountRow < 1) return
 
 if ( il_nindex == 1 )
 {
	 ds_2.DataID = is_ds_2_addr + ds_1.NameValue(ds_1.RowPosition,"class_tag") 
	 ds_2.SyncLoad=true
	 ds_2.Reset()
 }
 
 if ( il_nindex == 2 )
 {
	 ds_3.DataID = is_ds_3_addr + ds_5.NameValue(ds_5.RowPosition,"class_tag") 
	 ds_3.SyncLoad=true
	 ds_3.Reset()
	 
	 if (ds_3.CountRow < 1) return
	 ds_4.DataID = is_ds_4_addr + ( parseInt(ds_5.NameValue(ds_1.RowPosition,"class_num")) + 1 ) + "&arg_code=" + ds_3.NameValue(ds_3.RowPosition,"safety_code") 
	 ds_4.SyncLoad=true
	 ds_4.Reset()
 }
}

