	top.frames['main_ct'].location.href(gs_home + "/const/e_web/01/E_sa_amt_monthly_1h.html" + "?" + "u")  
}
//------------------------------------------------------------------------
function date_return(value)                     
//------------------------------------------------------------------------
{
	var result = gn_Cal_Yyyymmdd(); 
	if(!is_null(result))
	{ 
		tmp = eval ( value + '.text =  result.substr(0,8) ' );
	} 
}
//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
	if (f_reject_window()) return 
 	top.frames['main_title'].mouse_over()
 		
	em_ipdate.text = is_work_date.substr(0,4) + is_work_date.substr(5,2)
	tr_1.Action =  is_tr_1_addr
	tr_2.Action =  is_tr_2_addr
		
