<SCRIPT language=JavaScript>
//------------------------------------------------------------------------
function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{
   if  (is_dept_code == "") {
     alert("현장을 선택하십시요(검색을 누르십시요)")
   return
  }     

	ds_1.DataID = is_ds_1_addr + is_dept_code
	ds_1.SyncLoad=true
	ds_1.Reset()
	if (ds_1.CountRow < 1){       
	    ds_2.ClearData() 
     	ds_3.ClearData()
	}
	if (ds_2.CountRow < 1){
        ds_3.ClearData() 
    }
    	   
 	 div_btnquery_onclick()  
	   
}

//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
    if (ds_1.RowPosition > 1 ) { alert("최종 기성차수가 아닙니다"); return }
 	if (ds_2.CountRow < 1 ) return
	
 	ds_3.AddRow()   
	
	div_init_move()
}
//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
    if (ds_1.RowPosition > 1 ) { alert("최종 기성차수가 아닙니다"); return }
	if (ds_2.CountRow < 1 ) return
	if (ds_3.CountRow == 0)				 // 한건도 없을경우는  0 
	{                                       
		btnadd_onclick()   
		return
	}  
	ds_3.InsertRow(ds_3.RowPosition)   


	div_init_move() 
}
//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
  if (ds_1.RowPosition > 1 ) { alert("최종 기성차수가 아닙니다"); return }
  if (ds_2.CountRow < 1 ) return

  if  (gd_3.MultiRowSelect)
  {                                   // multi row select 일 경우
     for (i=ds_3.CountRow ;i >0 ;i--)                          // 로우의 제일끝에서부터 앞으로 루핑
     {
        if (ds_3.RowMark(i) == 1)                              // 선택되어 있으며는 
               ds_3.DeleteRow(i)
    }   
    ds_3.MarkRows(ds_3.RowPosition,ds_3.RowPosition)           // 삭제작업완료후 MARK(멀티로우 때문에 한번 마킹해준다)
  }
  else
 	ds_3.DeleteRow(ds_3.RowPosition)  
 }
//------------------------------------------------------------------------
 function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
  ds_3.Undo(ds_3.RowPosition)                                     //해당 row만 취소한다.
}
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
    
  if (ds_3.IsUpdated)											 //변경된것이 있으며는 
	
  {                                          
    if  (confirm("변경되었습니다 저장하시겠습니까?"))
    {
		for ( i = 0 ; i < ds_3.CountRow ; i++ )
		{
			if ( ds_3.NameValue(i,"usememo") == '' )
			{
				alert ("사용내역이 빠져있습니다.") ;
				ds_3.RowPosition = i ;
				return false ; 
			
			}
		
		}
        tr_1.Post()           // 저장(window_onload()에서 tr_1.Action 이 연결되어있어야한다.) 
    }    
 } 

}

//------------------------------------------------------------------------
function div_btnquery_onclick()                     // 하위 조회  
//------------------------------------------------------------------------
{
  
 if (ds_1.CountRow < 1) return

 ds_2.DataID = is_ds_2_addr +  is_dept_code + "&arg_yymmdd=" + ds_1.NameValue(ds_1.RowPosition,"yymm") + 
                                            "&arg_seq=" + ds_1.NameValue(ds_1.RowPosition,"seq")
 ds_2.SyncLoad=true
 ds_2.Reset()

 if (ds_2.CountRow < 1) return
 ds_3.DataID = is_ds_3_addr + is_dept_code + "&arg_order_code=" + ds_2.NameValue(ds_2.RowPosition,"order_number") + "&arg_yymm=" + ds_1.NameValue(ds_1.RowPosition,"yymm")
 ds_3.SyncLoad=true
 ds_3.Reset()
 
}

//----------------------------------------------------------------------------
function btnexit_onclick(ls_arg)                 // 종료 
//-----------------------------------------------------------------------------
{
	 if (ds_1.IsUpdated) {
     if  (confirm("변경되었습니다 저장하시겠습니까?")) 
        {  //저장 routine
        }   
 
   }     
top.frames['main_ct'].location.href(ls_arg)
top.frames['main_title'].div_r.style.visibility="hidden"             
top.frames['main_title'].div_u.style.visibility="hidden"             

top.frames['main_ct'].window.close()     
}

//----------------------------------------------------------------------
function div_init_move()                         // initial move
//----------------------------------------------------------------------
{
  ds_3.NameValue(ds_3.RowPosition,"item_code") = '01'
  ds_3.NameValue(ds_3.RowPosition,"dept_code") = is_dept_code
  ds_3.NameValue(ds_3.RowPosition,"yymm") = ds_2.NameValue(ds_2.RowPosition,"yymmdd")
  ds_3.NameValue(ds_3.RowPosition,"seq") = ds_1.NameValue(ds_1.RowPosition,"seq")
  ds_3.NameValue(ds_3.RowPosition,"order_number") = ds_2.NameValue(ds_2.RowPosition,"order_number")

}


//---------------------------------------------------------------------------
function dept_code_find()                       //현장 찾기 
//---------------------------------------------------------------------------
{
	result = f_dept_code_find(dept_name.value)
	
	if (result==false)return
	
	is_dept_code = top.frames['security'].gs_dept_code.value
	is_dept_name = top.frames['security'].gs_dept_name.value

	dept_name.value = is_dept_name

    il_screen = 1   //parent를 다시 읽음 
	btnquery_onclick()
}   
//------------------------------------------------------------------------
function keyDown()                       //검색어 입력후 enter를 쳤을경우
//------------------------------------------------------------------------
{
	if (window.event.keyCode == 13) { 
		dept_code_find();
	} 
}



//------------------------------------------------------------------------
function get_focus(focus) // gd가 포커스를 받을때 동작.
//------------------------------------------------------------------------
{
	il_screen = focus
	if (is_sec == 'r') return   // 조회권한일경우 
	
	switch ( il_screen )
	{
	case 1:
		top.frames['main_title'].div_r.style.visibility="visible"
		top.frames['main_title'].div_u.style.visibility="hidden"     
	    break;
	case 2:
	 	top.frames['main_title'].div_r.style.visibility="visible"
		top.frames['main_title'].div_u.style.visibility="hidden"  
	    break;
	case 3:
	 	top.frames['main_title'].div_r.style.visibility="hidden"
		top.frames['main_title'].div_u.style.visibility="visible"  
	    break;
	}

}

//---------------------------------------------------------------------------
function f_set_resize()
//---------------------------------------------------------------------------
{
	var objArr=new Array('gd_1', 'gd_2','gd_3');
	var xArr =new Array(false, false, true);
	var yArr =new Array(true,true, true);

	f_reSize(objArr, xArr, yArr);
}

//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 

  f_set_resize()

    top.frames['main_title'].mouse_over()

	ll_cnt = f_select1_q("select safety_code select_1, child_name select_2  from e_safety_code_child where class_tag = '033' order by safety_code") //공사구분

	is_dept_code = top.frames['security'].gs_dept_code.value
	is_dept_name = top.frames['security'].gs_dept_name.value
   	ds_seq.DataID = is_ds_seq_addr                       // SEQ NO 의 DATAID
	
	dept_name.value = is_dept_name

	tr_1.Action = is_tr_1_addr
	
	is_sec = gf_initial_security()
	is_temp = is_sec
   if (is_sec != 'r') {
     is_temp = 'u'
   }	
	if (is_sec == 'r') {
	   button_1.style.visibility='hidden'
	   button_3.style.visibility='hidden'
	   button_4.style.visibility='hidden'
	   button_5.style.visibility='hidden'
	   button_6.style.visibility='hidden'
	   button_7.style.visibility='hidden'
	}

	btnquery_onclick()

}


