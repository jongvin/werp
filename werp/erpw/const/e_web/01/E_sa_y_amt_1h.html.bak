{
}

//---------------------------------------------------------------------
function move_search()
//---------------------------------------------------------------------
{
 if (ds_2.CountRow < 1) 
 { ds_3.ClearData() ; }
 else 
 {
 	ds_3.DataID = is_ds_3_addr + is_dept_code + "&arg_order_code=" + ds_2.NameValue(ds_2.RowPosition,"order_number") + "&arg_yymm=" + ds_1.NameValue(ds_1.RowPosition,"yymm")
 	ds_3.SyncLoad=true
 	ds_3.Reset()
 }
} 
