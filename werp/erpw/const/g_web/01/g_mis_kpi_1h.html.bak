var gs_home = top.frames['security'].gs_home.value
var is_work_date = top.frames['security'].gs_sysdate.value
var is_ds_1_addr = gs_home + '/const/g_web/01/g_mis_kpi_1q.jsp?arg_year='
var is_tr_1_addr = gs_home + '/const/g_web/01/g_mis_kpi_1tr.jsp'
var is_ds_seq_addr = gs_home + '/const/g_web/99/g_spec_unq_no_1q.jsp'  

var is_year
//------------------------------------------------------------------------
function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{	
	ds_1.DataID = is_ds_1_addr + is_year
	ds_1.SyncLoad=true
	ds_1.Reset()

}
//-----------------------------------------------------------------------
function init_move()                       // 초기값 셋팅
//------------------------------------------------------------------------
{
	ds_1.NameValue(ds_1.RowPosition,"work_year")  = is_year
	ds_1.NameValue(ds_1.RowPosition,"calc_yn")  		 = "T"
	
	ds_seq.SyncLoad=true;              // 유니크키 구하기
	ds_seq.reset()
	
	ds_1.NameValue(ds_1.RowPosition,"unq_key") = ds_seq.NameValue(1,"nextval")
	
}
//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
	 ds_1.AddRow()
	 init_move()
}
//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
	 ds_1.InsertRow(ds_1.RowPosition)
	 init_move()
}
//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
	if  (gd_1.MultiRowSelect){               // multi row select 일 경우
		for (i=ds_1.CountRow ;i >0 ;i--) {
			if (ds_1.RowMark(i) == 1)
				ds_1.DeleteRow(i)
		}
		ds_1.MarkRows(ds_1.RowPosition,ds_1.RowPosition)  //삭제작업완료후 MARK
	} else
		ds_1.DeleteRow(ds_1.RowPosition)
}
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
  if (ds_1.IsUpdated){
     if  (confirm("변경되었습니다 저장하시겠습니까?")){
     		for ( i=1; i<=ds_1.CountRow; i++){
			  ds_1.NameValue(i, "no_seq") = i
			} 	
			
	      tr_1.Post()
	   }
 	} 
}
//----------------------------------------------------------------------------
function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
	btnquery_onclick()
}
//----------------------------------------------------------------------------
function btnexit_onclick(ls_arg)                 // 종료 
//-----------------------------------------------------------------------------

{
	if (ds_1.IsUpdated) {
	   if  (confirm("변경되었습니다 저장하시겠습니까?")) 
	       {  //저장 routine
	       }   
 
	}     
	top.frames['main_ct'].location.href(ls_arg)
	top.frames['main_title'].div_r.style.visibility="hidden"             
	top.frames['main_title'].div_u.style.visibility="hidden"             
}
//-------------------------------------------------------------------------------
function select_year()                  //년월 선택 
//--------------------------------------------------------------------------------
{
   is_year = em_select_year.text + '.01.01'
   ds_1.ClearData()

   btnquery_onclick()
}   
//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
	top.frames['main_title'].mouse_over()
  	em_select_year.text = is_work_date.substr(0,4)
	select_year()
	ds_seq.DataID = is_ds_seq_addr

	tr_1.Action = is_tr_1_addr
}
