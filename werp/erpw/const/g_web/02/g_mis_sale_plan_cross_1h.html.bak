function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{	

	ds_1.DataID = is_ds_1_addr + is_work_year
	ds_1.SyncLoad=true
	ds_1.Reset()
	
	ds_1_recal()	// 동일 Key Breake
   ds_1.SortExpr = "+proj_bus_type+spec_no_seq+work_yymm"
   ds_1.Sort();
	rds_1.DataID = 'ds_1'
   rds_1.SyncLoad = true
   rds_1.Reset()
   rds_1.DataID = ''
}
//----------------------------------------------------------------------------
function ds_1_recal()               // 동일 자체에 년월일경우 순번 자동구하기.
//-----------------------------------------------------------------------------
{
   if (ds_1.CountRow <= 1) return
	var i=1,j
	var sa_proj_bus_type
	var sa_work_yymm
	sa_aft_proj_unq_key=''
	while (sa_aft_proj_unq_key != 'last') {
	   // 그룹 전처리 
	   sa_proj_bus_type= ds_1.NameValue(i,'proj_bus_type')
	   sa_work_yymm= ds_1.NameValue(i,'work_yymm')
      j=0
	   alert(sa_proj_bus_type + ' ' + sa_work_yymm)
	   // 같은 그룹끼리 보여주기 위해 spec_no_seq를 위해 재부여함..
	   // 변수와 비교대상이 완전히 같을때만 loop.. 결국 j를 +1씩 증가시킴, 비교대상 값이 다르면 다시 j=1이 되게함...
	   while(sa_aft_proj_unq_key != 'last' && sa_proj_bus_type == ds_1.NameValue(i,'proj_bus_type')  
	                                       && sa_work_yymm == ds_1.NameValue(i,'work_yymm') ) {
         j = j + 1
	      ds_1.NameValue(i,'spec_no_seq') = j
	      alert('type=' + ds_1.NameValue(i,'proj_bus_type') + ' j=' + j + ' yymm=' + ds_1.NameValue(i,'work_yymm'))
	      i++
	      
	      if (i > ds_1.CountRow)  {
	           sa_aft_proj_unq_key = 'last'
	      }
	   }
	   //그룹 후처리 
	}
}
//----------------------------------------------------------------------------
