			}
			
	   	tr_1.Post()
	   }	   
 	} 
	setTimeout("ds_graph_rtn()",5)
}
//-----------------------------------------------------------------------
function ds_1_sum()                       // 소계및 인력추정 계산 로직  
//------------------------------------------------------------------------
{
  if (ds_1.CountRow <= 1) return

  var i=1,j
  var p_amt = new Array();
  var p_tot_amt = new Array();
  for (j=1;j<=48;j++) {
      p_amt[j] = 0              // 소계
      p_tot_amt[j] = 0          //인력 누계 
  }
  var il_iwon=-1
  var sa_proj_prog_type
  sa_aft_proj_unq_key=''
  while (sa_aft_proj_unq_key != 'last') {
      for (j=1;j<=48;j++) {
          p_amt[j] = 0
      }
      il_iwon++
      sa_proj_prog_type = ds_1.NameValue(i,'proj_prog_type')
      while(sa_aft_proj_unq_key != 'last' && sa_proj_prog_type == ds_1.NameValue(i,'proj_prog_type')) {
         for (j=1;j<=48;j++) {
             p_amt[j] = p_amt[j] + ds_1.NameValue(i,"p_" + j)
             p_tot_amt[j] = p_tot_amt[j] + ds_1.NameValue(i,"p_" + j)
         }      
         i++
         if (i > ds_1.CountRow)  {
              sa_aft_proj_unq_key = 'last'
         }     
      }
         // 소계삽입
         ds_1.InsertRow(i)
         init_move()
 	      ds_1.NameValue(ds_1.RowPosition,'proj_prog_type') = '7'
	      ds_1.NameValue(ds_1.RowPosition,'proj_name') = '소 계'
         for (j=1;j<=48;j++) {
             ds_1.NameValue(ds_1.RowPosition,"p_" + j) = p_amt[j] 
         }
         i = i + 1     
         if (sa_proj_prog_type == '1') {
             continue   //진행은 소계만 처리  
         }
         // 인력추정 누계  
         ds_1.InsertRow(i)
         init_move()
 	      ds_1.NameValue(ds_1.RowPosition,'proj_prog_type') = '8'
	      ds_1.NameValue(ds_1.RowPosition,'proj_name') = '인력 추정 ' + il_iwon
         for (j=1;j<=48;j++) {
             ds_1.NameValue(ds_1.RowPosition,"p_" + j) = p_tot_amt[j] 
         }      
         i = i + 1
  }
 
}

//----------------------------------------------------------------------------
function ds_graph_rtn()                     // 그래프 그리기 
//---------------------------------------------------------------------------- 
{
   if (ds_1.CountRow < 1) return
   
   var array_i=0
   var p_inwon_row = new Array();
   
   for (i=1;i<=ds_1.CountRow;i++) {
       if (ds_1.NameValue(i,'proj_prog_type') == '8') {
          array_i = array_i + 1
          p_inwon_row[array_i] = i
       }
   }
   
   array_cnt = p_inwon_row.length   
	ds_graph.ClearData()
	gd_graph.DataID=''
	ls_year = em_select_year.text
	ls_year = ls_year.substr(0,4)
	ls_year = parseInt(ls_year,10)
	for (i=1;i<=48;i++) {
	    ds_graph.AddRow()
	    if (i <= 12) {
	       ds_graph.NameValue(ds_graph.RowPosition,'work_yymm') = ls_year + '.' + i  
	   }
	   if (i == 13) {
	        ls_year = parseInt(ls_year,10)
	        ls_year = ls_year + 1
	   }
	   if (i >= 13 && i <= 24) {
	   	 ds_graph.NameValue(ds_graph.RowPosition,'work_yymm') = ls_year + '.' + (i - 12) 
	   }
	   if (i == 25) {
	        ls_year = parseInt(ls_year,10)
	        ls_year = ls_year + 1
	   }
	   if (i >=25 && i <= 36) {
	   	 ds_graph.NameValue(ds_graph.RowPosition,'work_yymm') = ls_year + '.' + (i - 24)   
	   }
	   if (i == 37) {
	        ls_year = parseInt(ls_year,10)
	        ls_year = ls_year + 1
	   }
	   if (i >= 37 && i <= 48) {
	   	 ds_graph.NameValue(ds_graph.RowPosition,'work_yymm') = ls_year + '.' + (i - 36)   
	   }
	   if (array_cnt >= 1)
	      ds_graph.NameValue(ds_graph.RowPosition,'p_1') = ds_1.NameValue(p_inwon_row[1],'p_' + i)
	   if (array_cnt >= 2)
	      ds_graph.NameValue(ds_graph.RowPosition,'p_2') = ds_1.NameValue(p_inwon_row[2],'p_' + i)
	   if (array_cnt >= 3)
	      ds_graph.NameValue(ds_graph.RowPosition,'p_3') = ds_1.NameValue(p_inwon_row[3],'p_' + i)
	}

	gd_graph.Dataid='ds_graph'
   gd_graph.Reset()
	gd_graph.Dataid=''

   
}
