    onclick=btnquery_onclick() type=button value='조회'>      
<INPUT id=Button_confrim 
    style="Z-INDEX: 106; LEFT: 265px; POSITION: absolute; TOP: 7px" 
    onclick=Button_confirm_onclick() type=button value='확인'>     
<INPUT id=Button_close 
    style="Z-INDEX: 106; LEFT: 310px; POSITION: absolute; TOP: 7px" 
    onclick=Button_close_onclick() type=button value='닫 기'>    
<OBJECT id=gd_1 
    style="Z-INDEX: 105; LEFT: 10px; WIDTH: 380px; POSITION: absolute; TOP: 35px; HEIGHT: 380px" 
    classid=clsid:1F57AEAD-DB12-11D2-A4F9-00608CEBEE49 >
	<PARAM NAME="AutoInsert" VALUE="0">
	<PARAM NAME="AllShowEdit" VALUE="0">
	 <param name="MultiRowSelect"   value="0">
   <PARAM NAME="BorderStyle"  VALUE=4>
	<PARAM NAME="Editable" VALUE="0">
	<PARAM NAME="ColSizing" VALUE="-1">
	<PARAM NAME="DataID" VALUE="ds_1">
	<PARAM NAME="Format" VALUE="
	   <C> Name='주민번호' ID=civil_register_number Mask='XXXXXX-XXXXXXX' HeadAlign=Center HeadBgColor=#BBE0F4  HeadColor=BLACK   Width=90 </C>
	   <C> Name='성    명' ID=labor_name HeadAlign=Center HeadBgColor=#BBE0F4  HeadColor=BLACK   Width=80 </C>
	   <C> Name='직 종 명' ID=job_name HeadAlign=Center HeadBgColor=#BBE0F4  HeadColor=BLACK   Width=100 </C>
