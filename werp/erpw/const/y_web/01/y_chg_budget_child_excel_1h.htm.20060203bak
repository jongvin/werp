//------------------------------------------------------------------------------
function gd_1_bt_paste()               //  실행 내역 붙여넣기
//------------------------------------------------------------------------------
{
gd_1.ReDraw=false
gd_1.DataID = 'ds_1'  
 sa_position = ds_1.RowPosition

 if ((ds_1.RowPosition == ds_1.CountRow) || (ds_1.CountRow == 0))
	      copy_add()
     else
         copy_insert()
gd_1.ReDraw=true
 
 ds_1.RowPosition = sa_position      
