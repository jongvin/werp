//-----------------------------------------------------------------
function excel_copy_add()                 //엑셀자료 올리기시 추가될때 로직
//-----------------------------------------------------------------
{
 i_excel = 0                             // 인스턴스 variable를 만듬으로 copy_move function에서 사용할수있슴.
   for (i_excel=1; i_excel <= ds_excel.CountRow ;i_excel++) {
        if (ds_excel.RowMark(i_excel) == 1){
            ds_1.AddRow()
            if  (ds_1.CountRow == 1)
                il_no_seq = 0
            else  
                il_no_seq = ds_1.NameValue(ds_1.RowPosition - 1,"no_seq")   
            il_no_seq = il_no_seq + 100   
            excel_copy_move()
        }        
    }   
}
//-----------------------------------------------------------------
function excel_copy_insert()                 //엑셀자료 올리기시 삽입될때 로직
//-----------------------------------------------------------------
{
 i_excel = 0     
 ll_to_no_seq = ds_1.NameValue(ds_1.RowPosition,"no_seq")                         // 인스턴스 variable를 만듬으로 copy_move function에서 사용할수있슴.
 if (ds_1.RowPosition > 1) 
     ll_from_no_seq = ds_1.NameValue(ds_1.RowPosition - 1,"no_seq")
 else
     ll_from_no_seq = 0
 var tot_row=0    
 for (i_excel=ds_excel.CountRow; i_excel > 0 ;i_excel--) {   // 복사할 건수  
        if (ds_excel.RowMark(i_excel) == 1) tot_row++
 }
   
 ll_incre = (ll_to_no_seq - ll_from_no_seq) / (tot_row + 1)  
 ll_incre = parseInt(ll_incre)    // ll_incre를 문자인것차럼하여 소수점이하를 짤라냄.
 
   for (i_excel=ds_excel.CountRow; i_excel > 0 ;i_excel--) {
        if (ds_excel.RowMark(i_excel) == 1){
	        ds_1.InsertRow(ds_1.RowPosition)
           il_no_seq = ll_to_no_seq - ll_incre
	        excel_copy_move()
           ll_to_no_seq = il_no_seq 
	        sa_position++
	     }   
   }   
}
//----------------------------------------------------------------
function excel_copy_move()                  // 붛혀넣기시 공통 복사 move루틴  
//-----------------------------------------------------------------
