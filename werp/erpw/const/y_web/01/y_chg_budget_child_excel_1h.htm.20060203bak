 gd_1.SetColumn("no_seq");
       

}
//-----------------------------------------------------------------
function copy_add()                 // 실행내역 붙여넣기시 추가
//-----------------------------------------------------------------
{
 i = 0                             // 인스턴스 variable를 만듬으로 copy_move function에서 사용할수있슴.

 for (i=1; i <=ds_1copy.CountRow ;i++){
        ds_1.AddRow()
        if  (ds_1.CountRow == 1)
            il_no_seq = 0
        else  
            il_no_seq = ds_1.NameValue(ds_1.RowPosition - 1,"no_seq")   
        il_no_seq = il_no_seq + 100   
        copy_move()    
 }   
}
//-----------------------------------------------------------------
function copy_insert()                 //실행내역 붙여넣기기 삽입
//-----------------------------------------------------------------
{
 i = 0     
 ll_to_no_seq = ds_1.NameValue(ds_1.RowPosition,"no_seq")                         // 인스턴스 variable를 만듬으로 copy_move function에서 사용할수있슴.
 if (ds_1.RowPosition > 1) 
     ll_from_no_seq = ds_1.NameValue(ds_1.RowPosition - 1,"no_seq")
 else
     ll_from_no_seq = 0
 ll_incre = (ll_to_no_seq - ll_from_no_seq) / ( ds_1copy.CountRow + 1)  
 ll_incre = parseInt(ll_incre)    // ll_incre를 문자인것차럼하여 소수점이하를 짤라냄.
  
           
 for (i=ds_1copy.CountRow; i > 0 ;i--){
        ds_1.InsertRow(ds_1.RowPosition)

        il_no_seq = ll_to_no_seq - ll_incre
        copy_move()
        ll_to_no_seq = il_no_seq 
        sa_position++
 }   
}
//----------------------------------------------------------------
function copy_move()                  // 붛혀넣기시 공통 복사 move루틴  
//-----------------------------------------------------------------
{

   for (j=1;j<ds_1.CountColumn;j++)              //  ds_1copy의 것을 ds_1으로 복사한다.
       ds_1.ColumnValue(ds_1.RowPosition,j)=ds_1copy.ColumnValue(i,j)
   insert_init()            // 윗 for문장으로 일단 ds_1copy의것을 ds_1으로 move한후 이루틴으로 key를 현재의 
                            // tree레벨에 맞게 key부분을 다시 move한다.
}

