   ds_1.NameValue(row,"lab_amt") = Math.round(ds_1.NameValue(row,"qty") * ds_1.NameValue(row,"lab_price"))
   ds_1.NameValue(row,"exp_amt") = Math.round(ds_1.NameValue(row,"qty") * ds_1.NameValue(row,"exp_price"))
   ds_1.NameValue(row,"equ_amt") = Math.round(ds_1.NameValue(row,"qty") * ds_1.NameValue(row,"equ_price"))
   ds_1.NameValue(row,"sub_amt") = Math.round(ds_1.NameValue(row,"qty") * ds_1.NameValue(row,"sub_price"))
} 
if (colid == "mat_price") {
   ds_1.NameValue(row,"mat_amt") = Math.round(ds_1.NameValue(row,"qty") *  ds_1.NameValue(row,"mat_price"))
}                                      
if (colid == "lab_price") {
   ds_1.NameValue(row,"lab_amt") = Math.round(ds_1.NameValue(row,"qty") *  ds_1.NameValue(row,"lab_price"))
}                                      
if (colid == "exp_price") {
   ds_1.NameValue(row,"exp_amt") = Math.round(ds_1.NameValue(row,"qty") *  ds_1.NameValue(row,"exp_price"))
}                                      
if (colid == "equ_price") {
   ds_1.NameValue(row,"equ_amt") = Math.round(ds_1.NameValue(row,"qty") *  ds_1.NameValue(row,"equ_price"))
}                                      
if (colid == "sub_price") {
   ds_1.NameValue(row,"sub_amt") = Math.round(ds_1.NameValue(row,"qty") *  ds_1.NameValue(row,"sub_price"))
}                                      
if ((colid =="qty") || (colid == "mat_price") || (colid == "lab_price") || (colid == "exp_price") ||
    (colid =="equ_price") || (colid == "sub_price")) {
   ds_1.NameValue(row,"amt") = ds_1.NameValue(row,"mat_amt")  + ds_1.NameValue(row,"lab_amt") + 
                                  ds_1.NameValue(row,"exp_amt")  + ds_1.NameValue(row,"equ_amt") +
                                  ds_1.NameValue(row,"sub_amt")
   ds_1.NameValue(row,"price")  = ds_1.NameValue(row,"mat_price") +  ds_1.NameValue(row,"lab_price") + 
                                  ds_1.NameValue(row,"exp_price") +   ds_1.NameValue(row,"equ_price") + 
                                  ds_1.NameValue(row,"sub_price")                           
}                                  

if ((colid == 'name') || (colid == 'ssize') || (colid == 'unit'))
    f_name_key()
    


