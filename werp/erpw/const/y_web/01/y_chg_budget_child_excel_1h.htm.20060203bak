<script language=JavaScript for=gd_excel event=OnUserColor(row,eventid)>
 if(eventid == "ucolor_name") {
   if (ds_excel.NameValue(row,'length_name') > 50) {
       return "red";
   }    
   else { 
     if ((row % 2) == 0)
         return "#F4F2D8"
     else
         return "ffffff"
   }
 }
 if(eventid == "ucolor_ssize") {
   if (ds_excel.NameValue(row,'length_ssize') > 40) {
       return "red";
   }    
   else { 
     if ((row % 2) == 0)
         return "#F4F2D8"
     else
         return "ffffff"
   }
 }
 if(eventid == "ucolor_unit") {
   if (ds_excel.NameValue(row,'length_unit') > 10) {
       return "red";
   }    
   else { 
     if ((row % 2) == 0)
         return "#F4F2D8"
     else
         return "ffffff"
   }
 }


</script>


