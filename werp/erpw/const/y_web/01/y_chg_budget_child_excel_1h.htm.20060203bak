//----------------------------------------------------------------
function stand_copy_add()                  // 표준자료 복사시 추가 할떄.....
//-----------------------------------------------------------------
{
   ls_tempvalue = new Array();
	ls_tempvalue=is_returnvalue.split("@");

   tot_cnt = ls_tempvalue.length - 2  // 넘오어는 배열중 마지막에 @이 붙어 있으므로 하나 뻄 
                                      // 그리고배열은 0부터 시작하니깐 하나더 뻄
      for (j=0; j < tot_cnt ; j=j+12){    //  12개씩 짝이므로 (1 row당 11column)
         btnadd_onclick_common()
         k = j                            // 추가시는 배열이 처음부터 col순으로 move
         stand_insert_init()
   }
}
//----------------------------------------------------------------
function stand_copy_insert()                  // 표준자료 복사시 삽입 할떄.....
//-----------------------------------------------------------------
{
   var tot_row
	ls_tempvalue = new Array();
	ls_tempvalue=is_returnvalue.split("@");
   tot_cnt = ls_tempvalue.length - 2  // 넘오어는 배열중 마지막에 @이 붙어 있으므로 하나 뻄
   tot_row = (tot_cnt + 1) / 12       // 총배열의 item수는 +1 array는 0부터 시작됨으로 
   tot_row = parseInt(tot_row)
   ll_to_no_seq = ds_1.NameValue(ds_1.RowPosition,"no_seq")                         // 인스턴스 variable를 만듬으로 copy_move function에서 사용할수있슴.
   if (ds_1.RowPosition > 1) 
       ll_from_no_seq = ds_1.NameValue(ds_1.RowPosition - 1,"no_seq")
   else
       ll_from_no_seq = 0
   ll_incre = (ll_to_no_seq - ll_from_no_seq) / (tot_row + 1)  
   ll_incre = parseInt(ll_incre)    // ll_incre를 문자인것차럼하여 소수점이하를 짤라냄.

   for (j=tot_cnt; j > 0 ; j=j-12){    //  삽입으므로 뒤에서 부터 삽입한다.(1row당 11column
      stand_insert_paste() 
      k = j - 11         // 삽입시는   배열이 뒤에서부터 앞으로 col순으로 move
      stand_insert_init()
      sa_position++
   }
}
//-------------------------------------------------------------------
function  stand_insert_init()              //표준자료 복사시 공통적으로 옮겨주는 루틴
//---------------------------------------------------------------------
{
      ds_1.NameValue(ds_1.RowPosition,"detail_code") = ls_tempvalue[k]
