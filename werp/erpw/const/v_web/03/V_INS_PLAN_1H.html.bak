//------------------------------------------------------------------------
function r_ins_section_move()                     // 조회  
//------------------------------------------------------------------------
{
	if ( r_ins_section[0].checked ==  true ) 
	{
		ccom_quarter_year.ResetData()
	}
	else if ( r_ins_section[1].checked ==  true)
	{
		ccom_quarter_year.ResetData()
	} 
	else if ( r_ins_section[2].checked ==  true)
	{
		ccom_quarter_year.ClearData()
		ccom_quarter_year.AddData('A','상반기')
		ccom_quarter_year.AddData('B','하반기')
		ccom_quarter_year.index = 0
	}   
	
	 btnquery_onclick()   
	
} 
