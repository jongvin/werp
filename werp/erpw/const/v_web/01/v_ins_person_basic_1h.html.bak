function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
	if (il_nindex == 1) {
		ll_cnt2 = f_ds_select_q("ds_count_2","select seq select_1 from v_ip_achievement where emp_no='" + ds_1.NameValue(ds_1.RowPosition, "emp_no") + "' ")	
