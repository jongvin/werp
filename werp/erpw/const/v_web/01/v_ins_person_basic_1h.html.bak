//---------------------------------------------------------------------------
function dept_code_find()                       //현장 찾기 
//---------------------------------------------------------------------------
{
	result = window.showModalDialog("z_dept_find_all_1h.html", gs_home + "@" + tx_dept_name.value,"resizable:no;scroll:no;status:no;help:no;dialogWidth:380px;dialogHeight:500px");
	if (result==false) return
	
	array = result.split("@")
	
	is_dept_code = array[0]
	is_dept_name = array[1]

	tx_dept_name.value = is_dept_name
	ds_1.NameValue(ds_1.RowPosition, "dept_code") = is_dept_code
}  

//-----------------------------------------------------------------------
function search_person()                            // 검색
//-----------------------------------------------------------------------
{
	tb_1.ActiveIndex = 1
