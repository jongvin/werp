//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
//	if (il_focus == 1 || il_nindex == 1) {

	if (il_nindex == 1) {

		if (ds_1.IsUpdated){
	
	   if (f_check_date(em_join_dt.text) == false  ) 
	   { alert("입사일이 유효하지 않습니다.") ; return ;  }
	   if (f_check_date(em_approve_dt.text) == false  ) 
	   { alert("인정일자이 유효하지 않습니다.") ; return ; } 		
	   if (f_check_date(em_effect_dt.text) == false  ) 
	   { alert("유효일자이 유효하지 않습니다.") ; return ; } 			   
			if (em_emp_no.text.length > 8) {
				alert("사번은 8자의 영문, 숫자만 입력 가능합니다")
				em_emp_no.focus()
				return
			}
			var ll_dup
	   	 ll_dup = f_dup_chk("ds_1","emp_no")
	   	 if (ll_dup > 0) {
	   	     alert("사번이 중복되었습니다: 사번을 수정하십시요")
	   	     ds_1.RowPosition = ll_dup
	        	  em_emp_no.focus()
	   	     return
	   	 }
	   	 
	   	 if (ds_1.NameValue(ds_1.RowPosition, "name") == "" || ds_1.NameValue(ds_1.RowPosition, "name") == null) {
	   	 	  alert("성명을 입력하십시요")
	   	 	  tx_name.focus()
	   	 	  return
	   	 }
		}
		
		if (ds_1.IsUpdated)  {
         if (confirm("변경되었습니다 저장하시겠습니까?")) 
             tr_1.Post()
             
      check_save = 0
      }
	}
	
	if ( il_nindex != 1) {
		div_btnsave_onclick()
		return	
	}
	
}

//---------------------------------------------------------------------------
function div_btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
	if (il_nindex == 2) {
		if (ds_2.IsUpdated) {
			if (ds_2.NameValue(ds_2.RowPosition, "graduation_dt").length != 0) {
				if (!f_check_date(ds_2.NameValue(ds_2.RowPosition, "graduation_dt"))) {
					alert("졸업일이 올바르지 않습니다")
					gd_2.SetColumn("graduation_dt");
				   return
				}
			}
			if (confirm("변경되었습니다 저장하시겠습니까?")) 
             tr_2.Post()
             
         check_save = 0
		}
	}
	
	if (il_nindex == 3) {
		if (ds_3.IsUpdated) {
			if (ds_3.NameValue(ds_3.RowPosition, "join_dt").length != 0) {
				if (!f_check_date(ds_3.NameValue(ds_3.RowPosition, "join_dt"))) {
					alert("입사일이 올바르지 않습니다")
					gd_3.SetColumn("join_dt");
				   return
				}
			}
			if (ds_3.NameValue(ds_3.RowPosition, "leave_dt").length != 0) {
				if (!f_check_date(ds_3.NameValue(ds_3.RowPosition, "leave_dt"))) {
					alert("퇴사일이 올바르지 않습니다")
					gd_3.SetColumn("leave_dt");
				   return
				}
			}
			if (confirm("변경되었습니다 저장하시겠습니까?")) 
             tr_3.Post()
             
         check_save = 0
		}
	}
	
	if (il_nindex == 4) {
		if (ds_4.IsUpdated) {
			if (ds_4.NameValue(ds_4.RowPosition, "dt").length != 0) {
				if (!f_check_date(ds_4.NameValue(ds_4.RowPosition, "dt"))) {
					alert("일자가 올바르지 않습니다")
					gd_5.SetColumn("dt");
				   return
				}
			}
			if (confirm("변경되었습니다 저장하시겠습니까?")) 
             tr_4.Post()
             
         check_save = 0
		}
	}
	
	if (il_nindex == 5) {
		if (ds_5.IsUpdated) {
			if (ds_5.NameValue(ds_5.RowPosition, "obtain_dt").length != 0) {
				if (!f_check_date(ds_5.NameValue(ds_5.RowPosition, "obtain_dt"))) {
					alert("취득일이 올바르지 않습니다")
					gd_5.SetColumn("obtain_dt");
				   return
				}
			}
			if (confirm("변경되었습니다 저장하시겠습니까?")) 
             tr_5.Post()
             
         check_save = 0
		}
	}
}

