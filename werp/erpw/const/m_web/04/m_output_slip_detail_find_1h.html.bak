<SCRIPT language=JavaScript>
var gs_home
var is_ds_1_addr = 'm_output_slip_detail_find_1q.jsp?arg_dept_code='                     //
var is_tr_1_addr
var is_dept_code ,is_approym,is_approseq,is_chg_cnt,is_unq_num, is_yymm
var id_date
var ii_seq,is_class
var is_confirm_check = 'no'
//---------------------------------------------------------------
function btnquery_onclick()               //조회 
//---------------------------------------------------------------
{
	ds_1.DataID = is_ds_1_addr + is_dept_code + '&arg_sdate=' + is_yymm
	ds_1.SyncLoad=true
	ds_1.reset()
}
//----------------------------------------------------------------
function Button_close_onclick()               //닫기
//----------------------------------------------------------------
{
 window.returnValue = "";
 window.close();
}
//--------------------------------------------------------------------
function Button_select()               //전체선택
//--------------------------------------------------------------------
{
var i
	for(i=1; i <= ds_1.CountRow ; i++){
		ds_1.NameValue(i,"comp_chk") = 'T'
		ds_1.NameValue(i,"vat_unq_num") = is_unq_num
		if (ds_1.NameValue(i,'vattag') == '3'){
			ds_1.NameValue(i,"vat_amt") = parseInt(ds_1.NameValue(i,"amt") * 0.1)
		}
	}
}
//--------------------------------------------------------------------
function Button_noselect()               //전체해제
//--------------------------------------------------------------------
{
var i
	for(i=1; i <= ds_1.CountRow ; i++){
		ds_1.NameValue(i,"comp_chk") = 'F'
		ds_1.NameValue(i,"vat_unq_num") = 0
		ds_1.NameValue(i,"vat_amt") = 0
	}
}
//--------------------------------------------------------------------
function Button_confirm_onclick()               //복사 버튼
//--------------------------------------------------------------------
{
	is_confirm_check = 'yes'

	tr_1.Post()
	
	btnquery_onclick()
}
//----------------------------------------------------------------------------
function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
	ds_1.Undo(ds_1.RowPosition)
}
//------------------------------------------------------------------------
function get_focus(focus) // gd가 포커스를 받을때 동작.
//------------------------------------------------------------------------
{
	il_screen = focus
}

//----------------------------------------------------------------
function window_onload()                  // window open event 
//-----------------------------------------------------------------
{
	var result;
	var array = new Array();
	result = window.dialogArguments;
	
	array = result.split("@");
	gs_home      = array[0];
	is_dept_code = array[1];
	is_unq_num   = array[2];
	is_yymm      = array[3];
	is_tr_1_addr = gs_home + '/const/m_web/04/m_output_slip_detail_find_1tr.jsp'
	tr_1.Action = is_tr_1_addr
	btnquery_onclick()
}
//-----------------------------------------------------------------
function window_onunload()     // windoe.close()문장을 만나면 이루틴은 자동으로 수행됨으로 복사여부를 체크.
//-----------------------------------------------------------------
{
  if (is_confirm_check == 'no')
      window.returnValue = ""
}
 
</SCRIPT>
<script language=JavaScript for=gd_1 event=OnClick(row,colid)>
if (colid == "comp_chk") {
	if (ds_1.NameValue(row,colid) == 'T'){
		ds_1.NameValue(row,colid) = 'F'
		ds_1.NameValue(row,"vat_unq_num") = 0
		ds_1.NameValue(row,"vat_amt") = 0
	}
	else{
		ds_1.NameValue(row,colid) = 'T'    
		ds_1.NameValue(row,"vat_unq_num") = is_unq_num
		if (ds_1.NameValue(row,'vattag') == '3'){
			ds_1.NameValue(row,"vat_amt") = parseInt(ds_1.NameValue(row,"amt") * 0.1)
		}
	}
}
</SCRIPT>

<SCRIPT language=JavaScript for=ds_2 event=OnColumnChanged(row,colid)>
	if (colid == "comp_chk") {
		if (ds_1.NameValue(row,"comp_chk") == 'T' ) {
			ds_1.NameValue(row,"vat_unq_num") = is_unq_num
			if (ds_1.NameValue(row,'vattag') == '3'){
				ds_1.NameValue(row,"vat_amt") = parseInt(ds_1.NameValue(row,"amt") * 0.1)
			}
		}
		else {
			ds_1.NameValue(row,"vat_unq_num") = 0
			ds_1.NameValue(row,"vat_amt") = 0
		}
	}
</SCRIPT>
