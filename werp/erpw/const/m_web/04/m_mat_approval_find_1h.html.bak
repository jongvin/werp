var is_ds_2_addr = 'm_mat_approval_find_2q.jsp?arg_dept_code='
var is_ds_3_addr = 'm_mat_approval_find_3q.jsp?arg_dept_code='
var is_confirm_check = 'no'
var is_dept_code
var tmp_array = new Array();
var ll_cnt
//------------------------------------------------------------------------
function keyDown()                       //검색어 입력후 enter를 쳤을경우
//------------------------------------------------------------------------
{
	if (window.event.keyCode == 13) { 
		btnquery_onclick();
	} 
}

//---------------------------------------------------------------
function btnquery_onclick()               //조회 
//---------------------------------------------------------------
{
  ds_1.DataID =  is_ds_1_addr + is_dept_code
  ds_1.SyncLoad=true
  ds_1.reset()
  
  ds_2.DataID =  is_ds_2_addr + is_dept_code
  ds_2.SyncLoad=true
  ds_2.reset()

  ds_3.DataID =  is_ds_3_addr + is_dept_code
  ds_3.SyncLoad=true
  ds_3.reset()

}
//----------------------------------------------------------------
function Button_close_onclick()               //닫기
//----------------------------------------------------------------
{
 window.returnValue = "";
 window.close();
}
//--------------------------------------------------------------------
function Button_confirm_onclick()               //확인
//--------------------------------------------------------------------
{
	var i,ls_chk_code;
	ls_chk_code = ds_1.NameValue(ds_1.RowPosition,'sbcr_code')
	if (ll_cnt > 0){
	   	for (i=0; i< ll_cnt; i++){
			if (ls_chk_code == tmp_array[i] ) {
				alert("이미 선정된 업체입니다")
				return
			}
		}
	}
  is_confirm_check = 'yes'
  var ls_return=""

	if (tb_1.ActiveIndex == 1 ) {
		ls_return = ds_1.NameValue(ds_1.RowPosition,'sbcr_code') + '@'  +
		           ds_1.NameValue(ds_1.RowPosition,'sbcr_name') + '@'  +
		           ds_1.NameValue(ds_1.RowPosition,'approym') + '@' +
		           ds_1.NameValue(ds_1.RowPosition,'approseq') + '@' +
		           ds_1.NameValue(ds_1.RowPosition,'chg_cnt') + '@' +
		           ds_1.NameValue(ds_1.RowPosition,'approtitle') + '@' +
		           '1' + '@' +
		           ds_1.NameValue(ds_1.RowPosition,'vattag') + '@' + 
		           ds_1.NameValue(ds_1.RowPosition,'paymentmethod') + '@'  
	}
	else {
		if (tb_1.ActiveIndex == 2 ) {
			ls_return = ds_2.NameValue(ds_2.RowPosition,'sbcr_code') + '@'  +
			           ds_2.NameValue(ds_2.RowPosition,'sbcr_name') + '@'  +
			           ds_2.NameValue(ds_2.RowPosition,'approym') + '@' +
			           ds_2.NameValue(ds_2.RowPosition,'approseq') + '@' +
			           ds_2.NameValue(ds_2.RowPosition,'chg_cnt') + '@' +
			           ds_2.NameValue(ds_2.RowPosition,'approtitle') + '@' +
			           '8' + '@' + '9' + '@' + 
			           ds_2.NameValue(ds_2.RowPosition,'paymentmethod') + '@'  
		}
		else {
			ls_return = ds_3.NameValue(ds_3.RowPosition,'sbcr_code') + '@'  +
			           ds_3.NameValue(ds_3.RowPosition,'sbcr_name') + '@'  +
			           ds_3.NameValue(ds_3.RowPosition,'approym') + '@' +
			           ds_3.NameValue(ds_3.RowPosition,'approseq') + '@' +
			           ds_3.NameValue(ds_3.RowPosition,'chg_cnt') + '@' +
			           ds_3.NameValue(ds_3.RowPosition,'approtitle') + '@' +
			           '9' + '@' + '9' + '@' + 
			           ds_3.NameValue(ds_3.RowPosition,'paymentmethod') + '@'  
		}
	}
              
        
 window.returnValue = ls_return
 window.close();
}
//----------------------------------------------------------------
function window_onload()                  // window open event 
//-----------------------------------------------------------------
{
	var result;
	var array = new Array();
	result = window.dialogArguments;
	
	if ( result != '' ) {
	tmp_array = result.split("@");
	ll_cnt = tmp_array.length - 1
	}
	else {
	ll_cnt = 0
	}
	
	if (result == "") return 
	is_dept_code = result
	
	btnquery_onclick()
}
//-----------------------------------------------------------------
function window_onunload()     // windoe.close()문장을 만나면 이루틴은 자동으로 수행됨으로 복사여부를 체크.
//-----------------------------------------------------------------
{
  if (is_confirm_check == 'no')
      window.returnValue = ""
}

