//-----------------------------------------------------------------------
function div_btninsert_onclick()                    //하위 삽입.
//-----------------------------------------------------------------------
{
	var parm;
	
	if (! date_check()) {
	    alert('동일한 년월이어야합니다')
	    return
	}    
	if (ds_1.IsUpdated){
		alert('상위화면을 저장하시고 추가하시기 바랍니다...')
		return
	}

	
	if (ds_1.CountRow < 1) { 
		alert("선택한 내역이 없기때문에 들어갈수 없습니다")
		return
	}   
	if (ds_1.NameValue(ds_1.RowPosition,"process_tag") != '1') {
		alert("작성중이 아니어서추가할 수 없습니다...")
		return
	}
	if (ds_1.NameValue(ds_1.RowPosition,"invoice_num") != '') {
		alert("이미 전표가 생성되어 추가할 수 없습니다...")
		return
	}
	if (ds_1.NameValue(ds_1.RowPosition,"vatcode") == ''){
		alert("부가세구분을 반드시 선택하시고 추가하십시오 ...")
		return
	}
	if (ds_1.NameValue(ds_1.RowPosition,"ditag") == ''){
		alert("자재분류를 반드시 선택하시고 추가하십시오 ...")
		return
	}
	

	parm = gs_home + '@' + ds_1.NameValue(ds_1.RowPosition,'acnt_status') + '@' + is_dept_code + '@' + ds_1.NameValue(ds_1.RowPosition,"approym") +
	      '@' + ds_1.NameValue(ds_1.RowPosition,"approseq") + '@' + ds_1.NameValue(ds_1.RowPosition,"chg_cnt") +
	      '@' + ds_1.NameValue(ds_1.RowPosition,"vat_unq_num") + '@' + ds_1.NameValue(ds_1.RowPosition,'sbcr_code') + 
	      '@' + ds_1.NameValue(ds_1.RowPosition,'vatcode') + '@' + + ds_1.NameValue(ds_1.RowPosition,'ditag')

	window.showModalDialog("m_mat_request_detail_find_1h.html" ,parm,"dialogWidth:"+1000+"px; dialogHeight:"+700+"px");
	
	div_btnquery_onclick()
	ds_1.NameValue(ds_1.RowPosition,"amt") = ds_2.Sum(13,0,0)
	ds_1.NameValue(ds_1.RowPosition,"vatamount") = parseInt(ds_2.Sum(13,0,0) * 0.1)
	tr_1.Post()
	comp_rtn()
}
//------------------------------------------------------------------------
function comp_rtn()                //부가세 재계산
//------------------------------------------------------------------------
{
	var ll_amt,ll_row
	ll_amt = ds_1.NameValue(ds_1.RowPosition,"vatamount") - ds_2.Sum(14,0,0)
	if (ll_amt != 0) {
		ll_row = ds_2.NameMax('amt',0,0);
		ll_row = ds_2.NameValueRow('amt',ll_row);
		if (ll_row == 0) {
			ll_row = ds_2.CountRow;
		}
		ds_2.NameValue(ll_row,"vatamt") = ds_2.NameValue(ll_row,"vatamt") + ll_amt;
		tr_2.Post()
	}
}
//------------------------------------------------------------------------
function div_btndelete_onclick()                //하위 삭제
//------------------------------------------------------------------------
{
	var ls_chk=0,ll_input_unq,ll_invoice_num,ls_tag

	//손료처리 여부검색
	if (Fn_RentChk()) return;
	
	if (ds_1.RowPosition < 1) return

	if (ds_1.NameValue(ds_1.RowPosition,"invoice_num") != '' ) {
		alert("이미 전표가 생성되어 삭제할 수 없습니다...")
		return
	}
	if (ds_1.NameValue(ds_1.RowPosition,"process_tag") != '1' ) {
		alert("작성중이 아니어서 삭제할 수 없습니다...")
		return
	}
	
	for (i=ds_2.CountRow ;i >0 ;i--)
	{
		ll_input_unq = ds_2.NameValue(i,'input_unq_num')
	 	f_select_q("select nvl(count(*),0) select_1 from m_output_detail" +
	 	           " where dept_code = '" + is_dept_code + "' and input_unq_num = " + ll_input_unq +
	 	           "  and vat_unq_num is not null and vat_unq_num <> 0 ")
	 	ls_chk = parseInt(ds_select.NameValue(ds_select.RowPosition,'select_1'),10);

	 	if (ls_chk > 0 ) {
	 		alert('출고전표에 자료가 존재하여 삭제하실 수 없습니다...')
	 		return
	 	}
	}  

	for (i=ds_2.CountRow ;i >0 ;i--)
	{
		if (ds_2.RowMark(i) == 1){
			ds_2.NameValue(i,"vat_unq_num") = 0
		}
	}  
	tr_2.Post()
	div_btnquery_onclick()
	ds_1.NameValue(ds_1.RowPosition,"amt") = ds_2.Sum(13,0,0)
	ds_1.NameValue(ds_1.RowPosition,"vatamount") = parseInt(ds_2.Sum(13,0,0) * 0.1)
	tr_1.Post()
	comp_rtn()
}
