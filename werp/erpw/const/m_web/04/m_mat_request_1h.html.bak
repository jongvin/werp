//---------------------------------------------------------------------------
function conf_rtn()                      // 세금계산서 의뢰
//---------------------------------------------------------------------------
{
	var i,ll_amt,ll_chk
	
	if (ds_1.CountRow < 1) {
		return
	}
	
	var li_sql,ll_cnt;

	//손료처리 여부검색
	if (Fn_RentChk()) return;

	//부가세금액 체크
	li_sql = ""
	li_sql = li_sql + " SELECT"
	li_sql = li_sql + " 	COUNT(*) select_1"
	//	B.DEPT_CODE,B.VAT_UNQ_NUM,B.yyyymmdd,B.VATAMOUNT,A.VATAMT
	li_sql = li_sql + " FROM"
	li_sql = li_sql + " 	(SELECT DEPT_CODE,VAT_UNQ_NUM,NVL(SUM(VATAMT),0) VATAMT,NVL(SUM(AMT),0) AMT FROM M_INPUT_DETAIL GROUP BY DEPT_CODE,VAT_UNQ_NUM) A,"
	li_sql = li_sql + " 	M_VAT B"
	li_sql = li_sql + " WHERE"
	li_sql = li_sql + " 	A.DEPT_CODE = B.DEPT_CODE"
	li_sql = li_sql + " AND A.VAT_UNQ_NUM = B.VAT_UNQ_NUM"
	li_sql = li_sql + " AND (B.VATAMOUNT <> A.VATAMT OR B.AMT <> A.AMT)"
	li_sql = li_sql + " AND B.yyyymmdd > '2005-06-30'"
	ll_cnt = f_select_q(li_sql)
	if (ll_cnt <= 0) {alert("에러발생");return}
	ll_cnt = ds_select.NameValue(ds_select.RowPosition,'select_1')
