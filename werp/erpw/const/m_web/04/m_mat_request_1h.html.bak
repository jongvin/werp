//-----------------------------------------------------------------------
function Fn_RentChk()   // 손료처리 여부 검색
//-----------------------------------------------------------------------
{
	var ls_rentChk;
	ll_cnt = f_select_q("select nvl(count(*),0) select_1 from m_tmat_proj_rent where dept_code = '" + is_dept_code + "' and YYMMDD >= '" + is_yymm + "'")
	ls_rentChk = ds_select.NameValue(ds_select.RowPosition,'select_1')
	if (ls_rentChk != 0 || ll_cnt == 0) {
		alert('손료처리가되어 대금청구 하실수 없습니다.')
		return true;
	}

	ll_cnt = f_select_q("select nvl(count(*),0) select_1 from m_tmat_stock a " +
						"where a.dept_code = '" + is_dept_code + "' and a.yymmdd < '" + is_yymm + "' and a.proc_yn = 'Y' and a.qty > 0")
	ls_rentChk = ds_select.NameValue(ds_select.RowPosition,'select_1')
	if (ls_rentChk != 0 || ll_cnt == 0) {
//		alert('전달 손료처리되어 있지 않아 대금청구 하실수 없습니다.')
//		return true;
	}

	return false;
}
