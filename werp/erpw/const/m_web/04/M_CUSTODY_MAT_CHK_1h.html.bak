<SCRIPT language=JavaScript>
//------------------------------------------------------------------------
function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{
	ds_1.DataID = is_ds_1_addr + '?arg_dept_code=' + is_dept_code + "&arg_to_date=" + em_to_year.text + "&arg_from_date=" + em_from_year.text 
  	ds_1.SyncLoad=true
 	ds_1.Reset()
}
//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
 	ds_1.AddRow()   
	
	div_init_move()
}
//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
	if (ds_1.CountRow == 0)				 // 한건도 없을경우는  0 
	{                                       
		btnadd_onclick()   
		return
	}  
	ds_1.InsertRow(ds_1.RowPosition)   


	div_init_move() 
}
//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{

  if  (gd_1.MultiRowSelect)
  {                                   // multi row select 일 경우
     for (i=ds_1.CountRow ;i >0 ;i--)                          // 로우의 제일끝에서부터 앞으로 루핑
     {
        if (ds_1.RowMark(i) == 1)                              // 선택되어 있으며는 
               ds_1.DeleteRow(i)
    }   
    ds_1.MarkRows(ds_1.RowPosition,ds_1.RowPosition)           // 삭제작업완료후 MARK(멀티로우 때문에 한번 마킹해준다)
  }
  else
 	ds_1.DeleteRow(ds_1.RowPosition)  

}
//------------------------------------------------------------------------
 function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
    ds_1.Undo(ds_1.RowPosition)                                     //해당 row만 취소한다.

}
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
	if (ds_1.IsUpdated)											 //변경된것이 있으며는 
	{                                          
		for ( i = 0 ; i <= ds_1.CountRow ; i ++ )
		{
		  if (  f_check_date (ds_1.NameValue(i,"yymmdd")) == false ) 
		  { 
		  		alert("날짜가 틀렸습니다.") ; 
		  		ds_1.RowPosition = i ;
		  		gd_1.SetColumn("yymmdd")
		  		return false ; 
		  }   		
		}

			if  (confirm("변경되었습니다 저장하시겠습니까?"))
			{
	      	tr_1.Post()                                           // 저장(window_onload()에서 tr_1.Action 이 연결되어있어야한다.) 
	 		}
	} 

}
//------------------------------------------------------------------------
function btnquery_prt_onclick()                     // 미리보기
//------------------------------------------------------------------------
{
   var arg_temp
   arg_temp ="const_rpt/m_web/04r/"      // 보고서 디렉토리
   arg_temp +=  "M_CUSTODY_MAT_CHK"   // 보고서명
	arg_temp +=  "&RptParams=" + em_to_year.text + "&RptParams=" + em_from_year.text + "&RptParams=" + is_dept_code 
  
//   alert(arg_temp);
   f_crystal_report(arg_temp)   //보고서 호출
}
//----------------------------------------------------------------------
function div_init_move()                         // initial move
//----------------------------------------------------------------------
{
   var temp = 0;
   
	for( i = 1 ; i <= ds_1.CountRow  ; i++ )	
	{
		if ( parseInt(ds_1.NameValue(i,"seq")) > parseInt(temp) )
	   	temp = ds_1.NameValue(i,"seq") 
	}
   ds_1.NameValue(ds_1.RowPosition,"seq") = parseInt(temp) + 1
   ds_1.NameValue(ds_1.RowPosition,"dept_code") = is_dept_code 
}
//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
  top.frames['main_title'].mouse_over()

//   f_set_resize() 

	is_dept_code = top.frames['security'].gs_dept_code.value
	is_dept_name = top.frames['security'].gs_dept_name.value

	dept_name.value = is_dept_name 

	tr_1.Action = is_tr_1_addr

	em_to_year.text =  getTime()  
	em_from_year.text = getTime()

	btnquery_onclick()
	
}
//---------------------------------------------------------------------
function getTime()
//---------------------------------------------------------------------
{
    today = new Date();
    
	 ty = today.getFullYear();
	 tm = today.getMonth()+1;
	 td = today.getDate();
 	 th = today.getHours();
//	 tm = today.getMinutes();
	 ts = today.getSeconds();
		
 	 if (tm < 10) tm = "0"+ tm;
 	 if (td < 10) td = "0"+ td;

  	 day =  ty + tm + td

    return day
}
//------------------------------------------------------------------------
function keyDown()                      //검색어 입력후 enter를 쳤을경우
//------------------------------------------------------------------------
{
	if (window.event.keyCode == 13) { 
		dept_code_find();
	} 
}
//---------------------------------------------------------------------
function window_onunload()
//---------------------------------------------------------------------
{
}
//---------------------------------------------------------------------------
function dept_code_find()                       //현장 찾기 
//---------------------------------------------------------------------------
{
	result = f_dept_code_find(dept_name.value)

	if (result==false) return

	is_dept_code = top.frames['security'].gs_dept_code.value
	is_dept_name = top.frames['security'].gs_dept_name.value

	dept_name.value = is_dept_name
   
	btnquery_onclick()	
}   
</script>
