//---------------------------------------------------------------------
function fc_request_copy()               //견적내역 복사
//---------------------------------------------------------------------
{
	var parm;
	if (ds_1.CountRow < 1) { 
		alert("선택한 내역이 없기때문에 들어갈수 없습니다")
		return
	}
	if (ds_1.IsUpdated){
		if  (confirm("변경되었습니다 저장하시겠습니까?")){
			tr_1.Post()
			if (tr_1.ErrorCode != 0) {alert("에러발생");return;}
		} else {
			return
		}
	}

	parm = gs_home + '@' + ds_1.NameValue(ds_1.RowPosition,"estimateyymm") + '@' + ds_1.NameValue(ds_1.RowPosition,"estimateseq")
	var ls_returnvalue
	ls_returnvalue = window.showModalDialog("../00/m_request_find_1h.html" ,parm,"dialogWidth:"+1000+"px; dialogHeight:"+700+"px");
	if (ls_returnvalue != '') {
      if (ds_1.NameValue(ds_1.RowPosition,'deliverylimitdate') == '')
         ds_1.NameValue(ds_1.RowPosition,'deliverylimitdate') = ls_returnvalue 
	}
	if (ds_1.IsUpdated){
		tr_1.Post()
	}
	div_btnquery_onclick()
}
//------------------------------------------------------------------------
function div_btndelete_onclick()                //하위 삭제
//------------------------------------------------------------------------
{
	var ls_dept_code,ll_request_unq_num,ll_qty,arg_cmd,j,ls_dept_code,ls_requestseq,ls_chg_cnt,arg_cmd
	var dept_array = new Array();
	var requestseq_array = new Array();
	var chg_cnt_array = new Array();

	if (ds_1.NameValue(ds_1.RowPosition,"choicetag") == 'Y') {
		alert("이미 선정된 업체는 자료를 삭제할 수 없습니다")
		return
	}

	ls_yymm = ds_1.NameValue(ds_1.RowPosition,"estimateyymm")
	ll_seq  = ds_1.NameValue(ds_1.RowPosition,"estimateseq")
	ll_cnt = f_select_q("select nvl(count(*),0) select_1 from m_vender_est where estimateyymm = '" + ls_yymm + "' and estimateseq = " + ll_seq + " ")
	if (ds_select.ErrorCode != 0) {alert("에러발생");return;}
	if (ll_cnt > 0) {
		ll_chk = ds_select.NameValue(ds_select.RowPosition,"select_1")
		if (ll_chk > 0 ) {
			alert("업체가 존재하면 자료를 삭제할 수 없습니다")
			return
		}
	}

	if (ds_2.IsUpdated ) {
		div_btnsave_onclick()
	}
	if  (confirm("삭제하시면 바로 저장됩니다 계속하시겠습니까?")){
	}
	else {
		return
	}
	j=0
	for (i=ds_2.CountRow ;i >0 ;i--)
	{
		if (ds_2.RowMark(i) == 1){
			ll_request_unq_num = ds_2.NameValue(i,'request_unq_num')
			ls_chg_cnt = ds_2.NameValue(i,'chg_cnt')
			ll_cnt = f_select_q("select dept_code select_1,requestseq select_2,chg_cnt select_3 from m_request_detail where request_unq_num = " + ll_request_unq_num + " and chg_cnt = " + ls_chg_cnt)
			if (ds_select.ErrorCode != 0) {alert("에러발생");return;}
			if (ll_cnt > 0) {
				ls_dept_code = ds_select.NameValue(ds_select.RowPosition,"select_1")
				ls_requestseq = ds_select.NameValue(ds_select.RowPosition,"select_2")
				ls_chg_cnt = ds_select.NameValue(ds_select.RowPosition,"select_3")
				if (dept_array[j] != ls_dept_code || requestseq_array[j] != ls_requestseq || chg_cnt_array[j] != ls_chg_cnt) {
					j = j + 1;
					dept_array[j] = ls_dept_code
					requestseq_array[j] = ls_requestseq
					chg_cnt_array[j] = ls_chg_cnt
				}
			}
			ds_2.DeleteRow(i)
		}
	}  

	if  (ds_2.RowPosition != 0)  {
		ds_2.MarkRows(ds_2.RowPosition,ds_2.RowPosition)  //삭제작업완료후 MARK
	}
	tr_2.Post()
	
	for(i=1;i<=j;i++){	
		ls_dept_code = dept_array[i]
		ls_requestseq = requestseq_array[i]
		ls_chg_cnt = chg_cnt_array[i]
		ll_cnt = f_select_q("select nvl(count(*),0) select_1 " +
		                    "  from m_est_detail a, " +
		                    "      ( select request_unq_num " +
		                    "          from m_request_detail " +
			 					  "         where dept_code = '" + ls_dept_code + "'" +
								  "           and requestseq = " + ls_requestseq +
			   				  "           and chg_cnt = " + ls_chg_cnt + ") b " +
								  "         where a.request_unq_num = b.request_unq_num " )
	   ll_cnt = ds_select.NameValue(ds_select.RowPosition,'select_1')
	   if (ll_cnt == 0) {
			arg_cmd = " update m_request " +
						"     set approve_class = '4'" +
						"   where dept_code = '" + ls_dept_code + "'" +
						"     and requestseq = " + ls_requestseq + 
						"     and chg_cnt = " + ls_chg_cnt
			f_update_sql(arg_cmd)
	   }
	}
 
 }
//---------------------------------------------------------------------------
function div_btnsave_onclick()                  //하위 저장 
//---------------------------------------------------------------------------
{
	var ls_chk=0
	
	
	if (ds_2.IsUpdated || ds_1.IsUpdated ){
		if  (confirm("변경되었습니다 저장하시겠습니까?")){
			if (ds_1.IsUpdated) {
				tr_1.Action = is_tr_1_addr
				tr_1.Post()
			}
			if (ds_2.IsUpdated) {
			   	for (i=1; i<= ds_2.CountRow; i++){
				   	if (ds_2.NameValue(i,"mtrcode") == ''){
			   			ls_chk++
		   			}
		   		}
		   		if (ls_chk == 0) {
					tr_2.Action = is_tr_2_addr
			       		tr_2.Post()
			       		sp_call()
		       		}
	       			else {
			   		alert("자재코드를 반드시 입력하십시오 \n        저장되지않았습니다.")
			       	}
			}
	       	}
	}
}
