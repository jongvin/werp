//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
   top.frames['main_title'].mouse_over()
	tr_1.Action = is_tr_1_addr
	tr_2.Action = is_tr_2_addr
  	ds_seq.DataID = is_ds_seq_addr                       // SEQ NO 의 DATAID
	top.frames['main_title'].title_name.value = " " + "견적내역등록"             

 // -------------------------업체별견적으로  들어갔다가 다시 돌아왔을경우 해당 견적번호로 가기위해서 다음로직수행
  	var result="";
	var array = new Array();
	result = readCookie("@m_vender_est_list@")
	if (result == "err") {
		result = readCookie("@m_vender_est_compare_detail@")
		if (result != "err"){
			array = result.split("@");
	      
   		ls_date = array[2]  //estimateyymm 
   		is_fr_date = array[6]
   		is_to_date = array[9]
	   	em_select_yymm.text = is_fr_date
	   	em_select_to_yymm.text = is_to_date
			ll_seq = parseInt(array[3])    // estimateseq
			btnquery_onclick()
			for (i=1; i<= ds_1.CountRow; i++) {
				if ((ds_1.NameValue(i,"estimateyymm") == ls_date) && (ds_1.NameValue(i,"estimateseq") == ll_seq)) {
					ds_1.RowPosition = i
					makeCookie("@m_vender_est_compare_detail@","")
					return
				}
			}
			makeCookie("@m_vender_est_compare_detail@","")
		}			
		else {
			is_fr_date = is_fr_date.substr(0,7) + ".01"
			em_select_yymm.text = is_fr_date
	   	em_select_to_yymm.text = is_to_date
     		btnquery_onclick()
     	}
	}   
	else {   
		array = result.split("@");

		ls_date = array[2]  //estimateyymm 
		is_fr_date = array[6]  //조회 입력날짜. 
		is_to_date = array[9]  //조회 입력날짜. 
   	em_select_yymm.text = is_fr_date
   	em_select_to_yymm.text = is_to_date
		ll_seq = parseInt(array[3])    // estimateseq
		btnquery_onclick()
		for (i=1; i<= ds_1.CountRow; i++) {
			if ((ds_1.NameValue(i,"estimateyymm") == ls_date) && (ds_1.NameValue(i,"estimateseq") == ll_seq)) {
				ds_1.RowPosition = i
				makeCookie("@m_vender_est_list@","")
				return
			}
		}
		makeCookie("@m_vender_est_list@","")
	}
	
}
