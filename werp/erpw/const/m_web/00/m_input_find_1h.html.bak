//--------------------------------------------------------------------------
function   rd_2_rtn()
//----------------------------------------------------------------
{
	var ll_cnt,li_seq
	if (is_inouttypecode == '2'){
		alert('전출,폐기,매각 에서만 선택이 가능합니다...')
		return
	}
	ll_cnt = f_select_q("select nvl(count(*),0) select_1 " +
	    " from m_tmat_proj_rent where dept_code = " + "'" + is_dept_code+ "'" +
	    " and trunc(month,'MM') = trunc(to_date('" + id_date + "'),'MM')" +
	    " and DEGREE = 1 ") 

	li_seq = parseInt(ds_select.NameValue(ds_select.RowPosition,"select_1"))
	if (li_seq > 0 ){
		alert('이미 손료가 계산되어 가설재는 전출,폐기,매각 처리를 하실 수 없습니다...')
		return
	}

	bt_1.checked=false
	bt_2.checked=true
	bt_3.checked=false
	bt_4.checked=false
	bt_5.checked=false
	
	is_tag = '2'
	btnquery_onclick()
} 
