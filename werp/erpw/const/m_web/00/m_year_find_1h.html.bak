<SCRIPT language=JavaScript>
var gs_home
var is_ds_1_addr = 'm_year_find_1q.jsp?arg_date='                        //품의 목록 
var is_ds_2_addr = 'm_year_detail_find_1q.jsp?arg_year='                     //품의내역
var is_ds_seq_addr = gs_home + '/const/m_web/00/m_spec_unq_no_1q.jsp'                 // seq_no구하기 
var is_dept_code = '',is_sbcr_code,is_inouttypecode,is_ditag
var id_date
var ii_seq
var is_confirm_check = 'no'
var il_screen = 1
//---------------------------------------------------------------
function btnquery_onclick()               //조회 
//---------------------------------------------------------------
{
	if (il_screen > 1){
		div_btnquery_onclick()
		return
	}   
	ds_1.DataID = is_ds_1_addr + id_date + "&arg_sbcr_code=" +is_sbcr_code
	ds_1.SyncLoad=true
	ds_1.reset()
	if (ds_1.CountRow > 0) {
		dept_name.value = ds_1.NameValue(1,'sbcr_name')
	}
	div_btnquery_onclick()
}
//----------------------------------------------------------------
function Button_close_onclick()               //닫기
//----------------------------------------------------------------
{
 window.returnValue = ds_1.NameValue(ds_1.RowPosition,'approtitle') + '@' + ds_1.NameValue(ds_1.RowPosition,'paymentmethod') + '@';
 window.close();
}
//--------------------------------------------------------------------
function Button_select()               //전체선택
//--------------------------------------------------------------------
{
var i
	for(i=1; i <= ds_2.CountRow ; i++){
		ds_2.NameValue(i,"comp_chk") = 'T'
		ds_2.NameValue(i,'input_qty') = 1
	}
}
//--------------------------------------------------------------------
function Button_noselect()               //전체선택
//--------------------------------------------------------------------
{
var i
	for(i=1; i <= ds_2.CountRow ; i++){
		ds_2.NameValue(i,"comp_chk") = 'F'
		ds_2.NameValue(i,'input_qty') = 0
	}
}
//--------------------------------------------------------------------
function Button_confirm_onclick()               //복사 버튼
//--------------------------------------------------------------------
{
	is_confirm_check = 'yes'
	var ls_return="",li_seq,ll_unq_key
	var ls_year,ls_sbcr_code,ls_seq_num,ls_d_seq_num,ls_mtrcode,ls_name
	var ls_ssize,ls_unitcode,ls_rent_section,ls_input_section,ls_unitcost
	var ls_standard_amt,ls_loss_amt,ls_remark,ls_ditag,ls_input_qty,ls_amt
	
	for (i=1 ; i <= ds_2.CountRow ;i++){
		if (ds_2.NameValue(i,"comp_chk") == 'T'){
			ls_year           = ds_2.NameValue(i,'year')
			ls_sbcr_code      = ds_2.NameValue(i,'sbcr_code')    
			ls_seq_num        = ds_2.NameValue(i,'seq_num')      
			ls_d_seq_num      = ds_2.NameValue(i,'d_seq_num')    
			ls_mtrcode        = ds_2.NameValue(i,'mtrcode')      
			ls_name           = ds_2.NameValue(i,'name')         
			ls_ssize          = ds_2.NameValue(i,'ssize')        
			ls_unitcode       = ds_2.NameValue(i,'unitcode')     
			ls_rent_section   = ds_2.NameValue(i,'rent_section') 
			ls_input_section  = ds_2.NameValue(i,'input_section')
			ls_unitcost       = ds_2.NameValue(i,'unitcost')     
			ls_standard_amt   = ds_2.NameValue(i,'standard_amt') 
			ls_loss_amt       = ds_2.NameValue(i,'loss_amt')     
			ls_remark         = ds_2.NameValue(i,'remark')       
			ls_ditag          = ds_2.NameValue(i,'ditag')        
			ls_input_qty      = ds_2.NameValue(i,'input_qty')    
			
			ls_amt = parseInt(ls_input_qty * ls_unitcost,10);
			
			if (ls_rent_section == '1'){
				ls_ditag = '5'
			}
			
			ll_cnt = f_select_q("select nvl(max(detailseq),0) select_1 " +
				    " from m_input_detail where dept_code = " + "'" + is_dept_code + "'" +
				    " and yymmdd = " + "'" + id_date + "'" + 
				    " and seq = " + ii_seq )
	
			li_seq = parseInt(ds_select.NameValue(ds_select.RowPosition,"select_1")) + 1
			ll_cnt = f_select_q("select m_spec_unq_no.nextval select_1 from dual");
			ll_unq_key = parseInt(ds_select.NameValue(1,'select_1'))
			
		  arg_cmd = " INSERT INTO M_INPUT_DETAIL  " +
		            " ( DEPT_CODE,YYMMDD,SEQ,INPUT_UNQ_NUM,DETAILSEQ, " +
		            "   MTRCODE,NAME,SSIZE,UNITCODE,DITAG, " +
		            "   QTY,UNITPRICE,AMT,VATAMT,DELIVERYTAG,INOUTTYPECODE, " +
		            "   SBCR_CODE,VATTAG,VAT_UNQ_NUM,SPEC_NO_SEQ,SPEC_UNQ_NUM,REQUEST_UNQ_NUM,EST_UNQ_NUM,APPROVAL_UNQ_NUM, " +
					   "   BIGO,NOOUT_QTY,TMAT_UNQ_NUM,ITEM_CODE )  " +
		            " VALUES ( '" + is_dept_code + "','" + id_date + "'," + ii_seq + "," + ll_unq_key + "," + li_seq + "," +
		            "          '" + ls_mtrcode + "','" + ls_name + "','" + ls_ssize + "','" + ls_unitcode + "','" + ls_ditag + "'," +
		            "           " + ls_input_qty + "," + ls_unitcost + "," + ls_amt + ",0,0,'" + is_inouttypecode + "'," +
		            "          '" + is_sbcr_code + "','1',0,0,0,0,0,0," +
		            "          '" + ls_remark + "'," + ls_input_qty + ",0,'') " +
						"@ INSERT INTO M_INPUT_PRICE  " +
						"        ( DEPT_CODE,YYMMDD,SEQ,INPUT_UNQ_NUM, " +
						"          YEAR,SBCR_CODE,SEQ_NUM,D_SEQ_NUM, " +
						"          RENT_SECTION,INPUT_SECTION,UNITPRICE,STANDARD_AMT,   " +
						"			  LOSS_AMT,REMARK1,REMARK2 )  " +
		            " VALUES ( '" + is_dept_code + "','" + id_date + "'," + ii_seq + "," + ll_unq_key + "," + 
			         "          '" + ls_year + "','" + is_sbcr_code + "'," + ls_seq_num + "," + ls_d_seq_num + "," +
			         "          '" + ls_rent_section + "','" + ls_input_section + "'," + ls_unitcost + "," + ls_standard_amt + "," +
			         "           " + ls_loss_amt + ",'','" + ls_remark + "')"

			f_update_sql_multy(arg_cmd)
		}
                                                                 
	}          
	arg_cmd = "  update m_input  " +
				 "     set total_amt = ( select nvl(sum(amt),0) " +
				 "                         from m_input_detail " +
				 "                        where dept_code = '" + is_dept_code + "'" +
				 "                          and yymmdd = '" + id_date + "'" +
				 "                          and seq = " + ii_seq + ") " +
				 "   where dept_code = '" + is_dept_code + "'" +
				 "     and yymmdd = '" + id_date + "'" +
				 "     and seq = " + ii_seq

	f_update_sql(arg_cmd)

	ds_spcall.DataID = gs_home + '/const/m_web/03/y_m_input_cmpt_procedure.jsp?arg_dept_code=' + is_dept_code + '&arg_date=' + id_date + '&arg_seq=' + ii_seq
	ds_spcall.SyncLoad = true
	ds_spcall.reset()
	
	Button_close_onclick()
//	div_btnquery_onclick()

//	if (ds_2.CountRow < 1 ) btnquery_onclick()
}
//----------------------------------------------------------------------------
function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
	ds_2.Undo(ds_2.RowPosition)
}
//------------------------------------------------------------------------
function div_btnquery_onclick()                     // 하위 조회  
//------------------------------------------------------------------------
{
	if (ds_1.CountRow < 1) return

	ds_2.DataID = is_ds_2_addr  + ds_1.NameValue(ds_1.RowPosition,'year') + 
			"&arg_sbcr_code=" + ds_1.NameValue(ds_1.RowPosition,"sbcr_code")  +
			"&arg_seq_num=" + ds_1.NameValue(ds_1.RowPosition,"seq_num") +
			"&arg_dept_code=" + is_dept_code + "&arg_yymmdd=" + id_date + "&arg_seq=" + ii_seq
	ds_2.SyncLoad=true
	ds_2.Reset()
}

//------------------------------------------------------------------------
function get_focus(focus) // gd가 포커스를 받을때 동작.
//------------------------------------------------------------------------
{
	il_screen = focus
}

//----------------------------------------------------------------
function window_onload()                  // window open event 
//-----------------------------------------------------------------
{
	var result;
	var array = new Array();
	result = window.dialogArguments;
	array = result.split("@");
	gs_home          = array[0];     
	is_dept_code     = array[1];
	id_date          = array[2];
	ii_seq           = array[3];
	is_sbcr_code     = array[4];
	is_inouttypecode = array[5];

	btnquery_onclick()
}
//-----------------------------------------------------------------
function window_onunload()     // windoe.close()문장을 만나면 이루틴은 자동으로 수행됨으로 복사여부를 체크.
//-----------------------------------------------------------------
{
  if (is_confirm_check == 'no')
      window.returnValue = ""
}
 
</SCRIPT>
<script language=JavaScript for=gd_2 event=OnClick(row,colid)>
if (colid == "comp_chk") {
	if (ds_2.NameValue(row,colid) == 'T'){
		ds_2.NameValue(row,colid) = 'F'
		ds_2.NameValue(row,'input_qty') = 0
	}
	else {
		ds_2.NameValue(row,colid) = 'T'    
		ds_2.NameValue(row,'input_qty') = 1
	}
}
</SCRIPT>

<SCRIPT language=JavaScript for=ds_2 event=OnColumnChanged(row,colid)>
	var ll_vatamt,ll_cmp_vatamt,ls_tag
	
	ls_tag = ds_1.NameValue(ds_1.RowPosition,'price_class')
	if (colid == "input_qty") {
     	if (ds_2.NameValue(row,"input_qty") == 0 ) {
     		ds_2.NameValue(row,"tmp_vatamt") = 0
     		ds_2.NameValue(row,"comp_chk") = 'F'
     	}
     	else {
     		ds_2.NameValue(row,"tmp_vatamt") = parseInt(ds_2.NameValue(row,"input_qty") * ds_2.NameValue(row,"UNITPRICE") * 0.1);
     		ds_2.NameValue(row,"comp_chk") = 'T'
     	}
        		
	}   
	if (colid == "comp_chk") {
		if (ds_2.NameValue(row,"comp_chk") == 'T' ) {
			ds_2.NameValue(row,"input_qty") = 1
		}
		else {
			ds_2.NameValue(row,"input_qty") = 0
		}
	}
</SCRIPT>
