<SCRIPT language=JavaScript>
//------------------------------------------------------------------------
function btnquery_onclick()                     // 현장별조회
//------------------------------------------------------------------------
{
	is_from_dt = em_from_date.text.substr(0,4) + "." + em_from_date.text.substr(4,2)  + "." +  em_from_date.text.substr(6,2)
	is_to_dt = em_to_date.text.substr(0,4) + "." + em_to_date.text.substr(4,2)  + "." +  em_to_date.text.substr(6,2)

	if (ilIndex == 1){
		ds_1.DataID = is_ds_1_addr + is_mtrcode + '&arg_ssize=' + is_ssize + '&arg_dept=' + is_dept_code +
							'&arg_sbcr=' + is_sbcr_code +	'&arg_from_dt=' + is_from_dt +	
							'&arg_to_dt=' + is_to_dt
		ds_1.SyncLoad=true
		ds_1.Reset()
	}else{
		ds_2.DataID = is_ds_2_addr + is_mtrcode + '&arg_ssize=' + is_ssize + '&arg_dept=' + is_dept_code +
							'&arg_sbcr=' + is_sbcr_code +	'&arg_from_dt=' + is_from_dt +	
							'&arg_to_dt=' + is_to_dt
		ds_2.SyncLoad=true
		ds_2.Reset()
	}	
		
}
//----------------------------------------------------------------
function excel_down()                      //엑셀자료 내리기
//----------------------------------------------------------------
{
	if (ilIndex == 1){
		gd_1.SetExcelTitle(0, "");
		gd_1.SetExcelTitle(1, "value:품목별 입고검색; font-face:굴림체; font-size:20pt; font-color:black;font-bold; font-underline; bgcolor:white; align:center; line-color:black;line-width:1pt; skiprow:0;");
		gd_1.SetExcelTitle(1, "value:" + mtr_name.value+'/'+mtr_name.value+'/'+is_from_dt+'~'+is_to_dt+'/'+is_dept_name+'/'+sbcr_name.value + "; font-face:굴림체; font-size:12pt; font-color:black; font-underline; bgcolor:white; align:left; line-color:black;line-width:1pt; skiprow:0;");
   	gd_1.GridToExcel('현장별','c:\aa.xls',0)  
   }else{
		gd_2.SetExcelTitle(0, "");
		gd_2.SetExcelTitle(1, "value:품목별 입고검색; font-face:굴림체; font-size:20pt; font-color:black;font-bold; font-underline; bgcolor:white; align:center; line-color:black;line-width:1pt; skiprow:0;");
		gd_2.SetExcelTitle(1, "value:" + mtr_name.value+'/'+mtr_name.value+'/'+is_from_dt+'~'+is_to_dt+'/'+is_dept_name+'/'+sbcr_name.value + "; font-face:굴림체; font-size:12pt; font-color:black; font-underline; bgcolor:white; align:left; line-color:black;line-width:1pt; skiprow:0;");
   	gd_2.GridToExcel('업체별','c:\aa.xls',0)  
	}  
}

//---------------------------------------------------------------------------
function dept_code_find()                       //현장 찾기 
//---------------------------------------------------------------------------
{
	result = f_dept_code_find(dept_name.value)
	if(result == false){
		is_dept_code = ''
		dept_name.value = '전체현장'
		return
	}

	is_dept_code = top.frames['security'].gs_dept_code.value
	is_dept_name = top.frames['security'].gs_dept_name.value

	dept_name.value = is_dept_name
}   
//---------------------------------------------------------------------------
function mtrname_find()                       //자재코드 찾기 
//---------------------------------------------------------------------------
{
	ls_mtr_name=mtr_name.value
	result = window.showModalDialog(gs_home + "/const/m_web/00/m_code_material_find_1h.html",ls_mtr_name,"dialogWidth:550px;dialogHeight:510px");

	if(result == ""){
		is_mtrcode = ''
		mtr_name.value = '전체품목'
		return
	}

	array = result.split("@");
	is_mtrcode 			= array[0];   // 코드 
	mtr_name.value 	= array[1];  // 명칭
	
  	ll_cnt = f_ds_select_q("ds_ssize", " select ssize select_1 " +
												  "  from (select 1 tag, '전체' ssize from dual " +
      										  "        union all " +
      										  "        select distinct 2 tag, ssize from m_code_material " +
      										  "        where mtrcode = '" + is_mtrcode + "' " +
      										  "          and ssize is not null " +       
      										  " order by tag) ")

// 철근,레미콘은 그룹핑검색
	if (is_mtrcode.substr(0,8) == '00100110' || is_mtrcode.substr(0,8) == '00100510'){
		is_mtrcode = is_mtrcode.substr(0,8)
		if (is_mtrcode.substr(0,8) == '00100110')
			mtr_name.value = '철근류'
		else
			mtr_name.value = '레미콘류'
	}		
				
}   
//---------------------------------------------------------------------------
function sbcr_code_find()                       //업체코드 찾기 
//---------------------------------------------------------------------------
{
	ls_sbcr_name=sbcr_name.value
	result = window.showModalDialog(gs_home + "/const/m_web/00/m_vender_find_2h.html",ls_sbcr_name,"dialogWidth:700px;dialogHeight:510px");

	if(result == ""){
		is_sbcr_code = ''
		sbcr_name.value = '전체품목'
		return
	}

	array = result.split("@");
	is_sbcr_code 		= array[0];   // 코드 
	sbcr_name.value 	= array[1];  // 명칭

}   
//------------------------------------------------------------------------
function keyDown()                       //검색어 입력후 enter를 쳤을경우
//------------------------------------------------------------------------
{
	if (window.event.keyCode == 13) { 
		dept_code_find();
	} 
}
//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
	top.frames['main_title'].mouse_over()

	dept_name.value 	= is_dept_name
	mtr_name.value 	= '전체품목'
	sbcr_name.value 	= '전체업체'

	em_from_date.text = is_work_date.substr(0,7) + '.01'
	is_from_dt = em_from_date.text.substr(0,4) + "." + em_from_date.text.substr(4,2)  + "." +  em_from_date.text.substr(6,2)
 	ll_cnt = f_ds_select_q("ds_date", " select to_char(last_day('" + is_work_date + "'),'yyyy.mm.dd') select_1 from dual") //마직막날짜
	em_to_date.text = ds_date.NameValue(1,"select_1")
	is_to_dt = em_to_date.text.substr(0,4) + "." + em_to_date.text.substr(4,2)  + "." +  em_to_date.text.substr(6,2)

// 규격콤보
  	ll_cnt = f_ds_select_q("ds_ssize", " select ssize select_1 " +
												  "  from (select 1 tag, '전체' ssize from dual " +
      										  "        union all " +
      										  "        select distinct 2 tag, ssize from m_code_material " +
      										  "        where ssize is not null " +       
      										  " order by tag) ")
  	ccom_ssize.Index = 0
   is_ssize = ''

}
