//----------------------------------------------------------------------
function init_move()                         // initial move
//----------------------------------------------------------------------
{
var temp = 0 ;


	for( i = 1 ; i <= ds_1.CountRow  ; i++ )	
	{
		if ( parseInt(ds_1.NameValue(i,"seq_num")) > parseInt(temp) )
	   	temp = ds_1.NameValue(i,"seq_num") 
	}

   ds_1.NameValue(ds_1.RowPosition,"year") = em_year.text + '.01.01'
   ds_1.NameValue(ds_1.RowPosition,"seq_num") = parseInt(temp) + 1   
 
  

}
//----------------------------------------------------------------------
function init_move2()                         // initial move
//----------------------------------------------------------------------
{
var temp = 0 ;


	for( i = 1 ; i <= ds_2.CountRow  ; i++ )	
	{
		if ( parseInt(ds_2.NameValue(i,"d_seq_num")) > parseInt(temp) )
	   	temp = ds_2.NameValue(i,"d_seq_num") 
	}

   ds_2.NameValue(ds_2.RowPosition,"year") = em_year.text + '.01.01'
   ds_2.NameValue(ds_2.RowPosition,"sbcr_code") = ds_1.NameValue(ds_1.RowPosition,"sbcr_code")
   ds_2.NameValue(ds_2.RowPosition,"seq_num") = ds_1.NameValue(ds_1.RowPosition,"seq_num")
   ds_2.NameValue(ds_2.RowPosition,"d_seq_num") = parseInt(temp) + 1   


}
