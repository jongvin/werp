//---------------------------------------------------------------------
function bt_vender_find()               //견적업체 찾기
//---------------------------------------------------------------------
{
	var result;
	var array = new Array();
	var varray = new Array();
	var i,j
	
	if (ds_1.CountRow > 0 ) {
	   	for (i=1; i<= ds_1.CountRow; i++){
			varray = varray + ds_1.NameValue(i,"sbcr_code") + "@"
		}
	}

	result = window.showModalDialog(gs_home + "/const/m_web/03/m_vender_find_1h.html", varray,"resizable:no;scroll:no;status:no;help:no;dialogWidth:700px;dialogHeight:510px");
	if(result == "") 
	{
		ds_1.Undo(ds_1.RowPosition) ;
		return;
	}
	array = result.split("@");
   ds_1.NameValue(ds_1.RowPosition,"sbcr_code") = array[0]
   ds_1.NameValue(ds_1.RowPosition,"sbcr_name") = array[1]
//	ds_1.NameValue(ds_1.RowPosition,'profession_wbs_code') = array[3]
}

//------------------------------------------------------------------------
function material_search()                       //자재 코드 찾기
//------------------------------------------------------------------------
{
	var result , ll_cnt;
	var array = new Array();
	var varray2 = ''
  
	if (ds_2.CountRow > 0 ) {
	   	for (i=1; i<= ds_2.CountRow; i++)
	   	{
				varray2 = varray2 + "@" + ds_2.NameValue(i,"mtrcode")  
			}
	}
	
   row = ds_2.RowPosition
   if (row > 0)
   {
   	ls_arg = ds_2.NameValue(row-1,'name')
   }
   else
   {
     ls_arg = ""
   }
   
   ls_arg += '^' + varray2 +'^'  
   
	result = window.showModalDialog(gs_home + "/const/m_web/03/m_code_material_find_1h.html",ls_arg,"resizable:no;scroll:no;status:no;help:no;dialogWidth:580px;dialogHeight:510px");

	if(result == "") {
//      ds_2.DeleteRow(row)
		return;
	}
	
	array = result.split("@");
	
	ll_cnt = parseInt(array.length) / 4 ;
	
	for( i =0 ; i <=  parseInt(ll_cnt) -1 ; i++ ) 
	{
	   var ll_value = i * 4
      var temp = 0;
  		ds_2.addrow()

		for( j = 1 ; j <= ds_2.CountRow  ; j++ )	
		{
			if ( parseInt(ds_2.NameValue(j,"d_seq_num")) > parseInt(temp) )
		   	temp = ds_2.NameValue(j,"d_seq_num") 
		}
	
	   ds_2.NameValue(ds_2.RowPosition,"year") = em_year.text + '.01.01'
	   ds_2.NameValue(ds_2.RowPosition,"sbcr_code") = ds_1.NameValue(ds_1.RowPosition,"sbcr_code")
	   ds_2.NameValue(ds_2.RowPosition,"seq_num") = ds_1.NameValue(ds_1.RowPosition,"seq_num")
	   ds_2.NameValue(ds_2.RowPosition,"d_seq_num") = parseInt(temp) + 1   
	   ds_2.NameValue(ds_2.RowPosition,"rent_section") = 1   
	   ds_2.NameValue(ds_2.RowPosition,"input_section") = 1   

		ds_2.NameString(ds_2.RowPosition,"mtrcode") = array[ll_value+0];   // 코드 
		ds_2.NameString(ds_2.RowPosition,"name") = array[ll_value+1];  // 명칭
		ds_2.NameString(ds_2.RowPosition,"ssize") = array[ll_value+2];     // 규격
		ds_2.NameString(ds_2.RowPosition,"unitcode") = array[ll_value+3];     // 단위
		
	}
}

