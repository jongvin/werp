//------------------------------------------------------------------------
function btnquery_prt_onclick()                     // 미리보기
//------------------------------------------------------------------------
{
   if (  ds_2.CountRow < 1  ) { alert("출력할 자료가 없습니다.")  ; return ; }
   var arg_temp
   arg_temp ="const_rpt/m_web/03r/"      // 보고서 디렉토리
   report = 'm_price_contract'
      
   arg_temp = arg_temp + report   // 보고서명
   
   arg_temp = arg_temp + "&RptParams=" + em_year.text + '.01.01'
   arg_temp = arg_temp + "&RptParams=" + ds_1.NameValue(ds_1.RowPosition,'sbcr_code')

   f_crystal_report(arg_temp)   //보고서 호출
}

//------------------------------------------------------------------------
function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{
 

	ds_1.DataID = is_ds_1_addr +  em_year.text+'.01.01'
//alert(ds_1.dataid) ;
 	ds_1.SyncLoad=true
 	ds_1.Reset()

  if (ds_1.CountRow == 0 )
		ds_2.ClearData();

}

//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
btnsave_onclick()
 
   if ( il_screen	== 1 ) 
 	{
 		ds_1.AddRow()   
		init_move()
		bt_vender_find() 
   }
   else if ( il_screen == 2 )
   {
		material_search()
   }
}
//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
btnsave_onclick()

   if ( il_screen	== 1 ) 
 	{
		if (ds_1.CountRow == 0)				 // 한건도 없을경우는  0 
		{                                       
			btnadd_onclick()   
			return
		}  
		ds_1.InsertRow(ds_1.RowPosition)  
		init_move() 
		bt_vender_find() 
   
   }
   else if ( il_screen == 2 )
   {
		if (ds_2.CountRow == 0)				 // 한건도 없을경우는  0 
		{                                       
			btnadd_onclick()   
			return
		}  
		material_search()

   }
   
}
//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{

   if ( il_screen	== 1 ) 
 	{
	 	if ( ds_2.CountRow > 0 ) 
	 	{
	 		alert("하위 자료가 남아있습니다.")
	 	   return ;
	 	}   
	 	
 	
	  if  (gd_1.MultiRowSelect)
	  {                                   // multi row select 일 경우
	     for (i=ds_1.CountRow ;i >0 ;i--)                          // 로우의 제일끝에서부터 앞으로 루핑
	     {
	        if (ds_1.RowMark(i) == 1)                              // 선택되어 있으며는 
	               ds_1.DeleteRow(i)
	    }   
	    ds_1.MarkRows(ds_1.RowPosition,ds_1.RowPosition)           // 삭제작업완료후 MARK(멀티로우 때문에 한번 마킹해준다)
	  }
	  else
	 	ds_1.DeleteRow(ds_1.RowPosition)  

   }
   else if ( il_screen == 2 )
   {
	  if  (gd_2.MultiRowSelect)
	  {                                   // multi row select 일 경우
	     for (i=ds_2.CountRow ;i >0 ;i--)                          // 로우의 제일끝에서부터 앞으로 루핑
	     {
	        if (ds_2.RowMark(i) == 1)                              // 선택되어 있으며는 
	               ds_2.DeleteRow(i)
	    }   
	    ds_2.MarkRows(ds_2.RowPosition,ds_2.RowPosition)           // 삭제작업완료후 MARK(멀티로우 때문에 한번 마킹해준다)
	  }
	  else
	 	ds_2.DeleteRow(ds_2.RowPosition)  
   }

 }
//------------------------------------------------------------------------
 function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{

   if ( il_screen	== 1 ) 
 	{
    ds_1.Undo(ds_1.RowPosition)
   }
   else if ( il_screen == 2 )
   {
    ds_2.Undo(ds_2.RowPosition)
   }

