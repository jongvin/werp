//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
    
  if (ds_1.IsUpdated || ds_2.IsUpdated )											 //변경된것이 있으며는 
	
  {                                          
    if  (confirm("변경되었습니다 저장하시겠습니까?"))
    {

			for ( i=1 ; i <= ds_1.CountRow ; i++ )
			{
				if( (f_check_date(ds_1.NameValue(ds_1.RowPosition,"from_dt")) == false ) && (ds_1.NameValue(ds_1.RowPosition,"from_dt") > ''))
				{
					alert("유효하지 않은 날짜가 입력되었습니다.\n수정하십시요 ")
					ds_1.RowPosition = i
					gd_1.SetColumn("from_dt");
					return false ; 
				}
			}
			
			for ( i=1 ; i <= ds_1.CountRow ; i++ )
			{
				if( (f_check_date(ds_1.NameValue(ds_1.RowPosition,"to_dt")) == false ) && (ds_1.NameValue(ds_1.RowPosition,"to_dt") > ''))
				{
					alert("유효하지 않은 날짜가 입력되었습니다.\n수정하십시요 ")
					ds_1.RowPosition = i
					gd_1.SetColumn("to_dt");
					return false ; 
				}
			}
			
		ll_row =	f_dup_chk('ds_1','sbcr_code')
		if ( ll_row != 0 )
		{
			ds_1.RowPosition = ll_row
			gd_1.SetColumn("sbcr_code");		
			return false ; 

		}	
	
		ll_row =	f_dup_chk('ds_2','mtrcode')
		if ( ll_row != 0 )
		{
			ds_1.RowPosition = ll_row
			gd_1.SetColumn("mtrcode");		
			return false ; 

		}	
	
        tr_1.Post()                                          
        tr_2.Post()
        
        return true ;
    }   
    else
        return false ;    	 
 } 

}

