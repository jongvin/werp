<SCRIPT language=JavaScript>

var gs_home = top.frames['security'].gs_home.value
var is_class_tag = ""
var il_focus = 1
var is_ds_1_addr = gs_home + '/const/r_web/99/r_code_lice_parent_1q.jsp'
var is_ds_2_addr = gs_home + '/const/r_web/99/r_code_lice_child_1q.jsp?arg_class_tag='
var is_tr_1_addr = gs_home + '/const/r_web/99/r_code_lice_parent_1tr.jsp'
var is_tr_2_addr = gs_home + '/const/r_web/99/r_code_lice_child_1tr.jsp'
//------------------------------------------------------------------------
function insert_init()                 //하위 입력시 초기 셋팅.
//------------------------------------------------------------------------
{
  ds_2.NameValue(ds_2.RowPosition,"license_class_code") = is_class_tag
}
//------------------------------------------------------------------------
function btnquery_onclick()                     // 조회  
//------------------------------------------------------------------------
{
	 if (il_focus == 1) { 
	   ds_1.SyncLoad=true
	   ds_1.Reset()
	 }   
	 else {
		is_class_tag = ds_1.NameString(ds_1.RowPosition,"license_class_code");
		ds_2.DataID=is_ds_2_addr +  is_class_tag;
	   ds_2.SyncLoad=true
	   ds_2.Reset()
	 }
}
//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
	if  (il_focus == 1) 
	   ds_1.AddRow()
	else
	   div_btnadd_onclick()  

}
//------------------------------------------------------------------------
function div_btnadd_onclick()                  // 하위 추가
//------------------------------------------------------------------------
{
    if (is_class_tag != ""){
       ds_2.AddRow()
       insert_init()
    }
 }
//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
  if  (il_focus == 1) 
      ds_1.InsertRow(ds_1.RowPosition);
  else
    if (is_class_tag != "")  {
      ds_2.InsertRow(ds_1.RowPosition);
      insert_init()    
  }    
}

//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
  if  (il_focus == 1) {
  	  if (ds_2.CountRow > 0 ){
  	  		alert('하위의 자료부터 삭제하십시오...')
  	  		return
  	  	}
     if  (gd_1.MultiRowSelect){               // multi row select 일 경우
        for (i=ds_1.CountRow ;i >0 ;i--){
           if (ds_1.RowMark(i) == 1)
                  ds_1.DeleteRow(i)
         } 
      if  (ds_1.RowPosition != 0)     
       ds_1.MarkRows(ds_1.RowPosition,ds_1.RowPosition)  //삭제작업완료후 MARK
      }    
    else
       ds_1.DeleteRow(ds_1.RowPosition)
  }     
  else
      div_btndelete_onclick()        
} 
//------------------------------------------------------------------------     
function div_btndelete_onclick()           //하위 삭제
//------------------------------------------------------------------------
{
  if  (gd_2.MultiRowSelect){               // multi row select 일 경우
     for (i=ds_2.CountRow ;i >0 ;i--){
        if (ds_2.RowMark(i) == 1)
               ds_2.DeleteRow(i)
      }  
   if  (ds_2.RowPosition != 0)          
       ds_2.MarkRows(ds_2.RowPosition,ds_2.RowPosition)  //삭제작업완료후 MARK
    }
  else
     ds_2.DeleteRow(ds_2.RowPosition)  
 }
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
   	
  if (ds_1.IsUpdated){
	 var ll_dup
	 ll_dup = f_dup_chk("ds_1","license_class_code")
	 if (ll_dup > 0) {
	     alert("코드가 중복되었습니다: 코드를 수정하십시요 ")
	     ds_1.RowPosition = ll_dup
     	  gd_1.SetColumn("license_class_code");
     	  il_focus = 1
	     return
	 }
  }
  if (ds_2.IsUpdated){
	 var ll_dup
	 ll_dup = f_dup_chk("ds_2","license_kind_code")
	 if (ll_dup > 0) {
	     alert("코드가 중복되었습니다: 코드를 수정하십시요 ")
	     ds_2.RowPosition = ll_dup
     	  gd_2.SetColumn("license_kind_code");
     	  il_focus = 2
	     return
	 }
  }


 is_class_tag = ds_1.NameValue(ds_1.RowPosition,"license_class_code");
 if (il_focus == 1) {
   if (ds_1.IsUpdated){
      if  (confirm("변경되었습니다 저장하시겠습니까?")){
			if (ds_2.IsUpdated){
				tr_2.Post()
				ds_2.DataID=is_ds_2_addr +  is_class_tag;
            ds_2.SyncLoad=true
				ds_2.reset()
			}	
          tr_1.Post()
      }
   }       
 }
 else
    div_btnsave_onclick()  
}
//---------------------------------------------------------------------------
function div_btnsave_onclick()                  //하위 저장
//--------------------------------------------------------------------------- 
{
	if (ds_2.IsUpdated){
		if  (confirm("변경되었습니다 저장하시겠습니까?")){
			if (ds_1.IsUpdated){
				tr_1.Post()
			}	
			tr_2.Post()
			is_class_tag = ds_1.NameString(ds_1.RowPosition,"license_class_code");
			ds_2.DataID=is_ds_2_addr +  is_class_tag;
         ds_2.SyncLoad=true
			ds_2.reset()
		}
	}         
}

//----------------------------------------------------------------------------
function btncancel_onclick()                     // 취소
//---------------------------------------------------------------------------- 
{
 if (il_focus == 1) 
    ds_1.Undo(ds_1.RowPosition);
 else
    ds_2.Undo(ds_2.RowPosition);    
}
//----------------------------------------------------------------------------
function btnexit_onclick(ls_arg)                 // 종료 
//-----------------------------------------------------------------------------
{
	  if (ds_1.IsUpdated) {
     if  (confirm("변경되었습니다 저장하시겠습니까?")) 
        {  //저장 routine
        }   
 
       }     
top.frames['main_ct'].location.href(ls_arg)
top.frames['main_title'].div_r.style.visibility="hidden"             
top.frames['main_title'].div_u.style.visibility="hidden"             

// top.frames['main_ct'].window.close()     
}


//---------------------------------------------------------------------------
function window_onload()                      // window open event
//---------------------------------------------------------------------------
{
  if (f_reject_window()) return 
   top.frames['main_title'].mouse_over()

	ds_1.DataID=is_ds_1_addr
	ds_1.SyncLoad=true
	ds_1.reset()
	ds_2.DataID=is_ds_2_addr +  is_class_tag
	
	tr_1.Action = is_tr_1_addr
	tr_2.Action = is_tr_2_addr
}
</script>


<script language=JavaScript for=gd_1 event=OnKeyPress(kcode)>
if (kcode == 13) {
    if (ds_1.RowPosition == ds_1.CountRow)  
        btnadd_onclick()
    else     
        ds_1.RowPosition++
}
</script>
<script language=JavaScript for=gd_2 event=OnKeyPress(kcode)>
if (kcode == 13) {
    if (ds_2.RowPosition == ds_2.CountRow)  
        div_btnadd_onclick()
    else     
        ds_2.RowPosition++
}
</script>
<SCRIPT language=JavaScript event=OnRowPosChanged(row) for=ds_1>
	if (row < 1)  return

	if (ds_2.IsUpdated){
		div_btnsave_onclick()
	}	
	
	is_class_tag = ds_1.NameString(row,"license_class_code");
	ds_2.DataID=is_ds_2_addr +  is_class_tag;
	ds_2.SyncLoad=true
	ds_2.reset();
</SCRIPT>
<SCRIPT language=JavaScript for=gd_1 event=CanColumnPosChange(row,colid)>
	if (colid == "license_class_code")
	   is_class_tag = ds_1.NameString(row,"license_class_code");
</SCRIPT>
<SCRIPT language=JavaScript for=gd_1 event=OnClick(row,colid)>
	il_focus = 1
</SCRIPT>
<SCRIPT language=JavaScript for=gd_2 event=OnClick(row,colid)>
	il_focus = 2
</SCRIPT>

