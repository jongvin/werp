//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
	var ls_chk=0,ls_spec_chk=0
	
	if (ds_2.IsUpdated || ds_3.IsUpdated || ds_4.IsUpdated){
		if  (confirm("변경되었습니다 저장하시겠습니까?")){
			if (ds_4.IsUpdated ) {
		   	for (i=1; i<= ds_4.CountRow; i++){
			   	if (ds_4.NameValue(i,"pq_code") == ''){
		   			ls_chk++
	   			}
	   		}
	   		if (ls_chk == 0) {
		       		tr_3.Post()
	       		}
       		else {
		   		alert("PQ코드를 반드시 입력하십시오 \n        저장되지않았습니다.")
	      	}
			}
			if (ds_3.IsUpdated ) {
		   	for (i=1; i<= ds_3.CountRow; i++){
			   	if (ds_3.NameValue(i,"membership_no") == ''){
		   			ls_chk++
	   			}
	   		}
	   		if (ls_chk == 0) {
		       		tr_2.Post()
	       		}
       		else {
		   		alert("회원사코드를 반드시 입력하십시오 \n        저장되지않았습니다.")
	      	}
			}
			if (ds_2.IsUpdated ){
				tr_1.Post()
			}
		}
	}
	return
}
