//-----------------------------------------------------------------------
function btnadd_onclick()                       // 추가 
//------------------------------------------------------------------------
{
	var ls_temp,ls_degree,ls_row

	if ( tb_1.ActiveIndex == 3 && il_screen != 2) {
		ds_3.InsertRow(1)
		ds_3.NameValue(ds_3.RowPosition,"dept_code")  = ds_2.NameValue(ds_2.RowPosition,"dept_code")
		ds_3.NameValue(ds_3.RowPosition,"cont_no")    = ds_2.NameValue(ds_2.RowPosition,"cont_no")
		ds_3.NameValue(ds_3.RowPosition,"chg_degree") = ds_2.NameValue(ds_2.RowPosition,"chg_degree")
		ds_3.NameValue(ds_3.RowPosition,"operation_tag") = 'N'
		return
	}

	if ( tb_1.ActiveIndex == 4 && il_screen != 2) {
		ds_4.InsertRow(1)
		ds_4.NameValue(ds_4.RowPosition,"dept_code")  = ds_2.NameValue(ds_2.RowPosition,"dept_code")
		ds_4.NameValue(ds_4.RowPosition,"cont_no")    = ds_2.NameValue(ds_2.RowPosition,"cont_no")
		ds_4.NameValue(ds_4.RowPosition,"chg_degree") = ds_2.NameValue(ds_2.RowPosition,"chg_degree")
		return
	}

	if (ds_2.CountRow < 1) {
		alert("1차 계약사항이 없어서 추가할수 없습니다")
		return
	}   
	ls_degree = ds_2.NameValue(1,'chg_degree')
	ll_degree = parseInt(ls_degree) + 1

	ds_2.NameValue(1,'last_tag') = 'N'

	ds_2.InsertRow(1)

	for (col=1; col <= ds_2.CountColumn; col++) 
		ds_2.ColumnValue(1,col) = ds_2.OrgValue(2,col);
	
	ds_2.NameValue(1,"chg_degree") = ll_degree
	ds_2.NameValue(1,'last_tag') = 'Y'

	ls_dept_code   = ds_2.NameValue(2,"dept_code")
	ls_cont_no     = ds_2.NameValue(2,"cont_no")
	ls_from_degree = ds_2.NameValue(2,"chg_degree")
	
	arg_cmd = " INSERT INTO R_CONTRACT_SUCCESFUL_BID  " +
	  			 " SELECT DEPT_CODE,CONT_NO," +  ll_degree + ",MEMBERSHIP_NO,CUSTOMER_NO,   " +
				 " COMPANY_TAG,OPERATION_TAG,DIVISION_RATE,DIVISION_AMT,   " +
				 " OPERATION_RATE,OPERATION_AMT,CHARGER,TEL,REMARK  " +
		       " FROM R_CONTRACT_SUCCESFUL_BID  " +
		       " WHERE ( DEPT_CODE = " + "'" + ls_dept_code + "'" + " ) AND  " +
				 " ( CONT_NO = " + ls_cont_no + "  ) AND  " +
				 " ( CHG_DEGREE = " + ls_from_degree + " )   "

	f_update_sql(arg_cmd)

	arg_cmd = " INSERT INTO R_TENDER_PQ  " +
	  			 " SELECT DEPT_CODE,CONT_NO," +  ll_degree + ",PQ_CODE,PQ_AMT,   " +
				 " ESTIMATION_STANDARD,IDENTITY_STANDARD  " +
		       " FROM R_TENDER_PQ  " +
		       " WHERE ( DEPT_CODE = " + "'" + ls_dept_code + "'" + " ) AND  " +
				 " ( CONT_NO = " + ls_cont_no + "  ) AND  " +
				 " ( CHG_DEGREE = " + ls_from_degree + " )   "

	f_update_sql(arg_cmd)

	tr_1.Post()
	ds_3.DataID = is_ds_3_addr + ls_dept_code + "&arg_cont_no=" + ls_cont_no + "&arg_chg_degree=" + ll_degree
	ds_3.SyncLoad=true
	ds_3.Reset()
	ds_4.DataID = is_ds_4_addr + ls_dept_code + "&arg_cont_no=" + ls_cont_no + "&arg_chg_degree=" + ll_degree
	ds_4.SyncLoad=true
	ds_4.Reset()
}
