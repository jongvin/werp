//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
	var ls_last_tag,ls_chg_degree,i
	if ( tb_1.ActiveIndex == 3 && il_screen != 2) {
		ds_3.DeleteRow(ds_3.RowPosition)  
		return
	}	
	if ( tb_1.ActiveIndex == 4 && il_screen != 2) {
		ds_4.DeleteRow(ds_4.RowPosition)  
		return
	}	
	ls_last_tag = ds_2.NameValue(ds_2.RowPosition,'last_tag')
	ls_chg_degree = ds_2.NameValue(ds_2.RowPosition,'chg_degree')
	
	if (ls_last_tag == 'N') {
		alert('확인 마지막 차수에서만 삭제가 가능합니다.')
		return 
	}
	if (ls_chg_degree > 1){
		ds_2.NameValue(2,'last_tag') = 'Y'
	}
	else {
		if  (confirm("계약대장이 삭제됩니다. 정말로 삭제하시겠습니까?")){
		}
		else {
			return
		}
	}

	var ls_dept_code,ls_cont_no

	ls_dept_code = ds_2.NameValue(ds_2.RowPosition,"dept_code")
	ls_cont_no   = ds_2.NameValue(ds_2.RowPosition,"cont_no")
	
	ll_cnt = f_select_q("select extablish_time select_1  from r_contract_time_extablished " + 
	                    " where dept_code = " + "'" + ls_dept_code + "'" + 
	                    " and cont_no = " + ls_cont_no + 
	                    " and chg_degree = " + ls_chg_degree ) 

	if (ll_cnt > 0) {
		alert("해당 차수의 기성청구내역이 존재함으로 삭제할 수 없습니다. \n      기성청구내역부터 삭제하십시오..")
		return
	}

	for (i=ds_3.CountRow; i >= 1; i--) 
	{
		ds_3.DeleteRow(i)  ;
	}	
	for (i=ds_4.CountRow; i >= 1; i--) 
	{
		ds_4.DeleteRow(i)  ;
	}	

	tr_2.Post()
	tr_3.Post()
	ds_2.DeleteRow(ds_2.RowPosition)  
	tr_1.Post()
	
}
