//---------------------------------------------------------------------------
{
	var ls_temp,ls_degree,ls_row,ls_cont_no,ls_dept_code,ls_new_cont_no

	ls_temp = ds_2.NameValue(ds_2.RowPosition,"contract_year_tag")
	if (ls_temp == 'N' ) {
		alert("년차공사가 아니므로 추가할 수 없습니다...")
		return
	}
	
	btnsave_onclick()
	
