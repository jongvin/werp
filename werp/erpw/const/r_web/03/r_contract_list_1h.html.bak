	ls_dept_code = ds_1.NameValue(ds_1.RowPosition,"dept_code")

	ll_cnt = f_select_q("select max(cont_no) select_1  from r_contract_register " + 
	                    " where dept_code = " + "'" + ls_dept_code + "'" ) 

