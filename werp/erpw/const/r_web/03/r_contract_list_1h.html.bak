             "			 ( CONT_NO = " + ls_cont_no + " ) AND  " +
             "			 ( CHG_DEGREE = " + ls_degree + " )   "

	f_update_sql(arg_cmd)

	arg_cmd = " INSERT INTO R_CONTRACT_SUCCESFUL_BID  " +
	  			 " SELECT DEPT_CODE," + ls_new_cont_no + ",1,MEMBERSHIP_NO,CUSTOMER_NO,   " +
