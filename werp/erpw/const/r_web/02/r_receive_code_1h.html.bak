//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
	if (gd_1.MultiRowSelect){               // multi row select 일 경우
		for (i=ds_1.CountRow ;i >0 ;i--)
		{
			if (ds_1.RowMark(i) == 1)
				if ( ds_1.NameValue(i,"approve_class") == '1') {
					ds_1.DeleteRow(i)
				}
		}   
		ds_1.MarkRows(ds_1.RowPosition,ds_1.RowPosition)  //삭제작업완료후 MARK
	} 
	else {
		if (ds_1.NameValue(ds_1.RowPosition,"approve_class") == '1') { 
			ds_1.DeleteRow(ds_1.RowPosition)
		}
		else {
			alert("작성중이 아니므로 삭제할 수 없습니다.")
		}
	}
}
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
	sa_position = ds_1.RowPosition
	if (ds_1.IsUpdated){
		if (tx_name.value.length == 0 || tx_name.value == "") {
			alert("공사명을 입력하십시요")
			tx_name.focus()
			return
		}
		if (tx_const_shortname.value.length == 0 || tx_const_shortname.value == "") {
			alert("공사명(단명)을 입력하십시요")
			tx_const_shortname.focus()
			return
		}
		var ll_dup
		ll_dup = f_dup_chk("ds_1","receive_code")
		if (ll_dup > 0) {
			alert("수주코드가 중복되었습니다: 수주코드를 수정하십시요")
			ds_1.RowPosition = ll_dup
			gd_1.SetColumn("receive_code");
			return
		}
	}
	
	if (ds_1.IsUpdated){
		if  (confirm("변경되었습니다 저장하시겠습니까?")) {
			//alert(ds_1.RowStatus(ds_1.RowPosition));
         
			tr_1.Post()
			//btnquery_onclick()
			//ds_1.RowPosition = sa_position
		}    
	} 
}
