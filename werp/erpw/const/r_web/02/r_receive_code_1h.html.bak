//-----------------------------------------------------------------------
function Fn_SetColumn()   // 커서위치 세팅
//-----------------------------------------------------------------------
{
	gd_1.Focus();
	gd_1.SetColumn("name");
}
//-----------------------------------------------------------------------
function init_move()               // 초기값세팅
//-----------------------------------------------------------------------
{
	var ll_cnt,ls_code,ll_seq,ls_tmp_code
	sa_position = ds_1.RowPosition
	ls_tmp_code = 'P' + is_work_date + '1';
	ll_cnt = f_select_q("select to_number(substrb(max(receive_code),7,3)) select_1 from r_receive_code where substrb(receive_code,1,6) = '" + ls_tmp_code + "'")
	if (ds_select.ErrorCode != 0) {alert("에러발행");return;}
	if (ll_cnt > 0 && ds_select.NameValue(ds_select.RowPosition,'select_1') != '') {
		ll_seq = parseInt(ds_select.NameValue(ds_select.RowPosition,'select_1')) + 1;
		if ( ll_seq < 10) {
			ls_code = ls_tmp_code + '00' + ll_seq;
		}
		else {
			if (ll_seq < 100) {
				ls_code = ls_tmp_code + '0' + ll_seq;
			}
			else {
				if (ll_seq < 1000) {
					ls_code = ls_tmp_code + ll_seq;
				}
				else {
					alert('수주코드가 한도를 초과했습니다')
					return
				}
			}
		}
	}	
	else {
		ls_code = ls_tmp_code + '001' ;
	}

	ds_1.NameValue(ds_1.RowPosition,"receive_code") = ls_code
	ds_1.NameValue(ds_1.RowPosition,"constkind_tag")  = '01'
	ds_1.NameValue(ds_1.RowPosition,"pq_tag") = 'A'
	ds_1.NameValue(ds_1.RowPosition,"join_cont_tag") = '1'
	ds_1.NameValue(ds_1.RowPosition,"receive_tag") = '1'
	ds_1.NameValue(ds_1.RowPosition,"receive_process_class") = '01'
	ds_1.NameValue(ds_1.RowPosition,"tender_tag") = '1'
	ds_1.NameValue(ds_1.RowPosition,"order_tag") = '1'
	ds_1.NameValue(ds_1.RowPosition,"approve_class") = '1'
	ds_1.NameValue(ds_1.RowPosition,"entry_tag") = 'N'
	ds_1.NameValue(ds_1.RowPosition,"siteexplan_tag") = 'N'
	ds_1.NameValue(ds_1.RowPosition,"regist_tag") = 'N'
	ds_1.NameValue(ds_1.RowPosition,"masterdept_tag") = '1'

	gd_1.redraw=false;
	tr_1.Post()
	//ds_1.ResetStatus();
	btnquery_onclick();
	ds_1.RowPosition =sa_position;
	gd_1.redraw=true;
	Fn_SetColumn()

}
