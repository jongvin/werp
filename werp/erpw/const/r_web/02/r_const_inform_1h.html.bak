//-----------------------------------------------------------------------
function btninsert_onclick()                    // 삽입
//-----------------------------------------------------------------------
{
	if (ds_1.CountRow > 0) {
		ds_1.InsertRow(1)
	}
	else {
		ds_1.AddRow()
	}
	init_move()
}

//------------------------------------------------------------------------
function btndelete_onclick()                    // 삭제 
//------------------------------------------------------------------------
{
	if  (gd_1.MultiRowSelect){               // multi row select 일 경우
	     for (i=ds_1.CountRow ;i >0 ;i--)
	     {
	        if (ds_1.RowMark(i) == 1)
	        	if ( ds_1.NameValue(i,"receive_process_class") == '01') {
	               		ds_1.DeleteRow(i)
	               	}
	     }   
	    ds_1.MarkRows(ds_1.RowPosition,ds_1.RowPosition)  //삭제작업완료후 MARK
	} 
	else {
		if (ds_1.NameValue(ds_1.RowPosition,"receive_process_class") == '01') {
	     		ds_1.DeleteRow(ds_1.RowPosition)  
	     	}
	     	else {
	     		alert("정보등록상태가 아니므로 삭제할 수 없습니다.")
	     	}
	}
}
//---------------------------------------------------------------------------
function btnsave_onclick()                       // 저장
//---------------------------------------------------------------------------
{
   	sa_position = ds_1.RowPosition

	if (ds_1.IsUpdated){
		if (em_const_from.text > em_const_to.text) {
			alert("공사기간이 올바르지 않습니다")
			return
		}
	    if  (confirm("변경되었습니다 저장하시겠습니까?")) {
            is_error = "N"	    
       		tr_1.Post()
       		if (is_error == "Y") return
	        btnquery_onclick()
	        ds_1.RowPosition = sa_position
	    }    
   	} 
}
