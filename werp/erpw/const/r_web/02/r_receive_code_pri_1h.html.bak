//---------------------------------------------------------------------------
function proj_request()                     //현장생성요청
//---------------------------------------------------------------------------
{
	if (ds_1.RowPosition < 1) {
		alert("요청할 자료를 선택하시오")
		return
	}

	if ( ds_1.NameValue(ds_1.RowPosition,'approve_class') != '1' ) {
		alert('작성중에만 처리가능합니다')
		return
	}
	if ( ds_1.NameValue(ds_1.RowPosition,'receive_process_class') != '05' ) {
		alert('낙찰된 경우만 현장코드 생성이 가능합니다...')
		return
	}
  	sa_position = ds_1.RowPosition

	var ls_receive_code,ls_class,ls_year
	
	ls_receive_code = ds_1.NameValue(ds_1.RowPosition,"receive_code")
	ls_class        = ds_1.NameValue(ds_1.RowPosition,"receive_tag")
	ls_year         = is_work_date.substr(2,2)
	
	if  (confirm("요청처리를 계속하시겠습니까?(요청 후 취소불가)")) {
		ds_spcall.DataID = 'y_sp_z_dept_code_request.jsp?arg_receive_code=' + ls_receive_code +
		                                           '&arg_class=' + ls_class  +
		                                           '&arg_year=' + ls_year
		ds_spcall.SyncLoad = true
		ds_spcall.reset()
		btnquery_onclick()
      ds_1.RowPosition = sa_position
		alert("정상처리되었습니다")
	}
	
}
//---------------------------------------------------------------------------
function proj_cancel()                     //현장생성요청취소
//---------------------------------------------------------------------------
{
	if ( ds_1.NameValue(ds_1.RowPosition,'approve_class') != '2' ) {
		alert('요청중에만 처리가능합니다')
		return
	}
	ds_1.NameValue(ds_1.RowPosition,'approve_class') = '1'
   sa_position = ds_1.RowPosition

	tr_1.Post()
   btnquery_onclick()
   ds_1.RowPosition = sa_position
   alert("정상처리되었습니다")
}
