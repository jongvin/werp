//-----------------------------------------------------------------
{
 var result;
 var array = new Array();
 result = window.dialogArguments;
	array = result.split("@");
	is_year = array[0]
	is_filter = array[1]
 Textbox_1.value = is_year         // 0: 찾고자하는 값.
 btnquery_onclick()
 
}
