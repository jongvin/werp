			ii_unit = 1;
			col_resize('90')
			break;
		case "2":
			ii_unit = 1000;
			if (gd_1.ColumnProp('cp_01','Width') != 70){
				col_resize('70')
			}
			break;
		case "3":
			ii_unit = 1000000;
			if (gd_1.ColumnProp('cp_01','Width') != 58){
				col_resize('58')
			}
			break;
		case "4":
			ii_unit = 100000000;
			if (gd_1.ColumnProp('cp_01','Width') != 58){
				col_resize('58')
			}
