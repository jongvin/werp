    onkeyDown="keyDown()">
<INPUT  type=button style="Z-INDEX: 106; LEFT: 180px; POSITION: absolute; TOP: 7px"  class="img_btnFind"  value=검색 onclick=btnquery_onclick() >
<INPUT  id=Button_confrim  type=button style="Z-INDEX: 106; LEFT: 230px; POSITION: absolute; TOP: 7px"  class="img_btnOk"  value=확인 onclick=Button_confirm_onclick() >
<INPUT   id=Button_close type=button style="Z-INDEX: 106; LEFT: 280px; POSITION: absolute; TOP: 7px"  class="img_btnClose"  value=닫기 onclick=Button_close_onclick() >
