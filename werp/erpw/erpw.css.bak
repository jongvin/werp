body {
	font-family:  "굴림", "Arial"; 
	font-size: 12px; 
	color: #353535; 
	line-height:16px;
}

body {
			scrollbar-face-color:#F8F8F8;
			scrollbar-highlight-color:#ffffff;
			scrollbar-3dlight-color:#B3B3B3;
			scrollbar-shadow-color:#B3B3B3;
			scrollbar-darkshadow-color:#FFFFFF;
			scrollbar-track-color:DDDDDD;
			scrollbar-arrow-color:8B9EA6;
}
td {	color : #353535 ; 
	Font-Family: "굴림", "Arial"; 
	Font-Size: 12px; 
	Line-height:16px; 
	text-decoration: none; 
	word-break:break-all;}

A:link {font-family: "굴림", "Arial"; text-decoration:none;color:#353535;}
A:visited {font-family: "굴림", "Arial"; text-decoration:none;color:#353535;}
A:hover {font-family: "굴림", "Arial"; text-decoration:none;color:#03348F;}

/*------------ 일반텍스트 입맛에 맞게 사용 --------------------------*/
.title {font-family: "굴림", "Arial";	color:#374657;	font-size:14px;	font-weight: bold;}
.item {	font-family: "굴림", "Arial";	color:#374657;	font-size:12px;	font-weight: bold; padding:5 0 0 7;}
.list {	font-family: "굴림", "Arial";	color:#353535;	font-size:12px;	padding:5 0 0 7;}
.list2 {	font-family: "굴림", "Arial";	color:#353535;	font-size:12px;	padding:5 0 0 0;}
.list3 {	font-family:"돋움", "Arial";	color:#353535;	font-size:11px;	padding:0 0 0 0;}
.list4 {	font-family: "굴림", "Arial";	color:#353535;	font-size:12px;	padding:2 0 0 3;}
.list5 {	font-family: "굴림", "Arial";	color:#353535;	font-size:12px;	padding:2 0 0 0;}


/*------------ 하이퍼링크 용 ( 페이지 분류별 이동  ) --------------------------*/
.location {font-family:"돋움", "Arial"; color:#666666 ; font-size:11px; }
.location A:link {font-family:"돋움", "Arial"; text-decoration:none;color:#666666;}
.location A:visited {font-family:"돋움", "Arial"; text-decoration:none;color:#666666;}
.location A:hover {font-family:"돋움", "Arial"; text-decoration:none;color:#fe8402;}

/*------------ 하이퍼링크 용 ( 게시판 페이지  ) --------------------------*/
.page {font-family:"돋움", "Arial"; color:#666666 ; font-size:11px; }
.page A:link {font-family:"돋움", "Arial"; text-decoration:none;color:#666666;font-size:11px; }
.page A:visited {font-family:"돋움", "Arial"; text-decoration:none;color:#666666;font-size:11px; }
.page A:hover {font-family:"돋움", "Arial"; text-decoration:none;color:#666666;font-size:11px; }


/*------------ 롤오버 블루 --------------------------*/
.Blue {	font-family: "굴림", "Arial";	color:#144AA7;	font-size:12px;	padding:0 0 0 3;}
.Blue A:link {font-family: "굴림", "Arial"; text-decoration:none;color:#144AA7;}
.Blue A:visited {font-family: "굴림", "Arial"; text-decoration:none;color:#709DE9;}
.Blue A:hover {font-family: "굴림", "Arial"; text-decoration:none;color:#709DE9;}

/*------------ 롤오버 그레이 --------------------------*/
.Gray {	font-family: "굴림", "Arial";	color:#999999;	font-size:12px;	padding:0 0 0 3;}
.Gray A:link {font-family: "굴림", "Arial"; text-decoration:none;color:#999999;}
.Gray A:visited {font-family: "굴림", "Arial"; text-decoration:none;color:#999999;}
.Gray A:hover {font-family: "굴림", "Arial"; text-decoration:none;color:#666666;}

/*------------ 모름 ) --------------------------*/
.Gray2 {	font-family: "굴림", "Arial";	color:#999999;	font-size:12px;	padding:0 0 0 0;}
.GrayNum {	font-family: "굴림", "Arial";	color:#999999;	font-size:12px;	padding:0 4 0 0; text-align:right}

/*------------ 숫자형 --------------------------*/
.num1 {	font-family: "굴림", "Arial";	color:#353535;	font-size:12px;	padding:0 4 0 0; text-align:right}
.num2 {	font-family: "굴림", "Arial";	color:#666666;	font-size:12px;	padding:0 6 0 0; text-align:right}

.NumTotal {	font-family: "굴림", "Arial";	color:#246695;	font-size:12px;	padding:0 4 0 0; text-align:right; font-weight:bold}
.NumSum {	font-family: "굴림", "Arial";	color:#353535;	font-size:12px;	padding:0 4 0 0; text-align:right; font-weight:bold}
.textTotal {	font-family: "굴림", "Arial";	color:#246695;	font-size:12px; font-weight:bold}

/*------------ 텍스트형 --------------------------*/
.text1 {	font-family:"돋움", "Arial";	color:#ffffff;	font-size:11px;}
.text2 {	font-family: "굴림", "Arial";	color:#666666;	font-size:12px;}
.text2b {	font-family: "굴림", "Arial";	color:#666666;	font-size:12px; font-weight:bold}
.text3 {	font-family: "굴림", "Arial";	color:#6F7887;	font-size:12px;}
.text4 {	font-family:"돋움", "Arial";	color:#341C24;	font-size:11px; font-weight:bold; padding-top:4px}
.text5 {	font-family:"돋움", "Arial";	color:#605A5C;	font-size:11px; font-weight:bold}
.bold {font-family: "굴림", "Arial";	color:#353535;	font-size:12px; font-weight:bold}


.text14 {	font-family:"돋움"; font-size:14px;}

/*------------ select box , text bpx  --------------------------*/
.box {font-family:"돋움", "Arial"; background-color:white; border:1 solid #C0C0C0;font-size:12px; color:#374657;}
.wbox {font-family:"돋움", "Arial"; background-color:white; border:1 solid #ffffff;font-size:12px; color:#374657;}

.space {padding:0 0 0 7;}
.t	{ border-top-color:#BCBCBC; border-top-style:solid; border-top-width:1px;}
.b	{ border-bottom-color:#BCBCBC; border-bottom-style:solid; border-bottom-width:1px;}
.r	{ border-right-color:#BCBCBC; border-right-style:solid; border-right-width:1px;}
.l	{ border-left-color:#BCBCBC; border-left-style:solid; border-left-width:1px;}

.view {	WORD-BREAK: break-all; LINE-HEIGHT: 140%; TEXT-ALIGN: justify; }





